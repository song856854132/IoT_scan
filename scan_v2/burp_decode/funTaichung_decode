    //            window.location.href = "/Frontend/Member/Register?isNative=True";
    //        }
    //    });
    //}

    // Google 第三方登入方式:
    // 取得token傳至GoolgeLoginAfterProcess後端處理相關問題
    function googleAPP() {
        var Google_clientId_forLogin = '421829144540-ks3pfdtv8stedkaorvf9jr892s7j8lbh.apps.googleusercontent.com';
        location.href = "https://accounts.google.com/o/oauth2/auth?response_type=token&" +
            "client_id=" + Google_clientId_forLogin +"&" +
            "redirect_uri=" + 'https://funtaichung.tw/Frontend/Member/GoolgeLoginAfterProcess' + "&" +
            "scope=https://www.googleapis.com/auth/userinfo.profile+https://www.googleapis.com/auth/userinfo.email";
        return false;
    }


</script>


