<script>
    // 點選頁籤時分別記住當前頁面
    $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
        // 當點選的頁籤為主功能
        if ($(e.target).attr('href') == "#tabMember" || $(e.target).attr('href') == "#tabFriend" ||
            $(e.target).attr('href') == "#tabReceipt" || $(e.target).attr('href') == "#tabInput") {
            localStorage.setItem('activeTabMain', $(e.target).attr('href'));
        }
        // 當點選的頁籤為發票管理內功能
        if ($(e.target).attr('href') == "#tabReceiptLogin" || $(e.target).attr('href') == "#tabReceiptLottery" ||
            $(e.target).attr('href') == "#tabReceiptInvalid") {
            localStorage.setItem('activeTabReceipt', $(e.target).attr('href'));
        }
        // 當點選頁籤為手動輸入內
        if ($(e.target).attr('href') == "#tabInputEWrite" || $(e.target).attr('href') == "#tabInputHWrite") {
            localStorage.setItem('activeTabInput', $(e.target).attr('href'));
        }
    });

    // 刷新頁面後展開先前所選的tab(主選單)
    var activeTabMain = localStorage.getItem('activeTabMain');
    if (activeTabMain) {
        $('.tabMain a[href="' + activeTabMain + '"]').tab('show');
    }
    // 刷新頁面後展開先前所選的tab(發票管理)
    var activeTabReceipt = localStorage.getItem('activeTabReceipt');
    if (activeTabReceipt) {
        $('.tabReceipt a[href="' + activeTabReceipt + '"]').tab('show');
    }
    // 刷新頁面後展開先前所選的tab(手動輸入)
    var activeTabInput = localStorage.getItem('activeTabInput');
    if (activeTabInput) {
        $('.tabInput a[href="' + activeTabInput + '"]').tab('show');
    }


    $(document).on("click", ".open-AddBookDialog", function () {
        //var Id = $(this).data('id');
        var storename = $(this).data('storename');
        var giftname = $(this).data('giftname');
        var scratchcode = $(this).data('scratchcode');
        //$("#ExchangeModel #ItemId").val(Id);

        var desc = "";
        

        
        if (storename == '')
        {
            storename = "請輸入活動內的店家代碼"
        }
        desc = '兌換說明';

        $("#storename").text(storename);
        $("#giftname").text(giftname)
        $("#scratchcode").val(scratchcode)
        
        $("#desc").text(desc)
    });
    function Exchange() {
        if ($("#exchangecode").val() == "") {
            swal(
                LC.get('請輸入兌換碼'),
                LC.get('兌換碼不得為空'),
                'warning'
            )
        } else {

            swal({
                title: LC.get('確認'),
                text: LC.get("確定是否要兌換？"),
                type: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: LC.get('是'),
                cancelButtonText: LC.get('否'),
            }).then(function () {

                $("#exchangeform").submit();
            }, function (dismiss) {
                if (dismiss === 'cancel') {
                    swal(
                        LC.get('取消'),
                        LC.get('取消成功'),
                        'error'
                    )
                    $.unblockUI();
                }
            })
        }
    }

    // 推薦人機制?是否已被取消
    function CheckPersonBeforeSubmit() {
        var p = "";
        $.ajax({
            type: "Post",
            url: "/Frontend/Member/GetRecommendPerson",
            dataType: "json",
            data: { phone: $("#RecommendPhone").val() },
            success: function (result) {
                p = result.Datas;
                swal({
                    title: LC.get('確認'),
                    text: LC.get("推薦人確定是否為") + p + "？",
                    type: 'warning',
                    showCancelButton: true,
                    confirmButtonColor: '#3085d6',
                    cancelButtonColor: '#d33',
                    confirmButtonText: LC.get('是'),
                    cancelButtonText: LC.get('否'),
                }).then(function () {

                    $("#recommendform").submit();
                }, function (dismiss) {
                    if (dismiss === 'cancel') {
                        swal(
                            LC.get('取消'),
                            LC.get('取消成功'),
                            'error'
                        )
                        $.unblockUI();
                    }
                })
            }
        });

    }

    // JSON日期格式轉換
    function getDateIfDate(d) {
        var dateString = d.substr(6);
        var currentTime = new Date(parseInt(dateString));
        var month = currentTime.getMonth() + 1;
        var day = currentTime.getDate();
        var year = currentTime.getFullYear();
        var date = year + "/" + month + "/" + day;
        return date;
    }

    // 推薦好友了解活動詳情
    function openDialog() {
        $("#MyDialog").dialog('open');
        return false;
    }

    // 複製專屬連結
    function copyurl(RecommendLink) {
        swal({
            text: LC.get('已複製連結，快分享給好友吧'),
            timer: 1500,
            showConfirmButton: false,
        });
        var textarea = document.createElement('textarea');
        textarea.textContent = "Hi～我發現了一個FUN TAICHUNG平台，裡面有臺中市的活動資訊和優惠店家訊息，快點擊下方連結完成註冊吧！" + RecommendLink;
        document.body.appendChild(textarea);

        var selection = document.getSelection();
        var range = document.createRange();
        range.selectNode(textarea);
        selection.removeAllRanges();
        selection.addRange(range);

        //console.log('copy success');
        document.execCommand('copy')
        selection.removeAllRanges();
        document.body.removeChild(textarea);
    }
    // 送出手動輸入電子發票前檢查
    function EWriteBeforeSubmit() {
        $('#ck_E_ReceiptDate').empty();
        $('#ck_E_ReceiptNumber').empty();
        $('#ck_E_RandomCode').empty();
        // 判斷發票日期是否填寫
        if ($("#receipt_EDate").val() == "") {
            $("#receipt_EDate").focus();
            $('#ck_E_ReceiptDate').append('<span class="message" style="color:#a94442" data-lang="請填寫發票日期"></span>')
            //載入語系初始化
            LangContoller.init();
            return false;
        }

        // 判斷發票號碼是否填寫
        if ($("#receipt_E_En").val() == "" || $("#receipt_E_Number").val() == "") {
            $("#receipt_E_En").focus();
            $('#ck_E_ReceiptNumber').append('<span class="message" style="color:#a94442" data-lang="發票號碼請填寫完整"></span>')
            //載入語系初始化
            LangContoller.init();
            return false;
        } else {
            if ($("#receipt_E_Number").val().length < 8) {
                $("#receipt_E_Number").focus();
                $('#ck_E_ReceiptNumber').append('<span class="message" style="color:#a94442" data-lang="發票號碼數字部分錯誤"></span>')
                //載入語系初始化
                LangContoller.init();
                return false;
            } else {
                if ($("#receipt_E_En").val().length < 2) {
                    $("#receipt_E_En").focus();
                    $('#ck_E_ReceiptNumber').append('<span class="message" style="color:#a94442" data-lang="發票號碼英文部分錯誤"></span>')
                    //載入語系初始化
                    LangContoller.init();
                    return false;
                }
            }
        }

        // 判斷隨機碼是否填寫
        if ($("#ACTReceiptItem_ACT_ReceiptVerify_RandomCode").val() == "") {
            $("#ACTReceiptItem_ACT_ReceiptVerify_RandomCode").focus();
            $('#ck_E_RandomCode').append('<span class="message" style="color:#a94442" data-lang="請填寫隨機碼"></span>')
            //載入語系初始化
            LangContoller.init();
            return false;
        } else {
            if ($("#ACTReceiptItem_ACT_ReceiptVerify_RandomCode").val().length < 4) {
                $("#ACTReceiptItem_ACT_ReceiptVerify_RandomCode").focus();
                $('#ck_E_RandomCode').append('<span class="message" style="color:#a94442" data-lang="隨機碼為四位數字"></span>')
                //載入語系初始化
                LangContoller.init();
                return false;
            }
        }

    }
    // 送出手動輸入傳統發票前檢查
    function HWriteBeforeSubmit() {

        $('#ck_H_ReceiptDate').empty();
        $('#ck_H_ReceiptNumber').empty();
        $('#ck_H_SellerBan').empty();
        $('#ck_H_Total').empty();
        // 判斷發票日期是否填寫
        if ($("#receipt_HDate").val() == "") {
            $("#receipt_HDate").focus();
            $('#ck_H_ReceiptDate').append('<span class="message" style="color:#a94442" data-lang="請填寫發票日期"></span>')
            //載入語系初始化
            LangContoller.init();
            return false;
        }

        // 判斷發票號碼是否填寫
        if ($("#receipt_H_En").val() == "" || $("#receipt_H_Number").val() == "") {
            $("#receipt_H_En").focus();
            $('#ck_H_ReceiptNumber').append('<span class="message" style="color:#a94442" data-lang="發票號碼請填寫完整"></span>')
            //載入語系初始化
            LangContoller.init();
            return false;
        } else {
            if ($("#receipt_H_Number").val().length < 8) {
                $("#receipt_H_Number").focus();
                $('#ck_H_ReceiptNumber').append('<span class="message" style="color:#a94442" data-lang="發票號碼數字部分錯誤"></span>')
                //載入語系初始化
                LangContoller.init();
                return false;
            } else {
                if ($("#receipt_H_En").val().length < 2) {
                    $("#receipt_H_En").focus();
                    $('#ck_H_ReceiptNumber').append('<span class="message" style="color:#a94442" data-lang="發票號碼英文部分錯誤"></span>')
                    //載入語系初始化
                    LangContoller.init();
                    return false;
                }
            }
        }

        // 判斷賣方統一編號是否填寫
        if ($("#ACTReceiptItem_ACT_Receipt_SellerBan").val() == "") {
            $("#ACTReceiptItem_ACT_Receipt_SellerBan").focus();
            $('#ck_H_SellerBan').append('<span class="message" style="color:#a94442" data-lang="請填寫賣方統一編號"></span>')
            //載入語系初始化
            LangContoller.init();
            return false;
        } else {
            if ($("#ACTReceiptItem_ACT_Receipt_SellerBan").val().length < 8) {
                $("#ACTReceiptItem_ACT_Receipt_SellerBan").focus();
                $('#ck_H_SellerBan').append('<span class="message" style="color:#a94442" data-lang="賣方統一編號為八位數字"></span>')
                //載入語系初始化
                LangContoller.init();
                return false;
            }
        }

        // 判斷消費金額是否填寫
        if ($("#ACTReceiptItem_ACT_Receipt_Total").val() == "") {
            $("#ACTReceiptItem_ACT_Receipt_Total").focus();
            $('#ck_H_Total').append('<span class="message" style="color:#a94442" data-lang="請填寫消費金額"></span>')
            //載入語系初始化
            LangContoller.init();
            return false;
        } else {
            // 判斷是否填寫可用之金額數字
            var regu = "^([1-9]([0-9]{0,9})|([0]))(/.[0-9]{1,2})?$";
            var re = new RegExp(regu);
            if (!re.test($("#ACTReceiptItem_ACT_Receipt_Total").val())) {
                $('#ck_H_Total').append('<span class="message" style="color:#a94442" data-lang="請填寫正確金額"></span>')
                //載入語系初始化
                LangContoller.init();
                return false;
            }
        }
        //載入語系初始化
        LangContoller.init();
    }
    // 查詢抽獎券發票清單
    function getReceiptOfLottery(input) {
        $('#receiptOfLottery').empty();

        // 取得當前activityID
        var e = document.getElementById('activity');
        var activityID = e.options[e.selectedIndex].value;

        $.ajax({
            type: "Post",
            url: "/Frontend/Member/GetReceiptOfLottery",
            dataType: "json",
            data: { LotteryId: input, activityID: activityID },
            success: function (result) {
                var html = "";
                for (i = 0; i < result.Datas.lstReceiptInLottery.length; i++) {
                    html = html + "<tr><td>" + getDateIfDate(result.Datas.lstReceiptInLottery[i].ReceiptDate) + "</td>";
                    html = html + "<td>" + result.Datas.lstReceiptInLottery[i].ReceiptType + "</td>";
                    html = html + "<td>" + result.Datas.lstReceiptInLottery[i].ReceiptNumber + "</td>";
                    html = html + "<td>" + result.Datas.lstReceiptInLottery[i].Location + "</td>";
                    if (result.Datas.lstReceiptInLottery[i].isSpecial) {
                        html = html + "<td>" + result.Datas.lstReceiptInLottery[i].total  /*"<span href='#' class='badge white bg-warning radius-lg ml-7' style='margin-top:-3px; margin-left: 7px;' data-lang='3倍'></span></td></tr>"*/;
                    } else {
                        html = html + "<td>" + result.Datas.lstReceiptInLottery[i].total + "</td></tr>";
                    }
                }
                $('#receiptOfLottery').append(html);
            }
        });
        //載入語系初始化
        LangContoller.init();
    }

    // 取得發票內容(作廢用)
    function getReceiptDetail(input) {
        //console.log(input)
        $.ajax({
            type: "Post",
            url: "/Frontend/Member/GetCancelReceiptDeatil",
            dataType: "json",
            data: { ReceiptNumber: input },
            success: function (result) {
                //console.log(result)
                // 消費日期
                $('#cancelReceiptDate').val(getDateIfDate(result.Datas.ReceiptDate));
                // 發票類型
                $('#cancelReceiptType').val(result.Datas.ReceiptType);
                // 發票號碼
                $('#cancelReceiptNumber').val(result.Datas.ReceiptNumber);
                $('#cReceiptNumber').val(result.Datas.ReceiptNumber);
                // 消費地點
                $('#cancelReceiptLocation').val(result.Datas.ReceiptLocation);
                // 消費金額
                $('#cancelReceiptTotal').val(result.Datas.total);
                // 發票ID
                $('#cReceiptID').val(result.Datas.ReceiptID);
            }
        });
    }
</script>





<script>
    $(document).ready(function () {
        // 瀏覽人數
        $.ajax({
            type: "Post",
            url: "/Frontend/VUC/Visitors",
            dataType: "json",
            success: function (response) {
                if (response.isSuccess === true) {
                    $.each($('.visitor'), function (index, element) {
                        element.innerText = response.data;
                    })
                }
            }
        });
    });

</script>
