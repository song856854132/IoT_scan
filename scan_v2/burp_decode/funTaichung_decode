                        <input class="hiddenPageCount" data-val="true" data-val-number="The field m_iPageCount must be a number." data-val-required="The m_iPageCount field is required." id="PagerInfo_m_iPageCount" name="PagerInfo.m_iPageCount" type="hidden" value="6" />
                        <input class="pager-Info-Search" data-val="true" data-val-required="The m_Search field is required." id="PagerInfo_m_Search" name="PagerInfo.m_Search" type="hidden" value="True" />
                        <script type="text/javascript">
    //$(document).ready(function () {
    //    //$('#pager-Info-Search').val(true);
    //    //$('.pager-Info-Search').val(true);

    //});

    function ChangeCurrentPage(page,e) {
        $($(e).parents('div')[1]).find('.hiddenCurrentPage').val(page)

        var form = $(e).closest('form').get(0);
        if (form != null) {
            $('#hiddenSearchSource').val('2');
            $($(e).parents('div')[1]).find('.pager-Info-Search').val(false);
            if($('#table-body').length == 1){
                $('#table-body').empty();
            }

            form.submit();
        }
    }

    function ChangePageSize(page) {
        var _pageCount = $(page).val();
        $($(page).parents('div')[1]).find('.hiddenPageCount').val(_pageCount)
        var form = $(page).closest('form').get(0);
        if (form != null) {
            $('#hiddenSearchSource').val('2');
            $(page).parent('div').parent('div').find('.pager-Info-Search').val(false);
            if($('#table-body').length == 1){
                $('#table-body').empty();
            }
            form.submit();
        }
    }



</script>

<div class="t-center pb-40 sm-pt xs-pt-mobile">
    <ul class="pagination pagination-lg">






            <li class="active ">
                <a href="#" class="number current" title="第1頁">1</a>
            </li>

            <li class="page-item">
                <a href="#" class="number page-link" title="第2頁" onclick="ChangeCurrentPage(2,this)">2</a>
            </li>

            <li class="page-item">
                <a href="#" class="number page-link" title="第3頁" onclick="ChangeCurrentPage(3,this)">3</a>
            </li>

            <li class="page-item visible-md visible-lg">
                <a href="#" class="number page-link" title="第4頁" onclick="ChangeCurrentPage(4,this)">4</a>
            </li>

            <li class="page-item visible-md visible-lg">
                <a href="#" class="number page-link" title="第5頁" onclick="ChangeCurrentPage(5,this)">5</a>
            </li>

            <li class="visible-md visible-lg">
                <a href="#" class="page bg-transparent gray9 bold bg-gray3-hover slow" title="下一頁" onclick="ChangeCurrentPage(2,this)" data-lang="下一頁"> 下一頁</a>
            </li>

            <li>
                <a href="#" class="page bg-transparent gray9 bold bg-gray3-hover slow" title="末頁" onclick="ChangeCurrentPage(473 ,this)" data-lang="末頁">末頁</a>
            </li>

    </ul>


