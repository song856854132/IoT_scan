<script>

    //const urlParams = new URLSearchParams(window.location.search);
    // const mstate = urlParams.get('state');


    var tcpass_clientId = 'Zjg5MTBkMDYtZjAyMS00MGQxLThhZDktZmMyYmZlYmQ2OTI1';
    var tcpass_clientSecret = '3k3nIiNMnOSsv_oXIiN_UVkocRnLfD5Vm3im46JAESQ';
    var tcpass_Url = 'https://tcpass-auth.taichung.gov.tw';

    var tcpass_redirectUri = "https://funtaichung.tw/Frontend/Member/Login";

    function tcpassLogin() { //台中e指通登入

        var url = tcpass_Url +"/authorize"
        url += '?scope=' + encodeURIComponent("openid profile")
        url += '&response_type=' + encodeURIComponent("code")
            + '&client_id=' + encodeURIComponent(tcpass_clientId)
            + '&redirect_uri=' + encodeURIComponent(tcpass_redirectUri )
            + '&state=tcpass'
            + '&nonce=3131313131';
        //  var w = window.open(url, 'oauth', 'width=680,height=520');
        //   console.log('w=',JSON.stringify(w));
        window.location = url;

    }

    $(document).ready(function () {
        const urlParams = new URLSearchParams(window.location.search);
        if (urlParams.get('state') == "tcpass") {
           // var murl = 'https://tcpass-auth-test.taichung.gov.tw/userinfo';
            //   alert('location.search: ' + location.search);
            if (urlParams.get('error') != null) {
                alert('error: ' + urlParams.get('error'));
                console.log('error: ' + urlParams.get('error'), "error_description:" + urlParams.get('error_description'));

                //} else if (urlParams.get('access_token') != null) {
                //    var mAuthor = 'Bearer ' + urlParams.get('access_token');
                //    murl = 'https://tcpass-auth-test.taichung.gov.tw/userinfo';
                //    tcpassUserinfo(mAuthor, {},murl);
            } else if (urlParams.get('code') != null) {
                 var mAuthor = "Basic " + btoa(tcpass_clientId + ":" + tcpass_clientSecret);
                var murl = tcpass_Url + "/token"
             //   console.log("mAuthor=", mAuthor, murl);
                var data = {}
                data["grant_type"] = "authorization_code";
                data["code"] = urlParams.get('code');
                data["redirect_uri"] = tcpass_redirectUri;
                tcpassGetToken(mAuthor, data, murl);
            }

        }


        //載入語系初始化
     //   window.ParsleyValidator.setLocale($("#sessionlang").val());
        //載入語系初始化
        LangContoller.init();
    });

    function tcpassGetToken(mAuthor, data = {}, murl) { // 取得 e指通使用者資料
       // console.log("access_token=", mAuthor);
       // console.log("tcpassGetToken=", JSON.stringify(data));

        $.ajax({
            type: "POST",
            url: murl,
            dataType: "json",
            data: data,
            headers: { 'Authorization': mAuthor },
            success: function (result) {
              //  console.log("tcpassGetToken:", JSON.stringify(result));
                if (result.error != null) {
                    alert("access_token= error");
                } else if (result.access_token != null) {
                    var maccess_token = "Bearer " + result.access_token;
                    tcpassUserinfo(maccess_token);
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                console.log(XMLHttpRequest.status, XMLHttpRequest.readyState, textStatus);

            }

        });
    }

    function tcpassUserinfo(mAuthor) { // 取得 e指通使用者資料
        //   console.log("tcpassUserinfo_token=", mAuthor);
        // var murl = 'https://tcpass-auth-test.taichung.gov.tw/userinfo';
        var murl = tcpass_Url + "/userinfo"
        $.ajax({
            type: "POST",
            url: murl,
            headers: { 'Authorization': mAuthor },
            success: function (data) {
                //  console.log("tcpassUserinfo:", JSON.stringify(data));
                if (data.error != null) {
                    alert("access_token= error");
                } else {
                    // alert("台中e指通登入成功:" + data.name);
                    // $("#acct").val(data.email);
                    var dataid = data.sub.toString();   // AccessToken
                    var dataEmail = data.email.toString();
                    var dataname = data.name.toString();
                    var dataPhone = data.mobile.toString();
                    //   var dataBirthday = data.birthdate.toString(); //生日yyyy-MM-dd
                    var sex = '';
                    if (data.gender.toString() == "male") {
                        sex = 'M';
                    } else if (data.gender.toString() == "female") {
                        sex = 'F';
                    }
                    location.href = '/Frontend/Member/OAuthLogin?Type=tcpass&Email=' + dataEmail + '&AccessToken=' + dataid + '&Name=' + dataname + '&CellPhone=' + dataPhone + '&Sex=' + sex;

                }
            }
        });
    }

    function Login() { //
        var fields = $("#login_form :input").serializeArray();

        console.log(JSON.stringify(fields));
        $.ajax({
            type: 'POST',
            url: "/Frontend/Member/Login",
            dataType: "json",
            data: fields,
            success: function (res) {
                console.log('5. Return => ', res);
            }
        });

    }

</script>
<script>
    var recaptchachecked = false;//Google驗證用
    var reCAPTCHA_v3 = '6LcnHcgaAAAAAALS_DbhdNArKBmnMneR3ezEBn9V';
    grecaptcha.ready(function () {
        //  grecaptcha.execute('6LeNU54aAAAAAFeqe4i_R2mWOPFwIGm_G31nWq3t', { action: '/Frontend/Home/VerifyBot' }).then((token) => {
        grecaptcha.execute(reCAPTCHA_v3, { action: '/Frontend/Home/VerifyBot' }).then((token) => {

            $.ajax({
                type: 'POST',
                url: '/Frontend/Home/VerifyBot',
                data: { token: token },
                success: (res) => {
                    recaptchachecked = res;
                    //console.log('5. Return => ', res);
                }
            });
        });
    });

    $(document).ready(function () {
        // 先判斷畫面上的radio停留在外國還是本國
        //var value = document.querySelector('input[name="UserInfoItem.IsTaiwanese"]:checked').value;
        var value = $('input[name="UserInfoItem.IsTaiwanese"]:checked').val();
        if (value == "False") {
            //外國人:輸入電子信箱
            $('#acct').attr('placeholder', '請輸入Email');
            $('#acct').attr('data-parsley-maxlength', '50');
            $('#acct').attr('data-parsley-type', 'email');
            $('#acct').attr('data-lang', '請輸入Email');

            document.getElementById('reSendLetter').style.display = 'inline';
            // 註冊連結
            document.getElementById('register_notNative').style.display = 'inline';
            document.getElementById('register_isNative').style.display = 'none';
            // 忘記密碼連結
            document.getElementById('forgetP_notNative').style.display = 'inline';
            document.getElementById('forgetP_isNative').style.display = 'none';
        }
        else {
            // 本國人:輸入手機號碼
            $('#acct').attr('placeholder', '請輸入手機號碼');
            $('#acct').attr('data-parsley-maxlength', '10');
            $('#acct').attr('data-parsley-type', 'cellphone');
            $('#acct').attr('data-lang', '請輸入手機號碼');

            document.getElementById('reSendLetter').style.display = 'none';
            // 註冊連結
            document.getElementById('register_notNative').style.display = 'none';
            document.getElementById('register_isNative').style.display = 'inline';
            // 忘記密碼連結
            document.getElementById('forgetP_notNative').style.display = 'none';
            document.getElementById('forgetP_isNative').style.display = 'inline';
        }

        // 當radio button被改變時...
        $('.IsTaiwanese').change(function () {
            $('#acct').removeAttr('data_parsley_maxlength');
            $('#acct').removeAttr('data_parsley_type');

            if ($(this).filter(':checked').val() == "False") {
                //外國人:輸入電子信箱
                if ($(_Selector_Lang).val() == 'tw') {
                    $('#acct').attr('placeholder', '請輸入Email');
                } else {
                    $('#acct').attr('placeholder', 'Please input your e-mail address');
                }
                $('#acct').attr('data-parsley-maxlength', '50');
                $('#acct').attr('data-parsley-type', 'email');
                $('#acct').attr('data-lang', '請輸入Email');

                document.getElementById('reSendLetter').style.display = 'inline';
                // 註冊連結
                document.getElementById('register_notNative').style.display = 'inline';
                document.getElementById('register_isNative').style.display = 'none';
                // 忘記密碼連結
                document.getElementById('forgetP_notNative').style.display = 'inline';
                document.getElementById('forgetP_isNative').style.display = 'none';
            } else {
                // 本國人:輸入手機號碼
                if ($(_Selector_Lang).val() == 'tw') {
                    $('#acct').attr('placeholder', '請輸入手機號碼');
                } else {
                    $('#acct').attr('placeholder', 'Please input your cell phone number');
                }
                $('#acct').attr('data-parsley-maxlength', '10');
                $('#acct').attr('data-parsley-type', 'cellphone');
                $('#acct').attr('data-lang', '請輸入手機號碼');

                document.getElementById('reSendLetter').style.display = 'none';
                // 註冊連結
                document.getElementById('register_notNative').style.display = 'none';
                document.getElementById('register_isNative').style.display = 'inline';
                // 忘記密碼連結
                document.getElementById('forgetP_notNative').style.display = 'none';
                document.getElementById('forgetP_isNative').style.display = 'inline';
            }
        });

    })

    function beforelogin() {

        if (recaptchachecked == false) {

            swal({
                title: LC.get('機器人驗證未通過'),
                text: LC.get('請稍後再試'),
                type: 'warning'
            });
            return false;
        } else {
            return true;
        }
    }

