<script>
    // 會員資料修改前判斷
    function Logout() {
        $.ajax({
            type: "Post",
            url: "/Frontend/Member/ajaxLogout",
            dataType: "json",
            success: function (result) {

                window.location = "/2021shopTaichung";
            }
        });
    }
</script>



    

