        });
        $("#allcheck").click(function () {
            $('#test0').show();
            $('#test1').hide();
            $('#test2').hide();
            $('#tabid').attr("value", "allcheck");
        });
        $("#infocheck").click(function () {
            $('#test1').show();
            $('#test0').hide();
            $('#test2').hide();
            $('#tabid').attr("value","infocheck");
        });
        $("#announce").click(function () {
            $('#test2').show();
            $('#test1').hide();
            $('#test0').hide();
            $('#tabid').attr("value", "announcecheck");
        });

