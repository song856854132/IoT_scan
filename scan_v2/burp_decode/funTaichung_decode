        // 判斷聯絡人姓名必填
        //if ($("#UserInfoItem_ContactName").val() == "" && $("#UserInfoItem_LoginType").val() == "legal") {
        //    $("#UserInfoItem_ContactName").focus();
        //    $('#ck_ContactName').append('<span class="message" style="color:#a94442" data-lang="請填寫聯絡人姓名"></span>')
        //    return false;
        //}
        if ($("#UserInfoItem_UserName").val() == "" || $("#UserInfoItem_CellPhone").val() == "" || $("#UserInfoItem_Email").val() == "") {
            //  提醒：姓名、手機、Email 等資料若未提供, 可能會影響中奬通知。
            //  Reminder: If the name, mobile phone, and email information are not provided, it may affect the notification of the lottery winning results.
            swal({             
                title: LC.get('提醒：姓名、手機、Email'),
                text: LC.get('資料若未提供,可能會影響中奬通知'),
                type: "warning",       
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                showConfirmButton:true,
                confirmButtonText: LC.get('確認'),
                cancelButtonText: LC.get('取消')
            }).then((result) => {             
                if (result) {
                   // console.log("ok")
                    $("#memberform").submit();                 
                    return true;
                } else {
                    console.log("cancal");
                    return false;
                }                
            })  
          //  console.log("cancadddddl")
          
        } else {
            $("#memberform").submit();
        }

         
    }

   

    // 下拉方式選擇活動(post刷新頁面)
    function reloadMemberZone() {
        document.getElementById("memberZoneForm").submit();
        
    }

    function reloadDescription(data) {

        $.ajax({
            type: "Post",
            url: "/Frontend/Member/GetReceiptDescription",
            dataType: "json",
            data: { activityID: data },
            success: function (result) {
                console.log(JSON.stringify(result))
                $('.ReceiptRule').text(result.Datas);
                //$('#activity').each().text(result.Datas)
                $(".activity").each(function (index) {
                    //console.log(index + ": " + $(this).text());
                    $(this).val(data);

                });

            }
        });
    }




    // 作廢發票submit
    function cancelReceipt() {
        document.getElementById("cancelReceiptForm").submit();
    }

</script>

