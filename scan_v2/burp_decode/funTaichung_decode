<script>
 //   console.log('url=', '','','');


    $(document).ready(function () {
        // 陣列regional內的字串填寫你要的語言, 如繁體中文
        $.datepicker.setDefaults($.datepicker.regional["zh-TW"]);
        // 初始化 select2套件
        $('.selectpicker').selectpicker();

        //-- 會員專區 生日欄位設定
        $('#UserInfoItem_Birthday').datepicker({
            changeMonth: true,
            changeYear: true,
            autoclose: true,
            yearRange: '-100:+0',
            format: 'yyyy/mm/dd',
            language: 'zh-TW',
            beforeShow: function (input) {
                $(input).css({
                    "position": "relative",
                    "z-index": 2
                });
            },
            defaultDate: "-20y" // 預設2000年
        });

        //-- 發票專區 發票日期設定-電子發票
        $('#receipt_EDate').datepicker({
            changeMonth: true,
            changeYear: true,
            autoclose: true,
            "setDate": new Date(),
            format: 'yyyy/mm/dd',
            language: 'zh-TW',
            maxDate: new Date,
        });
        $("#receipt_EDate").datepicker("setDate", new Date());

        //-- 發票專區 發票日期設定-傳統發票
        $('#receipt_HDate').datepicker({
            changeMonth: true,
            changeYear: true,
            autoclose: true,
            "setDate": new Date(),
            format: 'yyyy/mm/dd',
            language: 'zh-TW',
            maxDate: new Date,
        });
        $("#receipt_HDate").datepicker("setDate", new Date());

        var url = window.location.href;
        var tabid = url.split('#');
        if (tabid[1] != null) {
            $("[aria-controls='" + tabid[1] + "']").click();
        }


        $('#RecommendPhone').keyup(function () {
            //初始化移除所有訊息
            $('#RecommendPhoneDiv').find('span').remove();
            //格式驗證
            var data_length = $("#RecommendPhone").val().length;
            var data = $("#RecommendPhone").val();
            var re = /^09[0-9]{8}$/;
            if (data_length != 10 || !data.match(re))//不符合格式時
            {
                //不符合格式時的樣式、訊息
                $("#sendRecommendPhone").removeClass("bg-blue6");
                $("#sendRecommendPhone").addClass("bg-gray6");
                $("#RecommendPhone").css({
                    "color": "#B94A48",
                    "background-color": "#F2DEDE",
                    "border": "1px solid #EED3D7",
                });
                $('#sendRecommendPhone').attr('disabled', true);
                $('#RecommendPhoneDiv').append('<span class="message" style="color:#a94442" data-lagn="請輸入手機號碼,範例：0981123123"></span>')
                //不符合格式時的樣式、訊息
            } else {
                //符合格式時的樣式
                $('#sendRecommendPhone').attr('disabled', false);
                $("#sendRecommendPhone").removeClass("bg-gray6");
                $("#sendRecommendPhone").addClass("bg-blue6");
                $("#RecommendPhone").css({
                    "color": "#468847",
                    "background-color": "#DFF0D8",
                    "border": "1px solid #D6E9C6",
                });
                //符合格式時的樣式
            }
            //載入語系初始化
            LangContoller.init();
        });

        //-- 每日問答填完問題後，刷新頁面需要記住TAB
        var tabStep = localStorage.getItem('tabStep');
        if (tabStep) {
            $('.nav-pills a[href="' + tabStep + '"]').tab('show');
        }

        // click編輯按鈕，顯示input元件
        $('#btnEdit').on('click', function () {
            $("#UserInfoItem_Birthday").removeClass("data-val-date");
            $(".readable").css('display', 'none');
            $("form .clearfix").css('display', 'block');

            return false;
        });

        $("#UserInfoItem_CountryID").change(function () {
            var selectID = this.value;
            // 若國家為臺灣開啟縣市下拉選單
            if (selectID == "C000000223") {
                document.getElementById('ddlCity').style.display = 'inline';
            } else {
                document.getElementById('ddlCity').style.display = 'none';
            }
        });

        $("#UserInfoItem_CityID").change(function () {
            var selectID = this.value;
            // 若國家為臺灣開啟縣市下拉選單
            if (selectID != "-1") {
                $('#ddlCity_check').find('span').remove();
            }
        });

        // 隱藏"對話框"
        $("#MyDialog").dialog({
            draggable: false,   // 拖曳功能
            autoOpen: false,    // 自動開啟
            modal: true,        // 鎖定頁面
            width: 400,
            dialogClass: 'no-close success-dialog'
        });
    })

    // 會員資料修改前判斷
    function EditMemberBeforeSubmit() {
        $('#ck_Name').empty();
        $('#ddlCity_check').empty();
        $('#ck_Cellphone').empty();
        $('#ck_Email').empty();
        $('#ck_Birthday').empty();
        $('#ck_Career').empty();
        $('#ck_IndustryCode').empty();
        $('#ck_ContactName').empty();
                

