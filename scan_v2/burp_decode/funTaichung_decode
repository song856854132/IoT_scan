    <script>
        $(document).ready(function () {
           
            if ('' == "all") {
                $('.nav-tabs a[href="#tab1c"]').tab('show');
                $('#test0').show();
                $('#test1').hide();
                $('#test2').hide();
                $('#tabid').attr("value", "allcheck");

            } else if ('' == "info") {
                $('.nav-tabs a[href="#tab2c"]').tab('show');
                $('#test1').show();
                $('#test0').hide();
                $('#test2').hide();
                $('#tabid').attr("value", "infocheck");

            } else if ('' == "announce") {
                $('.nav-tabs a[href="#tab3c"]').tab('show');
                $('#test2').show();
                $('#test1').hide();
                $('#test0').hide();
                $('#tabid').attr("value", "announcecheck");

            }
            else {
                $('.nav-tabs a[href="#tab1c"]').tab('show');
                $('#test0').show();
                $('#test1').hide();
                $('#test2').hide();
                $('#tabid').attr("value", "allcheck");

            }

            //if ($("#LangList").length > 0) {               
            //}
            LangContoller.init();
            // 瀏覽人數
            //$.ajax({
