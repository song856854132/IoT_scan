    <script>
        $(document).ready(function () {
            if ($('.star-1').text() == $('#Rating1').val()) {
                $('.star-1').addClass('active');
                $('.stars .starspan').addClass('active');
            }
            if ($('.star-2').text() == $('#Rating1').val()) {
                $('.star-2').addClass('active');
                $('.stars .starspan').addClass('active');
            }
            if ($('.star-3').text() == $('#Rating1').val()) {
                $('.star-3').addClass('active');
                $('.stars .starspan').addClass('active');
            }
            if ($('.star-4').text() == $('#Rating1').val()) {
                $('.star-4').addClass('active');
                $('.stars .starspan').addClass('active');
            }
            if ($('.star-5').text() == $('#Rating1').val()) {
                $('.star-5').addClass('active');
                $('.stars .starspan').addClass('active');
            }
            $('.stars a').on('click', function () {
                $('.stars .starspan, .stars a').removeClass('active');

                $(this).addClass('active');
                $('.stars .starspan').addClass('active');
                $('#Rating1').val($(this).text());
            });
        });
    </script>
