/* =====table===== */
var is_common_menu_open="0";
var show_common_menu = new Array();
var add_pertain_function_index = 0;
var sub_add_pertain_function_index = 0;
var toggle_function_index = 0;
var common_menu_line_height = 0;
var global_on_off_name;
var is_mobile_login;
function table_on_off_function(storage_name)
{
var on_off_data=localStorage.getItem(storage_name);
if(on_off_data==null)
{
on_off_data="";
for (i = 0; i < $("#drag_sort_table").children().length; i++)
{on_off_data=on_off_data+"1";}
localStorage.setItem(storage_name, on_off_data);
}
else
{
if(on_off_data.length!=$("#drag_sort_table").children().length)
{
on_off_data="";
for (i = 0; i < $("#drag_sort_table").children().length; i++)
{on_off_data=on_off_data+"1";}
localStorage.setItem(storage_name, on_off_data);
}
}
on_off_data=localStorage.getItem(storage_name);
menu_name_to_id=[];
dw("<ul id='common_menu' style='display:none;z-index:1;' onmouseover=\"common_menu_open('1')\" onmouseout=\"common_menu_open('0')\">");
for (i = 0; i < on_off_data.length; i++)
{
table_id="table_zone_"+(i+1);
title=$("#" + table_id + " span[name='common_title']").html();
check_img="checked.gif";
menu_name_to_id[i]=title.replace("&amp;", "&");
if(on_off_data[i]=="0")
{
check_img="unchecked.gif";
if(document.getElementById(table_id)!=null)
{document.getElementById(table_id).style.display = "none";}
}
if(show_common_menu[i] == 1)
{
dw("<li id='table_state_"+(i+1)+"'><img id='menu_image_"+(i+1)+"' width='16' height='16' src='"+check_img+"' style='float:left'>");
dw("<span style='float:left'>&nbsp&nbsp&nbsp"+title+"</span></li>");
common_menu_line_height = common_menu_line_height+1;
}
else
{dw("<li id='table_state_"+(i+1)+"' style='display:none'></li>");}
$("#table_state_"+(i+1)).click({name: storage_name, m_id: "menu_image_"+(i+1), t_id: table_id, n: i}, table_set_state);
}
dw("</ul>");
dw("<div id='common_menu_line' class='menu-separator' style='display:none;'></div>");
var common_menu = document.getElementById("common_menu");
var common_menu_line = document.getElementById("common_menu_line");
$("body").on('mousedown', function(e) {
if(is_common_menu_open=="0")
{
common_menu.style.display = "none";
common_menu_line.style.display="none";
}
});
$("#set_table").on('mousedown', function(e) {
e.stopPropagation();
set_table = document.getElementById("set_table");
common_menu = document.getElementById("common_menu");
common_menu_line = document.getElementById("common_menu_line");
if(common_menu.style.display=="none")
{
common_menu_line.style.height=28;
if (typeof(hidden_menu_name) != "undefined")
{
for (i = 0; i <  hidden_menu_name.length; i++)
{
for (j = 0; j <  menu_name_to_id.length; j++)
{
if(menu_name_to_id[j]==hidden_menu_name[i])
{
document.getElementById("table_state_"+(j+1)).style.display="none";
break;
}
}
}
}
common_menu.style.display="";
common_menu_line.style.display="";
common_menu.style.top=set_table.offsetTop+set_table.offsetHeight-1;
common_menu.style.left=set_table.offsetLeft+set_table.offsetWidth-common_menu.offsetWidth;
common_menu_line.style.top=set_table.offsetTop+set_table.offsetHeight-1;
common_menu_line.style.left=set_table.offsetLeft+set_table.offsetWidth-common_menu.offsetWidth+23;
common_menu_line.style.height=28*common_menu_line_height;
if (typeof(menu_enable_number) != "undefined")
{common_menu_line.style.height=28*menu_enable_number;}
}
else
{
common_menu.style.display="none";
common_menu_line.style.display="none";
}
});
}
function common_menu_open(is_open)
{is_common_menu_open=is_open;}
function table_close(storage_name, m_id, t_id ,n)
{
var m = document.getElementById(m_id);
var t = document.getElementById(t_id);
var on_off_data=localStorage.getItem(storage_name);
var num_one=0
for (i = 0; i <  on_off_data.length; i++)
{
table_id="table_zone_"+(i+1);
title=$("#" + table_id + " span[name='common_title']").html();
if(show_common_menu[i]==1 && on_off_data[i]=="1")
{
if (typeof(hidden_menu_name) != "undefined")
{
is_have_hidden_name=0;
for (j = 0; j <  hidden_menu_name.length; j++)
{
if(title==hidden_menu_name[j])
{is_have_hidden_name=1;}
}
if(is_have_hidden_name==0)
{num_one=num_one+1;}
}
else
{num_one=num_one+1;}
}
}
if(num_one>1)
{
m.src="unchecked.gif";
t.style.display="none";
on_off_data = on_off_data.substring(0,n) + "0" + on_off_data.substring(n+1);
localStorage.setItem(storage_name, on_off_data);
}
}
function sub_table_close(t_id ,d_name)
{
var t = document.getElementById(t_id);
var enable_name = d_name.split("~");
var d;
t.style.display='none';
if(d_name.indexOf("~")>0)
{
for (i = 0; i <  enable_name.length; i++)
{
d = document.getElementsByName(enable_name[i]);
if(d[0]!=undefined)
{d[0].disabled = false;}
}
}
else
{
d = document.getElementsByName(d_name);
if(d[0]!=undefined)
{d[0].disabled = false;}
}
}
function table_set_state(e)
{
e.stopPropagation();
var name = e.data.name;
var m = document.getElementById(e.data.m_id);
var t = document.getElementById(e.data.t_id);
var n= e.data.n;
var on_off;
var on_off_data=localStorage.getItem(name);
var num_one=0
for (i = 0; i <  on_off_data.length; i++)
{
table_id="table_zone_"+(i+1);
title=$("#" + table_id + " span[name='common_title']").html();
if(show_common_menu[i]==1 && on_off_data[i]=="1")
{
if (typeof(hidden_menu_name) != "undefined")
{
is_have_hidden_name=0;
for (j = 0; j <  hidden_menu_name.length; j++)
{
if(title==hidden_menu_name[j])
{is_have_hidden_name=1;}
}
if(is_have_hidden_name==0)
{num_one=num_one+1;}
}
else
{num_one=num_one+1;}
}
}
if(m!=null && t!=null)
{
if(t.style.display=="")
{
m.src="unchecked.gif";
t.style.display="none";
on_off="0";
}
else
{
m.src="checked.gif";
t.style.display="";
on_off="1";
}
if(num_one==1)
{
m.src="checked.gif";
t.style.display="";
on_off="1";
}
on_off_data=on_off_data.substring(0,n) + on_off + on_off_data.substring(n+1);
localStorage.setItem(name, on_off_data);
}
}
function is_show_widget_button()
{
if(document.getElementById('drag_sort_table') != null)
{
$("#set_table").button();
var set_table = document.getElementById("set_table");
set_table.style.display="";
var is_open_menu=false;
var on_off_data = localStorage.getItem(global_on_off_name);
if (typeof(hidden_menu_name) != "undefined")
{
for (i = 0; i <  hidden_menu_name.length; i++)
{hidden_menu_name[i]=hidden_menu_name[i].replace("&amp;","&");}
}
for (k = 0; k < on_off_data.length; k++)
{
table_id="table_zone_"+(k+1);
title=$("#" + table_id + " span[name='common_title']").html();
title=title.replace("&amp;","&");
if(on_off_data[k]=="0" && show_common_menu[k] == 1)
{
if (typeof(hidden_menu_name) != "undefined")
{
is_have_hidden_name=0;
for (i = 0; i <  hidden_menu_name.length; i++)
{
if(title==hidden_menu_name[i])
{is_have_hidden_name=1;}
}
if(is_have_hidden_name==0)
{is_open_menu=true;}
}
else
{is_open_menu=true;}
}
}
if(is_open_menu)
{
set_table = document.getElementById("set_table");
common_menu = document.getElementById("common_menu");
common_menu_line = document.getElementById("common_menu_line");
common_menu_line.style.height=28;
if (typeof(hidden_menu_name) != "undefined")
{
for (i = 0; i <  hidden_menu_name.length; i++)
{
for (j = 0; j <  menu_name_to_id.length; j++)
{
if(menu_name_to_id[j]==hidden_menu_name[i])
{
document.getElementById("table_state_"+(j+1)).style.display="none";
break;
}
}
}
}
common_menu.style.display="";
common_menu_line.style.display="";
common_menu.style.top=set_table.offsetTop+set_table.offsetHeight-1;
common_menu.style.left=set_table.offsetLeft+set_table.offsetWidth-common_menu.offsetWidth-5;
common_menu_line.style.top=set_table.offsetTop+set_table.offsetHeight-1;
common_menu_line.style.left=set_table.offsetLeft+set_table.offsetWidth-common_menu.offsetWidth+18;
common_menu_line.style.height=28*common_menu_line_height;
if (typeof(menu_enable_number) != "undefined")
{common_menu_line.style.height=28*menu_enable_number;}
}
}
}
