function chkwlanurl()
{
loadXMLDocsAsync("/wps.xml?Nmode=1",loadxml_chkwlanurl);
}
function loadxml_chkwlanurl(xmlDoc){
wix=xmlDoc.getElementsByTagName("operationmode")[0].childNodes[0].nodeValue
reg=new RegExp("~"+mwlan[1])
reg=/~wpa/
SubMenulk[0]=SubMenulk[0].replace(reg,"~"+wmurl[wix]);
}
