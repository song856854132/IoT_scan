url: xmlfile[Ntype],
type: 'GET',
dataType: 'xml',//texthtml
cache:false,
error: function(xml){
},
success: function(xml){
var change_time = false;
var time = $(xml).find("time");
var memory_used = $(xml).find("mem_usage");
data0 = [];
options["yaxis"].max = memory_total;
options["yaxis"].tickSize = Math.floor(memory_total/10);
if(time.length>0){
for(var a=0;a<time.length;a++)
{
if(a>0){
var diff_time = (time.eq(a).text() - time.eq(a-1).text())/(updateInterval_arr[Ntype]);//get range time,ex:1s
if(diff_time!=1){
change_time = true;
parent.trigger('now');
}
}
