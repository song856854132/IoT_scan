div.appendChild(document.createTextNode(s));
return div.innerHTML;
}
function init_switch_ui2019(chkid){
itm=eval("document.getElementById('"+chkid+"')")
if(!itm.checked) {all_disabled_ui2019(chkid);}
}
function all_disabled_ui2019(chkidd,hidstatt,hidendd){
f=document.forms[0];
itmm=eval("document.getElementById('"+chkidd+"')");
if(arguments.length == 1){
hidstatt = "switch_start";
hidendd = "switch_end";
}
var sw_range1 = document.getElementsByName(hidstatt);
var sw_range2 = document.getElementsByName(hidendd);
var ary_rg1 = [];  // name="switch_startX , }lelement
var ary_rg2 = [];  // name="switch_endX , element
var t = 0 ; var t1 = 0 ;
var sw_Elm = false ;
for(q=0;q<f.length;q++){
if(sw_Elm){
if(f.elements[q].type=="hidden"   //hidden},Kybug
||f.elements[q].id=="_wary_99" //save button
||f.elements[q].id=="_wary_100" //undo button
||f.elements[q].id=="_rebo_100" //reboot button
||f.elements[q].id=="_wary_101" //refresh button
||f.elements[q].id=="set_table" //Widget button
||f.elements[q].id==chkidd //}v
){} // null
else{
if(itmm.checked){ //all items enable
f.elements[q].disabled=false;
}
else{            //all items disable
f.elements[q].disabled=true;
}
}
}
if((t < sw_range1.length)&&(f.elements[q].name == sw_range1[t].name)){
ary_rg1.push(hidstatt+t+"_"+q);
t = (t+1);
}
if((t1 < sw_range2.length)&&(f.elements[q].name == sw_range2[t1].name)){
ary_rg2.push(hidendd+t1+"_"+q);
t1 = (t1+1);
}
sw_Elm = ((t>0)&&(t>t1))?true:false ;
}
}
function fm_disabled_ui2019(chkidd){
f=document.forms[0];
itmm=eval("document.getElementById('"+chkidd+"')");
for(p=0;p<f.length;p++){
if(f.elements[p].type=="hidden"   //hidden},Kybug
||f.elements[p].id=="_wary_99" //save button
||f.elements[p].id=="_wary_100" //undo button
||f.elements[p].id=="_rebo_100" //reboot button
||f.elements[p].id=="_wary_101" //refresh button
||f.elements[p].id=="set_table" //Widget button
||f.elements[p].id==chkidd //Lv(`})
) continue;
if(itmm.checked){ //all items enable
f.elements[p].disabled=false;
}
else{            //all items disable
f.elements[p].disabled=true;
}
}
}
