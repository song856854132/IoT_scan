L2_button_row = "<TR "+ showl2 +"><td><table border=0 width=100%><TR CLASS=handlk "+L2_click_event;
L2_class_name = "LM2_mouse";
}
else { // other L2
L2_button_row = "<TR "+ showl2 +"><td><table border=0 width=100%><TR CLASS=handlk onMouseOver=\"changeClass('TA0_"+ L2ID +"','LM2_mouse')\" onMouseOut=\"changeClass('TA0_"+ L2ID +"','LM2')\" "+L2_click_event;
L2_class_name = "LM2";
}*/
L2_button_row = "<TR "+ showl2 +"><td><table border=0 width=100%>";
L2_button_row = L2_button_row+"<TR CLASS=handlk ";
if(localStorage.getItem("L2_menu_id")!=("TA0_"+ L2ID))
{
L2_button_row = L2_button_row+"onMouseOver=\"changeClass('TA0_"+ L2ID +"','LM2_mouse')\" ";
L2_button_row = L2_button_row+"onMouseOut=\"changeClass('TA0_"+ L2ID +"','LM2')\" ";
}
L2_button_row = L2_button_row+"onClick=\"save_L2_menu_id("+l+","+i+","+"'TA0_"+L2ID+"','"+L2_link+"',"+L3_num+")\">";
if(found_first_L2==false && L2_link.indexOf("remove_this")==-1) { // find the first available L2 button
show_L1_button(l, L2_link); // show the L1 button with the link of the first available L2
found_first_L2 = true; // found first L2
}
if(setTopMenus(TopMenus[l])=="")
{L2_button_str = L2_button_str + L2_button_row+ "<TD HEIGHT=19 id='TA0_"+ L2ID +"' CLASS='LM2' NOWRAP>&nbsp;<IMG SRC=ag219.gif>&nbsp;<A id='TA_"+ L2ID +"' xHREF=#>" + sms[i] +"</A></TD></TR>"+L3_button(l,i)+"</table></td></tr>\n";}
else
{L2_button_str = L2_button_str + L2_button_row+ "<TD HEIGHT=19 id='TA0_"+ L2ID +"' CLASS='LM2' NOWRAP>&nbsp;<IMG SRC=ag219.gif>&nbsp;<A id='TA_"+ L2ID +"' xHREF=#>" + sms[i] +"</A></TD></TR></table></td></tr>\n";}
}
if(found_first_L2 == false) { // none of available L2 buttons
show_L1_button(l, null);
}
dw(L2_button_str);
dw("</table></td></tr></div>");
}
dw("<TR><TD HEIGHT=100% valign=bottom colspan=3></TD></TR>");
dw("</TABLE>");
}
