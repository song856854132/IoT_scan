function getCookie(cname) {
var name = cname + "=";
var ca = document.cookie.split(';');
for(var i=0; i<ca.length; i++) {
var c = ca[i];
while (c.charAt(0)==' ') c = c.substring(1);
if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
}
return "";
}
function get_csrf_token(){
var csrf_cookie_name = "CSRFTOKEN";
return getCookie(csrf_cookie_name);
}
function append_url(url) { // we can modify this function to append paramters to url
url += (url.indexOf("?")==-1) ? "?" : "&";
var t = new Date();
return url+'ZT='+t.getTime()+'&csrftok='+get_csrf_token();
}
function GURL(x) {
window.location = append_url(x);
}
function sGURL(x) { // We can use this function to send ALTER or modify CSIDs in secret.
top.Hide.location = append_url(x);
}
function mGURL2(x) {
if(top.frames['main']!=null)top.frames['main'].location = append_url(x);
else location = append_url(x);
}
function mGURL(x) {
if(top.frames['main']!=null) top.frames['main'].location = append_url(x);
else top.location = append_url(x);
}
