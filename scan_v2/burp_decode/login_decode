function LM() { // show left menu
var ck_loginL = (typeof(left_menu_login) != "undefined");
if(ck_loginL && left_menu_login && !login) {
LM_loginL();
return;
}
LP=setLMtp();
LS=setLMts();
dw("<TABLE BORDER=0 WIDTH=200 HEIGHT='100%' CELLPADDING=0 CELLSPACING=0 align=center>")
dw("<TR><TD HEIGHT=10 colspan=3></TD></TR>")
for (l=0;l<TopMenus.length;l++){
var found_first_L2 = false; // use this flag to record whether found the first L2 button or not
sm=SecMenus[l];
sms=sm.split("~");
smu=SecMenulk[l];
smus=smu.split("~");
sml2=TAID_L2[l];         //L2
sml2s=sml2.split("~");   //L2 TAID
for (k=0;k<sms.length;k++){
if(sms[k]=="") sms.splice(k,1);
if(smus[k]=="") smus.splice(k,1);
}
var L2_button_str = "";
for(i=0;i<sms.length;i++){
showl2=(sms[i].indexOf("remove_this")!=-1)? "style='display:none'" : ""
c_link=smus[i]
L2ID=sml2s[i] //L2 TAID
if(c_link.indexOf("remove_this")!=-1){ //L3comporeomve, L2link
smv=SecMenuv[l];
try{    //L2smvs=smv.split("|")try
smvs=smv.split("|");
smvss=smvs[i].split(",")
for(a=2;a<smvss.length;a++){
if(eval(smvss[a]+"[1]").indexOf("remove_this")==-1){ //L2link
c_link=eval(smvss[a]+"[1]");
c_link=c_link.substring(0,c_link.length-1) //remove "~"
break;
}
}
}catch(e){}
}
if(setTopMenus(TopMenus[l])=="")
{
var L3_name=ThreeMenus[l].split("|");
var L3_name_show;
var str;
var L3_num=0;
str=L3_name[i];
str=str.substring(0, str.length-1);
L3_name_show=str.split("~");
for(i1=0;i1<L3_name_show.length;i1++)
{
if(L3_name_show[i1].indexOf("remove_this")==-1)
{L3_num=L3_num+1;}
}
}
if(l==0) sloc="top.frames['main'].location" //wizardmGURLGURL....liwei
else sloc="location"
var L2_link = clk(c_link); // L2 url link
var L2_click_event="ONCLICK=\"display_L3_menu('L3_e_"+l+"_"+i+"')";
if(L3_num==1)
{L2_click_event="ONCLICK=\""+sloc+"='" + L2_link +"'";}
/*
if(LP==l&&LS==i){ // selected L1 and selected L2
L2_button_row = "<TR "+ showl2 +"><td><table border=0 width=100%><TR CLASS=handlk "+L2_click_event;
L2_class_name = "LM2_mouse";
}
else { // other L2
L2_button_row = "<TR "+ showl2 +"><td><table border=0 width=100%><TR CLASS=handlk onMouseOver=\"changeClass('TA0_"+ L2ID +"','LM2_mouse')\" onMouseOut=\"changeClass('TA0_"+ L2ID +"','LM2')\" "+L2_click_event;
L2_class_name = "LM2";
}*/
L2_button_row = "<TR "+ showl2 +"><td><table border=0 width=100%>";
L2_button_row = L2_button_row+"<TR CLASS=handlk ";
if(localStorage.getItem("L2_menu_id")!=("TA0_"+ L2ID))
{
L2_button_row = L2_button_row+"onMouseOver=\"changeClass('TA0_"+ L2ID +"','LM2_mouse')\" ";
L2_button_row = L2_button_row+"onMouseOut=\"changeClass('TA0_"+ L2ID +"','LM2')\" ";
}
L2_button_row = L2_button_row+"onClick=\"save_L2_menu_id("+l+","+i+","+"'TA0_"+L2ID+"','"+L2_link+"',"+L3_num+")\">";
if(found_first_L2==false && L2_link.indexOf("remove_this")==-1) { // find the first available L2 button
show_L1_button(l, L2_link); // show the L1 button with the link of the first available L2
found_first_L2 = true; // found first L2
}
if(setTopMenus(TopMenus[l])=="")
{L2_button_str = L2_button_str + L2_button_row+ "<TD HEIGHT=19 id='TA0_"+ L2ID +"' CLASS='LM2' NOWRAP>&nbsp;<IMG SRC=ag219.gif>&nbsp;<A id='TA_"+ L2ID +"' xHREF=#>" + sms[i] +"</A></TD></TR>"+L3_button(l,i)+"</table></td></tr>\n";}
else
{L2_button_str = L2_button_str + L2_button_row+ "<TD HEIGHT=19 id='TA0_"+ L2ID +"' CLASS='LM2' NOWRAP>&nbsp;<IMG SRC=ag219.gif>&nbsp;<A id='TA_"+ L2ID +"' xHREF=#>" + sms[i] +"</A></TD></TR></table></td></tr>\n";}
}
if(found_first_L2 == false) { // none of available L2 buttons
show_L1_button(l, null);
}
dw(L2_button_str);
dw("</table></td></tr></div>");
}
dw("<TR><TD HEIGHT=100% valign=bottom colspan=3></TD></TR>");
dw("</TABLE>");
}
