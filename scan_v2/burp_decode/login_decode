function logout()
{
mGURL("/log/out?rd=/uir/start.htm?Nrd=2");
}
function clk(link)
{
var links=link;
if(link=="#")return link
if((link.indexOf(':')==-1)&&(link.indexOf('.')==-1)&&(link.indexOf('=')==-1))links="/"+link+".htm?rc="
return links
}
