function clrsel(b)
{
cfrom=(arguments.length==1)?0:arguments[1]
bl=b.length
for(j=0;j<bl-cfrom;j++)
{
b.options[cfrom].value=""
b.options[cfrom]=null
}
}
function pushselopt(sel,stext,sval)
{
sopt=new Option(stext,sval)
sellen=sel.length
sel.options[sellen]=sopt
}
function findInArr3(findkey,arr)
{
if(arr.length==0)return false
for (x in arr)
{
if(arr[x] == findkey)return x
}
return false
}
/***************** begin: function designed by Leaf ****************/
function setEnable(isCheck, hiddenID)
{
document.getElementById(hiddenID).value = ((isCheck.checked)? 1 : 0);
}
function getEnable(APEnable, Echeckbox)
{
var isEnable = document.getElementById(APEnable);
if(isEnable.value == "1"){
document.getElementById(Echeckbox).checked = true;
}
}
function disablebyDiv(div_id, isdisable)		//disable elements in Div
{
var target_id = document.getElementById(div_id);
disableElem(target_id, isdisable);
}
function disableElem(elem, disable)			//disable elements
{
var childs = elem.childNodes;
for(var i = 0; i < childs.length; i++){
if(childs[i].nodeType == 1){
childs[i].disabled = disable;
if(childs[i].childNodes){
disableElem(childs[i], disable);
}
}
}
}
/***************** end: function designed by Leaf ****************/
function init_prim_switch(chkid){
itm=eval("document.getElementById('"+chkid+"')")
if(!itm.checked) {prim_switch(chkid);}
}
function prim_switch(chkidd){
f=document.forms[0]
itmm=eval("document.getElementById('"+chkidd+"')")
for(s=0;s<f.length;s++){if(f.elements[s].name=="switch_start")break;}
for(e=0;e<f.length;e++){if(f.elements[e].name=="switch_end")break;}
for(p=s;p<e;p++){
if(f.elements[p].type=="hidden"   //hidden},Kybug
||f.elements[p].id=="_wary_99" //save button
||f.elements[p].id=="_wary_100" //undo button
||f.elements[p].id=="_rebo_100" //reboot button
) continue;
if(itmm.checked){ //all items enable
f.elements[p].disabled=false;
}
else{            //all items disable
f.elements[p].disabled=true;
}
}
}
function vwan_switch(chkidd,chkenablee,objname){
f=document.forms[0]
start_name="switch_start";
end_name=objname+"_switch_end";
if(objname){
start_name=objname+"_switch_start";
end_name=objname+"_switch_end";
}
itmm=eval("document.getElementById('"+chkidd+"')")
for(s=0;s<f.length;s++){if(f.elements[s].name==start_name)break;}
for(e=0;e<f.length;e++){if(f.elements[e].name==end_name)break;}
for(p=s;p<e;p++){
if(f.elements[p].type=="hidden"   //hidden},Kybug
||f.elements[p].id=="_wary_99" //save button
||f.elements[p].id=="_wary_100" //undo button
||f.elements[p].id=="_rebo_100" //reboot button
) continue;
if(chkenablee){ //all items enable
f.elements[p].disabled=false;
}
else{            //all items disable
f.elements[p].disabled=true;
}
}
}
function switch_hidden(chkidd,hidstatt,hidendd){
f=document.forms[0]
itmm=eval("document.getElementById('"+chkidd+"')")
for(s=0;s<f.length;s++){if(f.elements[s].name==hidstatt)break; }
for(e=0;e<f.length;e++){if(f.elements[e].name==hidendd)break; }
for(p=s;p<e;p++){
if(f.elements[p].type=="hidden"   //hidden},Kybug
||f.elements[p].id=="_wary_99" //save button
||f.elements[p].id=="_wary_100" //undo button
||f.elements[p].id=="_rebo_100" //reboot button
) continue;
if(itmm.checked){ //all items enable
f.elements[p].disabled=false;
chgprimode(1)
}
else{            //all items disable
f.elements[p].disabled=true;
}
}
}
/*
 prim_switch O iHv]w start  end id W
chkidd => chekcbox id
hidstatt => disabled start index
hidendd => disabled end index
by kent
*/
function all_disabled(chkidd,hidstatt,hidendd)
{
f=document.forms[0]
itmm=eval("document.getElementById('"+chkidd+"')")
for(s=0;s<f.length;s++){if(f.elements[s].name==hidstatt)break;}
for(e=0;e<f.length;e++){if(f.elements[e].name==hidendd)break;}
for(p=s;p<e;p++){
if(f.elements[p].type=="hidden"   //hidden},Kybug
||f.elements[p].id=="_wary_99" //save button
||f.elements[p].id=="_wary_100" //undo button
||f.elements[p].id=="_rebo_100" //reboot button
||f.elements[p].id=="_wary_101" //refresh button
) continue;
if(itmm.checked){ //all items enable
f.elements[p].disabled=false;
}
else{            //all items disable
f.elements[p].disabled=true;
}
}
}
/*
get xml node value
xmlDoc => xml objec
xml_name => tag name
value_index => value index
node = null => return empty
node = object => return nodeValue
by kent
*/
function xml_getvalue(xmlDoc,xml_name,value_index)
{
var value = xmlDoc.getElementsByTagName(xml_name)[value_index];
value = value.childNodes[0];
return (value != null)?value.nodeValue:"";
}
function Interfc_info(val){//get e router  wan , Lan and Vlan Info
/*
aryW : e router  wan Operation Mode's status
aryW[Wan1,Wan2,Wan3,...]
aryW[?]=1:Failover ; aryW[?]=0:Always on ; aryW[?]=2:Disable
aryVL : e router  LAN and VLan Enable's status
aryVL[LAN,VLAN1,VLAN2,VLAN3,...]
aryVL[0]=1:Lan Enabled.
aryVL[?]=1:VLan? Enabled.
val =0:return aryVL ; val =1:return aryW
*/
/*
C_WEBUI_WAN_TMP_OPMODE ^rN wan Operation Mode  status('-'jCWan)
C_VLAN_GROUP_ACTIVE  ^rN lan vlan1~3 _Q
vlan_enable[0]~[2] N vlan1~3 _Q
C_VLAN_GROUP_ACTIVE  | lan | valn1 | valn2 | valn3|
15           |  1  |   1   |   1   |   1  |
14           |  1  |   1   |   1   |   0  |
13           |  1  |   1   |   0   |   1  |
12           |  1  |   1   |   0   |   0  |
11           |  1  |   0   |   1   |   1  |
.........
*/
var wan_value = "";
var vlan_enable = [];
var vlan_active = parseInt("8");
var vlan_actice_arr = [];
var aryVL = [];
var aryW =[];
var str00= "N2"  //"del
vlan_enable[0] = 0;
vlan_enable[1] = 0;
vlan_actice_arr[0] = 8 & vlan_active;  //ck Lan
vlan_actice_arr[1] = 4 & vlan_active;  //ck VLan1
vlan_actice_arr[2] = 2 & vlan_active;  //ck VLan2
vlan_actice_arr[3] = 1 & vlan_active;  //ck VLan3
aryVL[0] = (vlan_actice_arr[0] != 0)?1:0   // PO lan O_Q
for(c=0;c<(vlan_enable.length-1);c++){  // PO vlan 1~x O_Q
aryVL[c+1] = (vlan_actice_arr[c+1] != 0)?"1":"0" ; // 20170104  C_VLAN_SVR_ENABLE
/*alert("del dd"+(c+1)+"="+aryVL[c+1]
+" ,vlan_actice_arr[c+1]="+vlan_actice_arr[c+1]
+" ,vlan_enable[c]="+vlan_enable[c]
+" ,vlan_actice_arr[c+1]="+vlan_actice_arr[c+1]
)*/
}
wan_value = "0-2".split("-");
for(q=0;q<wan_value.length;q++){  // PO Wan 1~x O_Q
aryW[q] = wan_value[q] ;
}
return (val==0)?aryVL:aryW
}
/*=================== wps.js =================================*/
var http_request,staret;
var mesg=new Array("&nbsp;","&nbsp;")
var elem=new Array()
function makeRequest(url,elem) {
http_request = false;
var t=new Date()
if (window.XMLHttpRequest) { // Mozilla, Safari,...
http_request = new XMLHttpRequest();
} else if (window.ActiveXObject) { // IE
try {
http_request = new ActiveXObject("Msxml2.XMLHTTP");
} catch (e) {
try {
http_request = new ActiveXObject("Microsoft.XMLHTTP");
} catch (e) {}
}
}
if (!http_request) {
alert('Giving up :( Cannot create an XMLHTTP instance');
return false;
}
http_request.open('GET', url+t.getTime(), true);
if(arguments.length > 1)
http_request.onreadystatechange = stateChangedXML;
else
http_request.onreadystatechange = stateChangedText;
http_request.send(null);
}
function loadXMLDocs(dname)//loadXMLDocs can't be used anymore becaause of Synchronize.
{
if(dname.indexOf("?")!=-1 && dname.indexOf("csrftok=")==-1) {
dname +='&csrftok='+get_csrf_token(); // append CSRF token to avoid changing CSID without sending csrftok
}
try
{
if (window.ActiveXObject)
{
var errormsg = "Check Browser and security settings";
xmlDoc = new ActiveXObject("Microsoft.XMLDOM");
xmlDoc.async=false;
xmlDoc.load(dname);
}
else if(window.XMLHttpRequest)
{
var errormsg = "Error handling XMLHttpRequest request";
var d = new XMLHttpRequest();
d.open("GET", dname, false);
d.send(null);
xmlDoc=d.responseXML.documentElement;
} else {
var errormsg = "Error.";
xmlDoc = document.implementation.createDocument("","",null);
xmlDoc.async=false;
xmlDoc.load(dname);
}
return xmlDoc;
}
catch(e)
{
return null;
}
}
function loadXMLDocsAsync(url, success_func, error_func, complete_func) {
var httpRequest = new XMLHttpRequest();
httpRequest.onreadystatechange = function() {
if(httpRequest.readyState == 4) { // request finished
if(httpRequest.status == 200) { // 200 OK response
success_func(httpRequest.responseXML); // pass the XMLDoc to success function
}
else { // There was a problem with the request, such as 404 (Not Found)
if(typeof(error_func)!="undefined") {
error_func(); // execute error function if exists
}
}
if(typeof(complete_func)!="undefined") {
complete_func(); // execute complete function if exists
}
}
};
httpRequest.open('GET', url, true);
httpRequest.send('');
}
function rlists(xmlurls,frs)
{
rlist(xmlurls,frs)
setTimeout("rlists('"+xmlurls+"','"+frs+"')",staret*1000)
}
function rlist(xmlurl,fr)
{
xmlDoc=null;
loadXMLDocsAsync(xmlurl,
function(fr){ // ajax success function START
return function(xml){
var xmlDoc=xml;
z=xmlDoc.getElementsByTagName("item")
if(z==null)return;
if(z.length <= 0)return;
var sw_topology=(typeof(swm_status_network) != "undefined")?swm_status_network:false;
for(j=0;j<z.length;j++)
{
zn=z[j].childNodes[0]
nv=(zn!=null)?zn.nodeValue:"&nbsp;"
ddiv=document.getElementById(z[j].getAttribute("name"))
if(ddiv!=null && ddiv.innerHTML!=nv){
ddiv.innerHTML=(nv=="")?"&nbsp;":nv //change value
}
}
if(fr=="status"){                 //from status.htm
reload_array(0,xmlDoc);   //rewrite Array local_conn and multi_conn
if(sw_topology) change_topology(); //base on local_conn and multi_conn`s value show topology
set_v4wan_value();
}
else if(fr=="sta_wireless"){      //from sta_wireless.htm
reload_array(1,xmlDoc);
if(sw_topology) change_topology();
chk_auth(); //for QR Code
chgLang();
}
else if(fr=="no_chglang") return; //@iJstatus.htm,]sta_refresh.xmlnrun chgLang(1),Mstatus.htm|run chgLang(1)
else if(fr=="sta_vpn"){           //from sta_vpn.htm
submk2v4(); //for ipsec Local Subnets and Remote Subnets
try{sw_dynvpn_client();} catch(e){}
vpn_refresh(z.length);
}
else if(fr=="diagnostic"){
trans_diag();
}
/*
statusbxml reload@stringiQsN, ex. "enable" "disable" "connect" "disconnect"
ostring|^
nkOustring
kObxmlspan idRW "_1~xml"  "_atbox_1~xml"
|Nstatus.htmspan
Mrun chgLang(1)...blang.jsr,NuidO~xmlstring
*/
chgLang(1) //only
};
}(fr) // ajax success function END
);
}
function reload_array(f,xmlDoc){
if(f==0){ //status.htm only
for(a=0;a<ALL_INDEXV_sta.length-1;a++){
eval("ALL_INDEXV_sta[a]=xmlDoc.getElementsByTagName('wan_ind"+a+"')[0].childNodes[0].nodeValue")
}
for(a=0;a<ALL_mac_sta.length-1;a++){
if(xmlDoc.getElementsByTagName("mac_ind"+a)[0].childNodes[0]!=null)
eval("ALL_mac_sta[a]=xmlDoc.getElementsByTagName('mac_ind"+a+"')[0].childNodes[0].nodeValue")
}
}
if(xmlDoc.getElementsByTagName("locon_ind")[0].childNodes[0]!=null){
local_conn[0]=xmlDoc.getElementsByTagName("locon_ind")[0].childNodes[0].nodeValue
}
for(a=0;a<16;a++){
if(xmlDoc.getElementsByTagName("mucon_ind"+a)[0].childNodes[0]!=null)
eval("multi_conn[a]=xmlDoc.getElementsByTagName('mucon_ind"+a+"')[0].childNodes[0].nodeValue")
}
}
/*=================== wps.js =================================*/
var xmlhttp;
function loadXMLDoc_post(url,val)
{
xmlhttp=null;
if (window.XMLHttpRequest)
{// code for all new browsers
xmlhttp=new XMLHttpRequest();
}
else if (window.ActiveXObject)
{// code for IE5 and IE6
xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
}
if (xmlhttp!=null)
{
xmlhttp.open("POST",url,true);
xmlhttp.setRequestHeader("Content-type","application/x-www-form-urlencoded");
xmlhttp.send(val+'&csrftok='+get_csrf_token());
}
else
{
}
}
