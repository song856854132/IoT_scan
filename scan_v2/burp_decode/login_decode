dw("<TD nowrap><IMG SRC=sp.gif WIDTH=10 HEIGHT=27 ALIGN=ABSMIDDLE><IMG SRC=dot1.gif WIDTH=16 HEIGHT=9 ALIGN=ABSMIDDLE><span name='common_title'>"+tl+"</span></TD>")
if(arguments.length==2)
{
if(hplk.indexOf("|")!=-1)
{
e=hplk.split("|")
lkn=e[0]
hplk=e[1]
}
else lkn=(hplk.indexOf("h-")!=-1)?GetElmCont('wary_0','Help'):GetElmCont('wary_88','Back')
if(hplk=="prim")
dw("<TD ALIGN=RIGHT><A HREF='javascript:history.go(-1)'>[ "+lkn+" ]</A></TD>")
