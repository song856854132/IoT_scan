function all_disabled_ui2019(chkidd,hidstatt,hidendd){
f=document.forms[0];
itmm=eval("document.getElementById('"+chkidd+"')");
if(arguments.length == 1){
hidstatt = "switch_start";
hidendd = "switch_end";
}
var sw_range1 = document.getElementsByName(hidstatt);
var sw_range2 = document.getElementsByName(hidendd);
var ary_rg1 = [];  // name="switch_startX , }lelement
var ary_rg2 = [];  // name="switch_endX , element
var t = 0 ; var t1 = 0 ;
var sw_Elm = false ;
for(q=0;q<f.length;q++){
if(sw_Elm){
if(f.elements[q].type=="hidden"   //hidden},Kybug
||f.elements[q].id=="_wary_99" //save button
||f.elements[q].id=="_wary_100" //undo button
||f.elements[q].id=="_rebo_100" //reboot button
||f.elements[q].id=="_wary_101" //refresh button
||f.elements[q].id=="set_table" //Widget button
||f.elements[q].id==chkidd //}v
){} // null
else{
if(itmm.checked){ //all items enable
f.elements[q].disabled=false;
}
else{            //all items disable
f.elements[q].disabled=true;
}
}
}
if((t < sw_range1.length)&&(f.elements[q].name == sw_range1[t].name)){
ary_rg1.push(hidstatt+t+"_"+q);
t = (t+1);
}
if((t1 < sw_range2.length)&&(f.elements[q].name == sw_range2[t1].name)){
ary_rg2.push(hidendd+t1+"_"+q);
t1 = (t1+1);
}
sw_Elm = ((t>0)&&(t>t1))?true:false ;
}
}
function fm_disabled_ui2019(chkidd){
f=document.forms[0];
itmm=eval("document.getElementById('"+chkidd+"')");
for(p=0;p<f.length;p++){
if(f.elements[p].type=="hidden"   //hidden},Kybug
||f.elements[p].id=="_wary_99" //save button
||f.elements[p].id=="_wary_100" //undo button
||f.elements[p].id=="_rebo_100" //reboot button
||f.elements[p].id=="_wary_101" //refresh button
||f.elements[p].id=="set_table" //Widget button
||f.elements[p].id==chkidd //Lv(`})
) continue;
if(itmm.checked){ //all items enable
f.elements[p].disabled=false;
}
else{            //all items disable
f.elements[p].disabled=true;
}
}
}
/* --- for UI2019  end ---------------------- */
/* ----------  for pop up start -------------- */
function swpop_fancybox(val,val2){
t=new Date();
obj1 = document.getElementById(val2);
obj1.src=val+"&ZT="+t.getTime()+'&csrftok='+get_csrf_token();
}
function showAction_pIPD_popup(div_id,iframe_id,div_iframe_id,min_width,is_iframe) {
var totalTop=0;
var totalHeight=0;
var totalWidth=0;
var totalLeft=0;
var jobj = $('#'+div_id);
var scrolltopv = $(window).scrollTop();
var viewv = window.innerHeight;
totalHeight = $('#LT1').height()-$('.L3menu').height();
totalTop = jobj.offset().top;
L3_long=false;
if($('.fancybox-outer').height()>=100)
{L3_long=true;}
if(L3_long){
var bottom_height = (document.body.scrollHeight-totalTop-totalHeight);
var scroll_bottom = $("body").height()-viewv;
var set_viewv = viewv - totalTop - bottom_height;
if (totalHeight > set_viewv){
totalHeight = viewv;
totalHeight = (totalTop>scrolltopv)?(totalHeight-(totalTop-scrolltopv)):totalHeight;
if((scroll_bottom - scrolltopv)<bottom_height){
totalHeight = totalHeight - (bottom_height - (scroll_bottom - scrolltopv));
}
}
totalTop = (totalTop < scrolltopv)?scrolltopv:totalTop;
}else{
if(iframe_id!=undefined)
{
var obj1 = $('#'+iframe_id);
last=$("body").height()-$(window).height()-50;
if((obj1 !=  null) && ($(window).scrollTop()>=last)){
var ifrm_H = parseInt(obj1.height());
var Winn_H = parseInt(window.innerHeight);
var nb1 = ((parseInt(totalHeight) /2) - (ifrm_H/2 ));
var strPos = 10;
if(parseInt(totalHeight) > ifrm_H){
if( (Winn_H - 60 ) <= ifrm_H ){
strPos= nb1 +30;
}
else{ strPos = nb1; }
}
$(window).scrollTop(strPos);
}
}
}
totalLeft = jobj.offset().left;
totalWidth = jobj.width();
var viewv_width = window.innerWidth;
right_width=viewv_width-totalLeft-totalWidth;
right_width=(right_width>0)?right_width:0;
set_viewv_width = viewv_width - totalLeft - right_width;
totalWidth = (totalWidth<set_viewv_width)?totalWidth:set_viewv_width;
totalWidth+=1;
$.fancybox.helpers.overlay.open({
'parent': jobj,
'closeClick' : false,
'css' : {
'position': 'absolute',
'height': totalHeight,
'width': totalWidth,
'top': totalTop,
'left': totalLeft,
'overflow': 'hidden'
}});
$.fancybox.update();
$(window).resize(function(){
$.fancybox.update()
$(window).unbind(); // unbind resize event
if(iframe_id==undefined)
{showAction_pIPD_popup(div_id);}
else
{
showAction_pIPD_popup(div_id,iframe_id,div_iframe_id,min_width,is_iframe);
iframe_object=document.getElementById(iframe_id);
div_iframe_object=document.getElementById(div_iframe_id);
box_width=jobj.width()*0.6;
if(box_width<min_width)
{box_width=min_width;}
}
});
$(window).keydown(function(e){//unbind resize event when clicking Esc button
var key_code = e.which;
if(key_code==27){
$(window).unbind(); // unbind resize event
}
})
}
function close_fancybox_popup(_id){
$(window).unbind("resize"); // unbind resize event
if(arguments.length > 0){
document.getElementById(_id).src="";
}
}
/* ----------  for pop up end -------------- */
