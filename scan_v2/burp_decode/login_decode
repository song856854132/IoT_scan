function check_login(){
/*      check username cannot be empty      */
var username = document.getElementsByName("xunx")[0].value;
if(username==""){
alert(GetElmCont("_0","Login failed!"));
return false;
}
/*  for check login when Ntry >= N (N:C_HTTP_AUTH_RETRY_MAX), wait 30 seconds  */
var default_second = 30;
var auth_failnum = 3;
var d = new Date();
var now = Date.UTC(d.getFullYear(),d.getMonth(),d.getDate(),d.getHours(),d.getMinutes(),d.getSeconds());
var code_time = "1628241887";
code_time = (code_time == "")?0:parseInt(code_time);
var second = (now/1000)-code_time;
var wait_second = (default_second - second);
if((typeof(Ntry) != "undefined") && ((Ntry%auth_failnum)==0) && wait_second >= 1 && Ntry!=0)
{
alert(GetElmCont('wary_0','Already reaching maximum trying times, please wait a few seconds!'))
return false
}
else
{
document.getElementById("Ntime").value = now/1000;
var frm=document.forms[0];
return true
}
}
function loginValidate ()
{
var txtFieldIdArr = new Array ();
txtFieldIdArr[0] = "txtUserName,Please enter a valid user name";
txtFieldIdArr[1] = "txtLoginPwd,Please enter a valid password";
if (txtFieldArrayCheck (txtFieldIdArr) == false){ return false; }
return true;
}
