if(swm_wifi_module1){ //WiFi Module One
ssid_str="DWM-313-C1FF";
}
if(!swm_wifi_module1&&swm_wifi_module2){  //module1module2module2 CSID
ssid_str="Staff_5G";
}
}
}
if (login){
dw("<td id=langsels>");
if(langselection) { dw("<span class='langsels_span'><span id=_wary_0>Language</span>  :  </span>"+langsel+" &nbsp;"); } // set language select menu
