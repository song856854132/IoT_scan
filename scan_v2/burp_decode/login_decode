function dw(str){document.write(str+'\n')}
function set_L5(comp_L5,comp_L5_2,comp_L5_3)
{
if(comp_L5!=null) dw("<script language=javascript src='"+comp_L5+"_func.js'></script>\n")
if(comp_L5_2!=null) dw("<script language=javascript src='"+comp_L5_2+"_func.js'></script>\n")
if(comp_L5_3!=null) dw("<script language=javascript src='"+comp_L5_3+"_func.js'></script>\n")
}
