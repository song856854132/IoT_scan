function mobile_open_left_menu()
{
var mobile_menu_data = document.getElementById('mobile_menu_data');
if(mobile_menu_data.style.display=="none")
{mobile_menu_data.style.display="";}
else
{mobile_menu_data.style.display="none";}
change_fancybox_shade();
}
