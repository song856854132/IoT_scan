function setLMtp() //1
{
sstp=99;
if(typeof(pages)!="undefined"){
for (i=0;i<ThreeMenulk.length;i++){
if(ThreeMenulk[i].indexOf(pages)!=-1) {
if(ThreeMenulk[i].indexOf(pages+"~")==0){sstp=i; break;}
else if((ThreeMenulk[i].indexOf("|"+pages)!=-1||ThreeMenulk[i].indexOf("~"+pages)!=-1)&&(ThreeMenulk[i].indexOf(pages+"~")!=-1)){sstp=i; break;}
}
if(pages.indexOf("voip")!=-1&&i!=0){
if(ThreeMenulk[i].indexOf("voip")!=-1) {sstp=i; break; }
}
if(typeof(TMtype_s)!="undefined"){
if(ThreeMenulk[i].indexOf("secrselect")!=-1) {sstp=i; break; }
}
}
for (j=0;j<ExMenulk.length;j++){
if(ExMenulk[j].indexOf(pages)!=-1) {sstp=j; break; }
}
}
return sstp
}
function setLMts() //2
{
stp=setLMtp();
sts=0;
if(typeof(pages)!="undefined"&&stp!=99){ //pages,&&stp==99L1
thrslk=ThreeMenulk[stp].split("|")
for (k=thrslk.length-1;k>=0;k--){
if(thrslk[k]=="") {thrslk.splice(k,1); }
}
for (j=0;j<thrslk.length;j++){
if(thrslk[j].indexOf(pages)!=-1) {sts=j; break; }
if(pages.indexOf("voip")!=-1&&pages!="sta_voip"){
if(thrslk[j].indexOf("voip")!=-1) {sts=j; break; }
}
if(typeof(TMtype_s)!="undefined"){
if(thrslk[j].indexOf("secrselect")!=-1) {sts=j; break; }
}
}
}
return sts
}
function setMenus(link)
{
tp=setLMtp();
ts=setLMts();
if (link) //from VoIP
{
/*if(link.indexOf(TMtype+':')!=-1)
{   TAID_L3_VoIP
thrss=(VoIP_SubMenu[TMtype].substring(2)).split("~")
thrslks=(VoIP_SubMenulk[TMtype].substring(2)).split("~")
thrsl3=TAID_L3_VoIP[TMtype] //L3 TAID
}*/
if(typeof(TMtype_s)!="undefined")
{
thrss=(SECR_SubMenu[TMtype_s].substring(2)).split("~")
thrslks=(SECR_SubMenulk[TMtype_s].substring(2)).split("~")
thrsl3=TAID_L3_SECR[TMtype_s] //L3 TAID
}
}
else
{
for (k=ThreeMenus.length-1;k>=0;k--)
{
if(ThreeMenus[k]=="") {ThreeMenus.splice(k,1); }
}
thrs=ThreeMenus[tp].split("|")
thrsl3=TAID_L3[tp].split("|") //L3 TAID
for (l=thrs.length-1;l>=0;l--)
{
if(thrs[l]=="") {thrs.splice(l,1); }
}
thrss=thrs[ts].split("~")
thrslks=thrslk[ts].split("~")
}
pg=""
for (i=0;i<thrss.length;i++)
{
if(thrslks[i]!=""){
if (i==0) lg=""
else lg="|"
if(thrslks[i]==pages) lks=""
else lks=i
pg=pg+lg+thrss[i]+"~"+thrslks[i]+"~"+lks;
}
}
if (link)
return pg+"#"+thrsl3
else
return pg+"#"+thrsl3[ts]
}
function setvoip(link)
{
/*if(link.indexOf(TMtype+':')!=-1){
thrss=(VoIP_SubMenu[TMtype].substring(2)).split("~")
thrslks=(VoIP_SubMenulk[TMtype].substring(2)).split("~")
}*/
for (l=thrss.length-1;l>=0;l--)
{
if(thrss[l]=="") {thrss.splice(l,1); }
if(thrslks[l]=="") {thrslks.splice(l,1); }
}
pg=""
for (i=0;i<thrss.length;i++)
{
if(thrslks[i]!="")
{
if (i==0) lg=""
else lg="|"
if(thrslks[i]==pages) lks=""
else lks=i
pg=pg+lg+thrss[i]+"~"+thrslks[i]+"~"+lks;
}
}
return pg;
}
