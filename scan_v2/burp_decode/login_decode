/* =====(id=zone_1,id=zone_2)===== */
var table_height_all=0;
for(i=0;i<5;i++)
{table_height_all=table_height_all+document.getElementById("table_"+(i+1)).clientHeight;}
table_height_all=table_height_all+1000;
document.getElementById("zone_1").style.height=table_height_all;
document.getElementById("zone_2").style.height=table_height_all;
setTimeout(set_zone_height,500);
function set_zone_height()
{
var id;
var data;
var height;
var height_1=0;
var height_2=0;
device_sort_array_1 = localStorage.getItem("device_dashboard_sort_1").split(",");
device_sort_array_2 = localStorage.getItem("device_dashboard_sort_2").split(",");
for(i=0;i<device_sort_array_1.length;i++)
{
data=device_sort_array_1[i];
id=sort_to_table_id(data);
if(sort_to_delete_id(data)=="1")
{height_1=height_1+document.getElementById(id).clientHeight;}
}
for(i=0;i<device_sort_array_2.length;i++)
{
data=device_sort_array_2[i];
id=sort_to_table_id(data);
if(sort_to_delete_id(data)=="1")
{height_2=height_2+document.getElementById(id).clientHeight;}
}
height=height_1;
if(height_2>height)
{height=height_2;}
height=height+100;
document.getElementById("zone_1").style.height=height;
document.getElementById("zone_2").style.height=height;
}
function sort_to_table_id(data)
{
if(data=="1-1")
{id="table_1";}
else if(data=="1-2")
{id="table_2";}
else if(data=="1-3")
{id="table_3";}
else if(data=="1-4")
{id="table_4";}
else
{id="table_5";}
return id;
}
function sort_to_delete_id(data)
{
var device_dashboard_on_off=localStorage.getItem("device_dashboard_on_off");
if(data=="1-1")
{id=device_dashboard_on_off[0];}
else if(data=="1-2")
{id=device_dashboard_on_off[1];}
else if(data=="1-3")
{id=device_dashboard_on_off[2];}
else if(data=="1-4")
{id=device_dashboard_on_off[3];}
else
{id=device_dashboard_on_off[4];}
return id;
}
/* =====\===== */
var sort_data;
var table_str="";
var zone_1=$("#zone_1");
var zone_2=$("#zone_2");
var device_sort_array_1;
var device_sort_array_2;
var device_sort_array_ui_1 = new Array();
var device_sort_array_ui_2 = new Array();
var device_dashboard_sort_1 = localStorage.getItem("device_dashboard_sort_1");
var device_dashboard_sort_2 = localStorage.getItem("device_dashboard_sort_2");
if(device_dashboard_sort_1 == null)
{localStorage.setItem("device_dashboard_sort_1", "1-1,1-2,1-3,1-4");}
if(device_dashboard_sort_2 == null)
{localStorage.setItem("device_dashboard_sort_2", "2-1");}
device_dashboard_sort_1 = localStorage.getItem("device_dashboard_sort_1");
device_dashboard_sort_2 = localStorage.getItem("device_dashboard_sort_2");
device_sort_array_1 = device_dashboard_sort_1.split(",");
device_sort_array_2 = device_dashboard_sort_2.split(",");
if(device_dashboard_sort_1!="")
{
for (i = 0; i < device_sort_array_1.length; i++)
{
sort_data=device_sort_array_1[i].split("-");
n=parseInt(sort_data[1])-1;
if(sort_data[0]=="1")
{device_sort_array_ui_1[i]=table_string(zone_1.children().get(n).innerHTML);}
else
{device_sort_array_ui_1[i]=table_string(zone_2.children().get(n).innerHTML);}
}
}
if(device_dashboard_sort_2!="")
{
for (i = 0; i < device_sort_array_2.length; i++)
{
sort_data=device_sort_array_2[i].split("-");
n=parseInt(sort_data[1])-1;
if(sort_data[0]=="1")
{device_sort_array_ui_2[i]=table_string(zone_1.children().get(n).innerHTML);}
else
{device_sort_array_ui_2[i]=table_string(zone_2.children().get(n).innerHTML);}
}
}
zone_1.children().remove();
zone_2.children().remove();
for (i = 0; i < device_sort_array_ui_1.length; i++)
{zone_1.append(device_sort_array_ui_1[i]);}
for (i = 0; i < device_sort_array_ui_2.length; i++)
{zone_2.append(device_sort_array_ui_2[i]);}
function table_string(str)
{
a = str.indexOf("script");
b = str.indexOf("/script");
table_str = str.substring(0, a-1) + str.substring(b+8);
table_str = "<div align=center>" + table_str + "</div>";
return table_str;
}
if(!is_mobile_login)
{
var check_sort_update=0;  //tablet@,sortableupdate|2,H]mcheck_sort_updateupdate2
$(function() {
$(".sort_table").sortable({
connectWith: ".sort_table",
placeholder: "table_shade_device",
disabled: true,
start: function(e, ui){
ui.placeholder.height(ui.item.height()-4);
ui.placeholder.width(ui.item.width()-4);
start_index=ui.item.index();
start_id=ui.item.parent().attr('id');
},
update: function(e, ui){
end_index=ui.item.index();
end_id=ui.item.parent().attr('id');
var s_name;
if(start_id==end_id)
{
if(start_id=="zone_1")
{s_name="device_dashboard_sort_1";}
else
{s_name="device_dashboard_sort_2";}
same_zone_sort(s_name,start_index,end_index);
}
else
{
if(check_sort_update==0)
{
check_sort_update=1;
diff_zone_sort(start_id,start_index,end_index);
set_zone_height();
}
else
{check_sort_update=0;}
}
}
});
});
}
function same_zone_sort(sort_name,s,e)
{
zone_sort=localStorage.getItem(sort_name);
zone_sort_array=zone_sort.split(",");
if(s<e)
{
zone_sort_array.splice(e+1 ,0 ,zone_sort_array[s]);
zone_sort_array.splice(s ,1);
}
else
{
zone_sort_array.splice(e ,0 ,zone_sort_array[s]);
zone_sort_array.splice(s+1 ,1);
}
zone_sort="";
for (i = 0; i < zone_sort_array.length; i++)
{zone_sort=zone_sort+zone_sort_array[i]+",";}
zone_sort=zone_sort.substring(0, zone_sort.length-1);
localStorage.setItem(sort_name, zone_sort);
}
function diff_zone_sort(id,s,e)
{
zone_1_sort_array = localStorage.getItem("device_dashboard_sort_1").split(",");
zone_2_sort_array = localStorage.getItem("device_dashboard_sort_2").split(",");
if(id=="zone_1")
{
zone_sort=zone_1_sort_array[s];
zone_1_sort_array.splice(s ,1);
zone_2_sort_array.splice(e ,0 ,zone_sort);
}
else
{
zone_sort=zone_2_sort_array[s];
zone_2_sort_array.splice(s ,1);
zone_1_sort_array.splice(e ,0 ,zone_sort);
}
zone_sort="";
zone_num=zone_1_sort_array.length;
if(zone_1_sort_array[zone_num-1]=="")
{
if(zone_num>1)
{zone_1_sort_array.splice(zone_num-1 ,1);}
}
for (i = 0; i < zone_1_sort_array.length; i++)
{zone_sort=zone_sort+zone_1_sort_array[i]+",";}
zone_sort=zone_sort.substring(0, zone_sort.length-1);
localStorage.setItem("device_dashboard_sort_1", zone_sort);
zone_sort="";
zone_num=zone_2_sort_array.length;
if(zone_2_sort_array[zone_num-1]=="")
{
if(zone_num>1)
{zone_2_sort_array.splice(zone_num-1 ,1);}
}
for (i = 0; i < zone_2_sort_array.length; i++)
{zone_sort=zone_sort+zone_2_sort_array[i]+",";}
zone_sort=zone_sort.substring(0, zone_sort.length-1);
localStorage.setItem("device_dashboard_sort_2", zone_sort);
}
function drag_dash_table_enable()
{
$("#zone_1").sortable("enable");
$("#zone_2").sortable("enable");
}
function drag_dash_table_cancel()
{
$("#zone_1").sortable("disable");
$("#zone_2").sortable("disable");
}
/* =====}table===== */
document.getElementById("set_table").style.display="";
$("#set_table").button();
var is_menu_open="0";
var set_table = document.getElementById("set_table");
var menu = document.getElementById("menu");
var menu_line = document.getElementById("menu_line");
menu.style.top=set_table.offsetTop+set_table.offsetHeight-1;
menu.style.left=set_table.offsetLeft+set_table.offsetWidth-205;
menu_line.style.top=set_table.offsetTop+set_table.offsetHeight-1;
menu_line.style.left=set_table.offsetLeft+set_table.offsetWidth-182;
menu_line.style.height=28*5;
$("body").on('mousedown', function(e) {
if(is_menu_open=="0")
{
document.getElementById("menu").style.display = "none";
menu_line.style.display="none";
}
});
$("#set_table").on('mousedown', function(e) {
e.stopPropagation();
if(menu.style.display=="none")
{
menu.style.display="";
menu_line.style.display="";
}
else
{
menu.style.display="none";
menu_line.style.display="none";
}
});
function menu_open(is_open)
{is_menu_open=is_open;}
var device_dashboard_on_off=localStorage.getItem("device_dashboard_on_off");
if(device_dashboard_on_off==null)
{localStorage.setItem("device_dashboard_on_off", "11111");}
device_dashboard_on_off=localStorage.getItem("device_dashboard_on_off");
for (i = 1; i <= device_dashboard_on_off.length; i++)
{
if(device_dashboard_on_off[i-1]=="0")
{
document.getElementById("menu_figure_"+i).src="unchecked.gif";
document.getElementById("table_"+i).style.display = "none";
}
$("#table_on_off_"+i).click({m_id: "menu_figure_"+i, t_id: "table_"+i, n: (i-1)}, table_on_off);
}
function table_dash_close(m_id,t_id,n)
{
var m = document.getElementById(m_id);
var t = document.getElementById(t_id);
var device_dashboard_on_off=localStorage.getItem("device_dashboard_on_off");
var num_one=0;
for (i = 0; i < device_dashboard_on_off.length; i++)
{
if(device_dashboard_on_off[i]=="1")
{num_one=num_one+1;}
}
if(num_one>1)
{
m.src="unchecked.gif";
t.style.display="none";
device_dashboard_on_off = device_dashboard_on_off.substring(0,n) + "0" + device_dashboard_on_off.substring(n+1);
localStorage.setItem("device_dashboard_on_off", device_dashboard_on_off);
set_zone_height();
}
}
function table_on_off(e)
{
e.stopPropagation();
var m = document.getElementById(e.data.m_id);
var t = document.getElementById(e.data.t_id);
var n= e.data.n;
var on_off;
var device_dashboard_on_off=localStorage.getItem("device_dashboard_on_off");
var num_one=0;
for (i = 0; i < device_dashboard_on_off.length; i++)
{
if(device_dashboard_on_off[i]=="1")
{num_one=num_one+1;}
}
if(t.style.display=="")
{
m.src="unchecked.gif";
t.style.display="none";
on_off="0";
}
else
{
m.src="checked.gif";
t.style.display="";
on_off="1";
}
if(num_one==1)
{
m.src="checked.gif";
t.style.display="";
on_off="1";
}
device_dashboard_on_off =device_dashboard_on_off.substring(0,n) + on_off + device_dashboard_on_off.substring(n+1);
localStorage.setItem("device_dashboard_on_off", device_dashboard_on_off);
set_zone_height();
}
/* =====tablei}MX===== */
var device_dashboard_toggle=localStorage.getItem("device_dashboard_toggle");
for (i = 1; i <= device_dashboard_toggle.length; i++)
{
if(device_dashboard_toggle[i-1]=="0")
{document.getElementById("toggle_table_"+i).style.display = "none";}
}
function table_dash_toggle(up_id,down_id,t_id,storage_name,n)
{
var up = document.getElementById(up_id);
var down = document.getElementById(down_id);
var toggle_on_off = localStorage.getItem(storage_name);
var on_off;
$(t_id).animate({height: 'toggle'});
if(down.style.display=="")
{
down.style.display="none";
up.style.display="";
on_off="1";
}
else
{
down.style.display="";
up.style.display="none";
on_off="0";
}
toggle_on_off = toggle_on_off.substring(0,n) + on_off + toggle_on_off.substring(n+1);
localStorage.setItem(storage_name, toggle_on_off);
setTimeout(set_zone_height,400);
}
/* =====dash_network.htm\===== */
function get_notify(xml)
{
var xmlDoc=xml;
if(xmlDoc!=null && xmlDoc.getElementsByTagName("notify_en")[0].childNodes[0]!=null){
rv = xmlDoc.getElementsByTagName("notify_en")[0].childNodes[0].nodeValue;
}
var notify=(xmlDoc!=null && xmlDoc.getElementsByTagName("notify_en")[0].childNodes[0]!=null)?parseInt(rv):0;
/*update cpu & mem page*/
if(notify != 0){
frames['dashboard_hid'].location = "dashboard_hid.htm?N8015043E=0&N80150430=1&N80150431=1&N80150432=1&csrftok="+get_csrf_token(); //be used in default
}
if(notify == 99){ //ALL page
change_cpu_type();     //refresh dashboard_cpu_flow.htm
change_memory_type();  //refresh dashboard_memory_flow.htm
}else if(notify == 1){
change_cpu_type();     //refresh dashboard_cpu_flow.htm
}else if(notify == 2){
change_memory_type();  //refresh dashboard_memory_flow.htm
}
}
function init()
{
trigger();
sw_port_link();           //liwei--show "I/O Port Link Status"
change_cpu_type();        //kent--send type to dashboard_cpu_flow.htm
change_memory_type();     //kent--send type to dashboard_memory_flow.htm
show_uptime(document.getElementById("uptime").innerHTML); //toshi
refresh_dashboard();
force_logout=1; //check wiqb01.js ps1.
}
function callajax(){
$.ajax({
url:"/DH_notify.xml",
type:"GET",
dataType:"xml",
error:get_notify,
success:get_notify
});
}
function refresh_dashboard()
{
var t=new Date();
/*update current page*/
var url="/current_traffic_sec_0.xml?rc="+t.getTime();
$.ajax({
url:url,
type:"GET",
dataType:"xml",
error:function(){
},
success:function(xml){
var xmlDoc_d = xml;
if(xmlDoc_d != null){
change_dashboard_info(xmlDoc_d); //toshi--show "Network Interface Status"
change_poe_info();
show_cpu_mem_ses(xmlDoc_d);      //toshi--show "System Information"
update_port(xmlDoc_d);           //liwei--show "I/O Port Link Status"
}
}
});
callajax();
setTimeout("refresh_dashboard()",5000);
}
function trigger(mod)
{
if(mod=='now'){
var xmlurl = "dashboard_hid.htm?N80150430=1&N80150431=1&N80150432=1&csrftok="+get_csrf_token(); //be used in default
$.ajax({
url:xmlurl,
type:'GET'
});
}else{
frames['dashboard_hid'].location = "dashboard_hid.htm?N80150430=1&N80150431=1&N80150432=1&csrftok="+get_csrf_token(); //be used in default
setTimeout("trigger()",30000);
}
}
