function setMenu()
{
for(i=0;i<menus.length;i++) //L2false,L2L3 linkremove_this..toshi
{
if (eval("typeof(sw"+menus[i]+")")=="undefined") unmenus=true;
else unmenus=(eval("sw"+menus[i])=='')||(eval("sw"+menus[i])=='0')||!(eval("sw"+menus[i]));
if(unmenus){
if((eval(menus[i]).length)>=3) {
for (j=2;j<eval(menus[i]).length;j++){
secm=eval(menus[i]+"["+ j +"]");
eval(secm+"=new Array('remove_this~','remove_this~')");}
}
eval(menus[i]+"=new Array('remove_this~','remove_this~')")
}
}
for(i=0;i<smenus.length;i++) //secrselect.htm,radio,radio..toshi
{
if(eval("typeof(sw"+smenus[i]+")")=="undefined") unsmenus=true;
else unsmenus=(eval("sw"+smenus[i])!='')||(eval("sw"+smenus[i])!='0')||(eval("sw"+smenus[i]));
if(unsmenus){
for (j=2;j<eval(smenus[i]).length;j++){
secm=eval(smenus[i]+"["+ j +"]");
if (eval("typeof("+secm+"[1])")=="undefined") unsecm=true;
else unsecm=eval(secm+"[1]")!=""&&eval(secm+"[1]")!="remove_this~";
if(unsecm) {eval(smenus[i]).splice(1,1,eval(secm+"[1]")); break; }
if(j==eval(smenus[i]).length-1&&unsecm){eval(smenus[i]+"=new Array('remove_this~','remove_this~')");}
}
}
}
if(typeof(swm_sta_vpn) != 'undefined'){ //vpn status add L3 : 379-OpenVPN Status
_m_status_vpn0 = m_sta_vpn[0]+m_sta_opvpn[0] ;
_m_status_vpn1 = m_sta_vpn[1]+m_sta_opvpn[1] ;
}else{
_m_status_vpn0 = m_status_vpn[0];    //vpn status add L3 : 379-OpenVPN Status
_m_status_vpn1 = m_status_vpn[1];    //vpn status add L3 : 379-OpenVPN Status
}
}
setMenu() //remove UI component link
function setTopMenus(ss)  //1menus
{
swtop=""
if (eval("typeof(sw"+ss+")")=="undefined") swtop="style='display:none'";
else if((eval("sw"+ss)=='')||(eval("sw"+ss)=='0')||!(eval("sw"+ss))){
swtop="style='display:none'"
}
return swtop
}
function swdesc(m)
{
for (i=0;i<m.length;i++){
if(document.getElementById(m[i])==null) continue;
if (eval("typeof(sw"+m[i]+")")=="undefined") unm=true;
else unm=(eval("sw"+m[i])=='')||(eval("sw"+m[i])=='0')||!(eval("sw"+m[i]));
document.getElementById(m[i]).style.display=(unm)?"none":"";
}
}
function swsta(m,n)
{
if (eval("typeof(sw"+m[i]+")")=="undefined") unm=true;
else unm=(eval("sw"+m)=='')||(eval("sw"+m)=='0')||!(eval("sw"+m));
document.getElementById(n).style.display=(unm)?"none":"";
}
