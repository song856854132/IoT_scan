function DASH_SPgTl(title,num,is_dialog)
{
var device_dashboard_toggle=localStorage.getItem("device_dashboard_toggle");
var title;
if(num==1)
{title=GetElmCont('atoolbox_0',title);}
else if(num==2)
{title=GetElmCont('dashboard_0',title);}
else if(num==3)
{title=GetElmCont('dashboard_0',title);}
else if(num==4)
{title=GetElmCont('dashboard_0',title);}
else if(num==5)
{title=GetElmCont('dashboard_0',title);}
if(device_dashboard_toggle==null)
{localStorage.setItem("device_dashboard_toggle", "11111");}
device_dashboard_toggle=localStorage.getItem("device_dashboard_toggle");
dw("<table width=100% height=27 border=0 cellpadding=0 cellspacing=0 class='L4title'>");
dw("<tr>");
dw("<td nowrap><img src=sp.gif width=10 height=27 align=absmiddle><img src=dot1.gif width=16 height=9 align=absmiddle>"+title+"</td>");
dw("<td align=right>");
dw("<ul id='icons' style='float:right'>");
dw("<li class='ui-state-default ui-corner-all' id='toggle_up_"+num+"'");
dw("onClick=\"table_dash_toggle('toggle_up_"+num+"','toggle_down_"+num+"','#toggle_table_"+num+"','device_dashboard_toggle',"+(num-1)+")\" ");
dw("onmouseover=\"title_figure_color('#toggle_up_"+num+"','1')\" ");
dw("onmouseout=\"title_figure_color('#toggle_up_"+num+"','0')\" ");
dw("onmousedown=\"drag_dash_table_cancel()\">");
dw("<span class='ui-icon ui-icon-triangle-1-n'></span>");
dw("</li>");
dw("<li class='ui-state-default ui-corner-all' id='toggle_down_"+num+"'");
dw("onClick=\"table_dash_toggle('toggle_up_"+num+"','toggle_down_"+num+"','#toggle_table_"+num+"','device_dashboard_toggle',"+(num-1)+")\" ");
dw("onmouseover=\"title_figure_color('#toggle_down_"+num+"','1')\" ");
dw("onmouseout=\"title_figure_color('#toggle_down_"+num+"','0')\" ");
dw("onmousedown=\"drag_dash_table_cancel()\">");
dw("<span class='ui-icon ui-icon-triangle-1-s'></span>");
dw("</li>");
if(device_dashboard_toggle[num-1]=="0")
{document.getElementById("toggle_up_"+num).style.display="none";}
else
{document.getElementById("toggle_down_"+num).style.display="none";}
if(is_dialog==1)
{
dw("<li class='ui-state-default ui-corner-all' id='dialog_figure_"+num+"'");
dw("onClick=\"open_dash_dialog()\" ");
dw("onmouseover=\"title_figure_color('#dialog_figure_"+num+"','1')\" ");
dw("onmouseout=\"title_figure_color('#dialog_figure_"+num+"','0')\" ");
dw("onmousedown=\"drag_dash_table_cancel()\">");
dw("<span class='ui-icon ui-icon-gear'></span>");
dw("</li>");
}
dw("<li class='ui-state-default ui-corner-all' id='delete_figure_"+num+"'");
dw("onClick=\"table_dash_close('menu_figure_"+num+"','table_"+num+"',"+(num-1)+")\" ");
dw("onmouseover=\"title_figure_color('#delete_figure_"+num+"','1')\" ");
dw("onmouseout=\"title_figure_color('#delete_figure_"+num+"','0')\" ");
dw("onmousedown=\"drag_dash_table_cancel()\">");
dw("<span class='ui-icon ui-icon-closethick'></span>");
dw("</li>");
dw("</ul>");
dw("</td>");
dw("</tr>");
dw("</table>");
}
/* ========== */
function open_dash_dialog()
{
$(function() {
$("#dialog").dialog({
autoOpen: false,
resizable:false,
draggable:true,
position:'center',  //iHOcenter,left,right,top,bottom,[1000,50]
width: 400,
height: 300
});
});
$("#dialog").dialog("open");
}
$("#dialog_ok").button();
$("#dialog_cancel").button();
$("#dialog_ok").click(function(){
$("#dialog").dialog( "close" );
});
$("#dialog_cancel").click(function(){
$("#dialog").dialog( "close" );
});
