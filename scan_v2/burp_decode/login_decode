}
}
function swid(div,sw)
{
swids.apply(this,arguments); // just call swids() instead of writing the same code
}
function findInArr(findstr,arr)
{
for (x in arr)
{
if(arr[x].indexOf(findstr)!=-1)return x
}
return 0
}
function findSelectIndex(selobj,searchval)
{
for(o=0;o<selobj.length;o++)
{
if(selobj.options[o].value==searchval){return o}
}
return false;
}
function links(link)
{
if((link.indexOf('.')==-1)&&(link.indexOf('=')==-1)&&(link.indexOf('javascript:')==-1))link="/"+link+".htm"
return link
}
