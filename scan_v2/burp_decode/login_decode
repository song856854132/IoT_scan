var all_langnames=[
 "English"
,"Espaol"
,""
,""
,""
]
var all_langmodes=[
 "us"
,"sp"
,"tc"
,"sc"
,"jp"
]

var langnames=[];
var langmodevs=[];
function langary(langlist)
{
  var langtab=langlist;
  for (i=0,len=all_langnames.length;i<len;i++){
    var show = (langtab >> i) & 1;    //1.i1
    if (langtab == "0"||show){  //2.
      langnames.push(all_langnames[i]);
      langmodevs.push((i+1));
    }
  }
}
var langfiles=new Array("setup","help")

function includejs(jsfn,searchpath)
{
   	var jst=new Date();ztv='rc='+jst.getTime()
    aa=(arguments.length<2)?"":(searchpath+"&")
	  srcv=jsfn+".js?"+aa+ztv
	  document.write("<script type='text/javascript' src='/"+srcv+"'></script>")
}
function loadjs(lm)
{
   lm = (lm == 0) ? 0:(lm-1); //5.
   if(all_langmodes[lm]!="us"){
//Englishjs must load..toshi
	   includejs("ssetup_us")
	   includejs("ssetup_us2")
       includejs("ssetup_us3")
	   includejs("shelp_us")
//other language
   	 includejs("ssetup_"+all_langmodes[lm])
   	 includejs("ssetup_"+all_langmodes[lm]+"2")
     includejs("ssetup_"+all_langmodes[lm]+"3")
   	 includejs("shelp_"+all_langmodes[lm])
     
     if(typeof(help_more)!="undefined")
     {includejs("help_more_100_"+all_langmodes[lm]);}
     if(typeof(help_less)!="undefined")
     {includejs("help_less_100_"+all_langmodes[lm]);}
   }
   else
   {
     if(typeof(help_more)!="undefined")
     {includejs("help_more_100_us");}
     if(typeof(help_less)!="undefined")
     {includejs("help_less_100_us");}
   } 
}
loadjs(_LM_)
function countchar(str,schar)
{
   cc=0;
   for(s=0;s<str.length;s++)
   {
      if(str.substr(s,1)==schar)cc++; //one word
   }
   return cc;
}

//chaglang2012/11/01UI..toshi
function chgLang(fr)
{
   func=lang
   if((typeof func) != "string"){return}
/*
,Array
""catch errorreturn, Array
HTML, , HTML loading
*/
   try {eval("langa="+func)}catch(e){return}
   qSetCont('span',fr)
   qSetCont('input',fr)
   qSetCont('option',fr)
}
function qSetCont(tags,fr)
{
   tagary=document.getElementsByTagName(tags);
   for(t=0;t<tagary.length;t++)
   {
     tid=tagary[t].id
//item id"_",skip
     if(tid.substring(0,1)!="_") continue;
//,"~xml",skip
     if(isNaN(tid.substring(tid.length-1,tid.length))&&tid.substring(tid.length-4,tid.length)!="~xml") continue;
//fr=1,status.htmrefresh rlist(),span id~xml, see HELP in "ui.js"
     if(fr==1 && tid.indexOf('~xml')==-1) continue;

//start....
     
	   if(countchar(tid,'_')==1){      //ex. "_1", or "_ind1" or "_1~xml"
       langa_eng=eval(lang+"_en")
       eval("langa="+lang)           //langa=Array
     }
	   else if(countchar(tid,'_')==2){ //ex. "_atbox_1" or "_atbox_ind1" or "_atbox_1~xml"
	   	 tids=tid.split("_")
	   	 lang2=tids[1] //"_atbox_1"  "atbox"
	     try {eval("langa_eng="+lang2+"_en")}catch(e){continue}
       eval("langa="+lang2)          //langa=atbox Array
	   }
	   else continue;
//===================== compare by string =====================
     if(tid.indexOf("ind")==-1){
       for(l=0;l<langa_eng.length;l++){ //atbox_en Arraysearch this string
         if(tags=="span"){ //for <span>
         	 if(tagary[t].innerHTML.indexOf("&")!=-1){
         	 	 /*innerHTML&&amp;, ex. a & b -> a &amp; b
         	 	   a & bArraycompare
         	 	   innerHTMLtextContentsolve "&" issue, but IE8 not support textContent*/
         	   if(tagary[t].innerHTML.replace(/&amp;/g, '&')==langa_eng[l]){
         	 	   tagary[t].innerHTML=langa[l]
       	   	   break;
       	     }
         	 }
         	 else{ //case
         	   if(tagary[t].innerHTML==langa_eng[l]){
         	 	   tagary[t].innerHTML=langa[l]
       	   	   break;
       	     }
       	   }
         }
         if(tags=="input"){ //for button
           if((tagary[t].type=="button")||(tagary[t].type=="submit")||(tagary[t].type=="reset")) //button
           {
             if(tagary[t].value==langa_eng[l]){ 
           	   tagary[t].value=langa[l]
           	   break;
             }
           }
         }
         if(tags=="option"){ //for selectbox
         	 //case, for Group 1, Group 2, Group 3, Group 4......in vpndynip.htm
         	 if(tagary[t].text.substring(0,5)=="Group" && langa_eng[l]=="Group"){
         	 	 gnum=tagary[t].text.substring(6,tagary[t].text.length)
       	 	   tagary[t].text=langa[l]+gnum
       	 	   break;
         	 }
         	 //case
       	   if(tagary[t].text==langa_eng[l]){ 
       	 	   tagary[t].text=langa[l]
       	 	   break;
       	   }
         }
       }
     }
//===================== compare by index =====================
     else{
     	 if(fr==1){
     	 	 tido=tid.substring(tid.indexOf("ind")+3,tid.length-4) //_ind9~xml => 9 or _wary_ind9~xml => 9
     	 }
     	 else{
         tido=tid.substring(tid.indexOf("ind")+3,tid.length) //_ind9 => 9 or _wary_ind9 => 9
       }
       if(tido>=langa.length) continue; //the index is over arrary`s length
       
       if(tags=="span"){	 
         tagary[t].innerHTML=langa[tido]
       }
       if(tags=="input"){
         if((tagary[t].type=="button")||(tagary[t].type=="submit")||(tagary[t].type=="reset"))
         {
       	   tagary[t].value=langa[tido]
         }
       }
       if(tags=="option"){
   	 	   tagary[t].text=langa[tido]
       }
     }
   }
}

function SetElmCont(idx,cont)
{
   ddiv=document.getElementById(idx);
   if(cont=="ddiv.innerHTML")return false;
   if(ddiv!=null)
   {
      if((ddiv.type=="button")||(ddiv.type=="submit")||(ddiv.type=="reset"))
      {
          ddiv.value=cont;
      }
      else if(ddiv.text!=null) ddiv.text=cont;
      else if(ddiv.value!=null) ddiv.value=cont;
      else ddiv.innerHTML=cont;
      return true;
   }
   else return false;
}
function GetElmCont(idx,dcont)
{
/*
1.GetElmCont()id"atbox_1" or "atbox_ind1"
  ind,0,
  "_"
2."_1" or "_ind1"id
3.id,
  <span id>
  arguments.length=1
*/
   retv=dcont;
   if(arguments.length<2) return document.getElementById(idx).innerHTML;
   if(isNaN(idx.substring(idx.length-1,idx.length))) return retv;
   if(idx.indexOf("_")!=-1)
   {
     idxs=idx.split("_")          //idxs[0]="atbox"
     if(idxs[0]=="") idxs[0]=lang //ex. _1 or "_ind1"
	   try {eval("langa_eng="+idxs[0]+"_en")}catch(e){return retv}
     //langa=atbox Array,languageEnglishArrayreturn
	   try {eval("langa="+idxs[0])}catch(e){return retv}

//===================== compare by string =====================
	   if(idx.indexOf("ind")==-1){
       for(l=0;l<langa_eng.length;l++){ //atbox_en Arraysearch this string
     	   if(dcont==langa_eng[l]){ 
           retv=langa[l]
           break;
     	   }
     	 }
     }
//===================== compare by index (old language way) =====================
     else{
       idxo=idx.substring(idx.indexOf("ind")+3,idx.length)
       if(idxo>=langa.length) return retv;
       retv=langa[idxo]
     }
   }
   else retv=dcont

   return retv;
}

//htmlHelp
function chgHelp()
{
    dw("<div id='help_content_page' style='display:none;' onmouseover='help_page_enable()' onmouseout='help_page_cancel()'>");    
    dw("<div id='help_content_up_figure'>");dw("</div>");
    dw("<div id='help_content_word'>");dw("</div>");
    dw("<div id='help_content_down_figure'>");dw("</div>");
    dw("</div>");
    
    if(typeof(help_more)!="undefined" && typeof(help_less)!="undefined")
    {    
        help_content_data_more=eval(help_more);
        help_content_data_less=eval(help_less);
        $('span').each(function(index){
            help_name=$(this).html();
            help_class_name=$(this).attr('class');
        
            lang_type=(_LM_ == 0) ? 0:(_LM_-1);
            if(all_langmodes[lang_type]!="us")
            {   
                help_name_en=help_name;
                span_id=$(this).attr('id');
           
                if(span_id!=undefined)
                {    
                    if(!(span_id.substring(0,1)!="_") && !(isNaN(span_id.substring(span_id.length-1,span_id.length))))
                    {
                        if(countchar(span_id,'_')==1)
                        {     
                            lang_word_en=eval(lang+"_en");
                            eval("lang_word="+lang);
                        }
                        else if(countchar(span_id,'_')==2)
                        {
                            span_ids=span_id.split("_");                       
                            try
                            {eval("lang_word_en="+span_ids[1]+"_en");}
                            catch(e)
                            {}
                            eval("lang_word="+span_ids[1]);         
                        }
                    
                        en_index=-1;
                        for(i=0;i<lang_word.length;i++)
                        {
                            if(help_name==lang_word[i])
                            {
                                en_index=i;
                                break;
                            }                    
                        }
                    
                        if(en_index!=-1)
                        {help_name_en=lang_word_en[en_index];}                   
                    }
                }
            }
            else            
            {help_name_en=help_name;}          
    
            if(help_class_name!=undefined)
            {
                if(help_content_data_more[help_name_en]!=undefined)
                {
                    if(help_content_data_more[help_name_en][help_class_name]!=undefined)
                    {
                        help_string_more=help_content_data_more[help_name_en][help_class_name];                  
                        help_string_less=help_content_data_less[help_name_en][help_class_name];
                        if(switch_help_page)
                        {    
                            $(this).mouseover({s_more: help_string_more,s_less: help_string_less,s_name: $(this).html(),n: index}, open_help);
                            $(this).mouseout({s_more: help_string_more,s_less: help_string_less,s_name: $(this).html(),n: index}, close_help);
                            
                            browser=navigator.userAgent;
                            if(browser.indexOf("Trident")>0 || browser.indexOf("MSIE")>0)  //IE
                            {$(this).addClass("help_icon_ie");}
                            else if(browser.indexOf("Edge")>0)  //Edge
                            {$(this).addClass("help_icon_ie");}    
                            else
                            {$(this).addClass("help_icon");}
                        
                            span_element=document.getElementsByTagName('span');
                            span_element[index].innerHTML=help_name;
                        }
                    }
                }
            }
        });
    }
}
