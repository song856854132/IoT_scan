var open_win;
function openwin(url,sx,sy)
{
var width=sx;
var height=sy;
open_win=window.open('','_new','width='+width+',height='+height+',resizable=yes,scrollbars=yes,status=yes,menubar=no,toolbar=no');
open_win.focus();
open_win.location.href = url;
open_win.opener = window;
}
