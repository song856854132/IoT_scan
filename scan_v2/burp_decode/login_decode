function swWLAN(sw)
{
f2=document.forms[0]
s=false;
for(r=0;r<f2.length;r++)
{
f2elm=f2.elements[r].name
if((f2elm=="_eny_start")||(f2elm=="_eny_start2")){s=true;}
else if((f2elm=="_eny_end")||(f2elm=="_eny_end2")){s=false;}
if(s) f2.elements[r].disabled=!sw
else continue;
}
}
function DisableEnableForm(xForm,xHow)
{
for(i=0;i<xForm.length;i++)
{
if(xForm[i].type.toLowerCase()=="button")
{
xForm[i].disabled = xHow;
}
}
}
/*
compare_repeatvalue:
:value
:vid,vlan_tagbase
Designer:Avril
*/
function compare_repeatvalue() {
flag = 0;
if(arguments.length == 3 && arguments[2] == 1)
{
var temp = []; arr1 = arguments[0] ; arr2 = arguments[1] ;
for (var i = 0; i < arr2.length; i++) {
if(arr2[i]>0) temp[arr2[i]] = true;
};
for (var i = 0; i < arr1.length; i++) {
if (temp[arr1[i]]==true) { flag=1; break; } ;
};
}
else if (arguments.length == 2 && arguments[1] == 2)
{
arr1 = arguments[0] ;
for(h = 0 ; h < arr1.length; h++)
{
for (n = 0 ; n < arr1.length; n++)
{
if( h != n && arr1[h] == arr1[n] && arr1[h]>0 ) { flag = 1; break; }
}
}
}
return flag;
}
/*
Combinevalue:
:
objv:
idlist:id,"-"
ex:Combinevalue(100-200,aa-bb) =>aa=100 ; bb=200
:qos_rulebasededit
Designer:Avril
*/
function Combinevalue(objv,idlist) {
var equalsign = objv.match(/\W/);
var idlist = idlist.split("-");
if(equalsign != -1)
{
arr = objv.split(equalsign);
for (var i = 0 ; i < arr.length ; i++)
{
document.getElementById(idlist[i]).value = arr[i];
}
}
else
{
document.getElementById(idlist).value = objv;
}
}
/*
getValues:
:
v:
l:
ex:getValues(192.168.123.100,1) =>100
Designer:Avril
*/
function getValues(v,l){
if(l==0) //ipmask
vv=v.substring(0,v.lastIndexOf(".")+1);
else if (l==1)
vv=v.substring(v.lastIndexOf(".")+1,v.length);
else if (l==2)
vv=v.substring(v.lastIndexOf("/")+1,v.lastIndexOf(")")-1);
return vv;
}
/*
getmaskrange:
::DHCP_MASK "/"VALUE
v:MASK VALUE
ex:getmaskrange(255.255.255.0 (/30)) =>/30
:qos_balance,qos_rulebased,qos_rulebasededit
Designer:Avril
*/
DHCP_MASK=new Array(
"255.0.0.0 (/8)",
"255.128.0.0 (/9)",
"255.192.0.0 (/10)",
"255.224.0.0 (/11)",
"255.240.0.0 (/12)",
"255.248.0.0 (/13)",
"255.252.0.0 (/14)",
"255.254.0.0 (/15)",
"255.255.0.0 (/16)",
"255.255.128.0 (/17)",
"255.255.192.0 (/18)",
"255.255.224.0 (/19)",
"255.255.240.0 (/20)",
"255.255.248.0 (/21)",
"255.255.252.0 (/22)",
"255.255.254.0 (/23)",
"255.255.255.0 (/24)",
"255.255.255.128 (/25)",
"255.255.255.192 (/26)",
"255.255.255.224 (/27)",
"255.255.255.240 (/28)",
"255.255.255.248 (/29)",
"255.255.255.252 (/30)",
"255.255.255.254 (/31)",
"255.255.255.255 (/32)"
)
function getmaskrange(v)
{
for(m = 0 ; m < DHCP_MASK.length ; m++)
{
if(DHCP_MASK[m].match(v)!=null) break;
}
vv = DHCP_MASK[m].match(/\/[0-9]+/);
return vv;
}
function show_onoff(idd,v){
if(document.getElementById("tr_"+idd)) document.getElementById("tr_"+idd).style.display="" //dont remove this
xx=document.getElementById(idd)
for(a=1;a<xx.rows.length;a++){
xx.rows[a].style.display=(v==1)?'':'none'
}
document.getElementById(idd+"_add").style.display=(v==1)?'none':''
document.getElementById(idd+"_subtract").style.display=(v==1)?'':'none'
}
function add_empty_tr(idd){
xx=document.getElementById(idd)
if(xx.rows.length==1){ //only have <tr><th>.....
emp=xx.insertRow(1).insertCell(0)
emp.colSpan=10
emp.innerHTML=GetElmCont('_0','Empty')
emp.id="tr_"+idd
emp.style.display="none"
}
}
function get_csid_with_offset(csid_base, offset) {
if(typeof(offset)=="string") offset = parseInt(offset, 10);
var csid_type = csid_base.charAt(0); // first character is CSID type
var csid_addr = parseInt(csid_base.substring(1), 16) + offset; // parse the CSID address (hex string) to integer and then plus offset
var csid_addr_str = csid_addr.toString(16); // change from integer to hex string
var pad = "00000000";
var csid_addr_str = pad.substring(0, pad.length - csid_addr_str.length) + csid_addr_str; // padding head zeros to let address string length be 8
return csid_type + csid_addr_str;
}
function busy_loading(div_id) {
var totalTop=0;
var totalHeight=0;
var jobj = $('#'+div_id);
var isIE = navigator.userAgent.search("MSIE") > -1;
if(isIE){// check using IE browser or not
totalHeight = jobj.height()+15;// the height div in IE8 browser need more 15px
totalTop = getScrollTop()+jobj.offset().top-15;// the starting side of div in IE8 browser need to pull up 15px
} else {// using other browsers except IE
totalHeight = jobj.height();
totalTop = jobj.offset().top;
}
$.fancybox.showLoading(); // show loading icon
$.fancybox.helpers.overlay.open({parent: jobj, closeClick : false, css : {
'position': 'absolute',
'height': totalHeight+20,
'width': jobj.width(),
'top': totalTop,
'left': jobj.offset().left,
'overflow': 'hidden'
}}); // show overlay to avoid user clicking in div
var loading_left = parseInt($("#"+div_id).css("width"))/2+jobj.offset().left;
$("#fancybox-loading").css({
'position': 'absolute',
'top': totalHeight/2+totalTop,
'left': loading_left,
});
$(window).bind("resize", function() { // bind resize event to adjust the size of overlay
close_busy_loading();
busy_loading(div_id);
});
}
function close_busy_loading() {
$.fancybox.hideLoading(); // hide loading icon
$.fancybox.helpers.overlay.close(); // hide overlay
$(window).unbind("resize"); // unbind resize event
}
function getScrollTop() {// to get the scroll value in IE8
var scrollPos;
if (window.pageYOffset) {
scrollPos = window.pageYOffset;
}
else if (document.compatMode && document.compatMode != 'BackCompat') {
scrollPos = document.documentElement.scrollTop;
}
else if (document.body) {
scrollPos = document.body.scrollTop;
}
return scrollPos;
}
function ckeckDivHeight(div){// check div height exceeding 530px or not before using busy_loading
totalHeight = document.getElementById(div).offsetHeight;
if(totalHeight<530){
document.getElementById(div).style.height=530;
}
}
