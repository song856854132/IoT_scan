/* =====Help Start===== */
var is_show_help_page=0;
var help_name_string="";
var help_more_string="";
var help_less_string="";
function open_help(e)
{
is_show_help_page=1;
help_name_string=e.data.s_name;
help_more_string=e.data.s_more;
help_less_string=e.data.s_less;
setTimeout(function(){
open_help_page(e.data.n)
},2000);
}
function close_help(e)
{
is_show_help_page=0;
setTimeout("close_help_page()", 200);
}
function open_help_page(n)
{
help_content_page=document.getElementById("help_content_page");
help_content_word=document.getElementById("help_content_word");
$('span').each(function(index){
if(n==index)
{
help_top=$(this).offset().top;
help_left=$(this).offset().left;
word_width=$(this).width();
word_height=$(this).height();
}
});
help_content_page.style.display="";
help_content_page.style.top=help_top-word_height+5;
help_content_page.style.left=help_left+word_width;
if(help_more_string==help_less_string)
{help_data=help_less_string;}
else
{
help_data=help_less_string;
help_data=help_data+"<br/><br/><span style='color:blue' onclick=\"show_all_help_content()\">...more</span>";
}
help_content_word.innerHTML=help_data;
if(is_show_help_page==0)
{help_content_page.style.display="none";}
}
function close_help_page()
{
if(is_show_help_page==0)
{document.getElementById("help_content_page").style.display="none";}
}
function show_all_help_content()
{
help_content_up_figure=document.getElementById("help_content_up_figure");
help_content_word=document.getElementById("help_content_word");
help_content_down_figure=document.getElementById("help_content_down_figure");
help_data=help_more_string;
help_data=help_data+"<br/><br/><span style='color:blue' onclick=\"show_part_help_content()\">...less</span>";
help_content_up_figure.style.width="250px";
help_content_word.style.width="220px";
help_content_word.style.padding = "0px 10px 0px 20px";
help_content_word.innerHTML=help_data;
help_content_down_figure.style.width="250px";
}
function show_part_help_content()
{
help_content_up_figure=document.getElementById("help_content_up_figure");
help_content_word=document.getElementById("help_content_word");
help_content_down_figure=document.getElementById("help_content_down_figure");
help_data=help_less_string;
help_data=help_data+"<br/><br/><span style='color:blue' onclick=\"show_all_help_content()\">...more</span>";
help_content_up_figure.style.width="200px";
help_content_word.style.width="180px";
help_content_word.style.padding = "0px 7px 0px 13px";
help_content_word.innerHTML=help_data;
help_content_down_figure.style.width="200px";
}
function help_page_enable()
{
is_show_help_page=1;
}
function help_page_cancel()
{
is_show_help_page=0;
setTimeout("close_help_page()", 200);
}
/* =====Help End===== */
/* =====pop up Start===== */
function add_iframe_border_popup(div_id,iframe_id,w)
{
if(w==1)
{setTimeout("add_iframe_border_popup('"+div_id+"','"+iframe_id+"',0)", 300);}
else
{
div_object=document.getElementById(div_id);
iframe_object=parent.document.getElementById(iframe_id);
iframe_object.style.height=div_object.offsetHeight;
}
}
function iframe_scroll_top(iframe_id,div_iframe_id,t)
{
h=$('#LT1').height()-$('.L3menu').height()-100;
$("#"+iframe_id).contents().scrollTop(t);
document.getElementById(iframe_id).style.height= h;
document.getElementById(div_iframe_id).style.height= h;
}
/* =====pop up End===== */
