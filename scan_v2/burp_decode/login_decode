var showL1Help = false; // judge whether to show L1 help page when user clicks L1 button
function downloadFile(file_path, file_name) {
var link = document.createElement('a');
link.href = file_path;
link.download = file_name;
document.body.appendChild(link);
link.click();
setTimeout( function () { link.parentNode.removeChild( link ); }, 10 ); // remove garbage element
}
function changeClass(id0, class0){
if(document.getElementById(id0)!=null)
{document.getElementById(id0).className = class0;}
}
function tempchglang(lgsel)
{
fm=lgsel.form
if(location.pathname=="/primx.htm"||location.pathname=="/primx_sim.htm"||location.pathname=="/vprimx.htm")
loc=String(location+"&N0002000F="+lgsel.value+"&_sck=httpd&Nlang="+lgsel.value)
else
loc=String(location.pathname+"?N0002000F="+lgsel.value+"&_sck=httpd&Nlang="+lgsel.value)
GURL(loc);
}
