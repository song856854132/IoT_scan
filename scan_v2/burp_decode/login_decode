change_time = chg_pre_time(a-1,time.eq(a).text(),updateInterval_arr[Ntype]);
}
var memory_used_value = memory_used.eq(a).text();
memory_used_value = float_change(memory_used_value,1000);
data0.push([time.eq(a).text(),memory_used_value]);
}
options["xaxis"].min = get_xaxis_min();
options["xaxis"].max = get_xaxis_max();
}
redraw();
}})
}
