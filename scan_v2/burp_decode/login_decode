for(p=0;p<arguments.length/2;p++)
{
ddiv=document.getElementById(arguments[p*2])
if(ddiv!=null)ddiv.disabled=!(arguments[p*2+1]);
}
}
function showsPg(pgs)
{
pgss=pgs.split("#");
pg=pgss[0].split("|"); //diff. with showsPgX
pgl3=pgss[1].split("~");
wid="100%";
dw("<table class='L3menu' border=0 width="+wid+"><tr><td><TABLE BORDER=0 CELLPADDING=0 CELLSPACING=0><TR>");
is_mobile_login=false;
login_machine_content = navigator.userAgent.toLowerCase();
if(login_machine_content.indexOf('android')!=-1)
{is_mobile_login=true;}
else if(login_machine_content.indexOf('iphone')!=-1)
{is_mobile_login=true;}
else if(login_machine_content.indexOf('ipad')!=-1)
{is_mobile_login=true;}
if(!is_mobile_login)
{dw("<td height=30><span class='L3_title_margin'></span></td>");}
else
{
dw("<td><span class='L3_title_margin'></span><span style='font-size:20px;cursor:pointer' onClick='mobile_open_left_menu()'>&#9776;</span>&nbsp;&nbsp;&nbsp</td>");
document.getElementById("panel_img").style.display="none";
document.getElementById("mobile_menu_data").style.display="none";
}
for(i=0;i<pg.length;i++)
{
showl3=(pg[i].indexOf("remove_this")!=-1)? "style='display:none'" : "";
pga=pg[i].split("~");
L3ID=pgl3[i]; //L3 TAID
lk = append_url( links(pga[1]) ); // append CSRF token at the tail of url
if(pga[2]=="")  //selected item.  //diff. with showsPgX
{
dw("<TD id=TA_"+L3ID+" NOWRAP class='LM3_mouse' onClick=\"L3_menu_link('"+lk+"','"+L3ID+"')\"><img src=parr.gif height=15 class='show_parr'><B><A href='#'>"+pga[0]+"</A></B></TD>");
}
else
{
dw("<TD id=TA_"+L3ID+" "+ showl3 +" NOWRAP class='LM3' onMouseOver=\"changeClass('TA_"+L3ID+"','LM3_mouse');L3_float('div_"+L3ID+"','over')\" onMouseOut=\"changeClass('TA_"+L3ID+"','LM3');L3_float('div_"+L3ID+"')\" onClick=\"L3_menu_link('"+lk+"','"+L3ID+"')\"><div id='div_"+L3ID+"' class='hvr-float hvr-icon-buzz-out'><img src=parr.gif height=15 class='show_parr hvr-icon'><B><A href='#'>"+pga[0]+"</A></B></div></TD>");
}
}
dw("<input type=button id='set_table' value='"+GetElmCont('pmac_0','Widget')+"' style='float:right;display:none;'>");
dw("</TR></TABLE></TD></TR></TABLE>");
dw("<div class='LM3_hr'></div>");
}
function L3_float(id,mod){
document.getElementById(id).style.transform = (mod=='over')?"translateY(-8px)":"";
}
function progress_time()
{
/*
ps1.
statusread xml
code logout
set force_logout=1, UIforce logout
set force_logout=2, UIforce logoutiframe
*/
if(force_logout==1) GURL("/log/out?rd=/index.htm"); //check ps1.
if(force_logout==2) {
parent.location.href = append_url("/log/out?rd=/index.htm");
}
xhr = createXHR()
if (xhr) {
xhr.onreadystatechange = progress_time_update
xhr.open("GET", "timeout.xml", true)
xhr.send(null)
}
else {
alert("xhr fail")
}
}
function progress_time_update() {
if (xhr.readyState == 4) {
if (xhr.status == 200) {
var xmldoc = xhr.responseXML
db = xmldoc.getElementsByTagName("login")[0].childNodes[0].nodeValue
if(db==7) setTimeout('progress_time()',add_tout*1000); //not time out
else top.location="/index.htm" //time out
}
}
}
var timeout_timer;
var force_logout=0;
add_tout=parseInt("300")+2
timeout_timer=setTimeout('progress_time()',add_tout*1000);
function showdbm(){
if(document.getElementById("_sgn1")){ //only in status.htm
sgnv=document.getElementById("_sgn1").innerHTML
dbmv=document.getElementById("_dbm1").innerHTML
if(sgnv>0){
dbmv=dbmv*2-113
document.getElementById("s_3gmodem_signal1").innerHTML=sgnv+"% "+"("+dbmv+"dBm)"
}
else{
document.getElementById("s_3gmodem_signal1").innerHTML="N/A"
}
}
if(document.getElementById("_sgn2")){ //only in status.htm
sgnv=document.getElementById("_sgn2").innerHTML
dbmv=document.getElementById("_dbm2").innerHTML
if(sgnv>0){
dbmv=dbmv*2-113
document.getElementById("s_3gmodem_signal2").innerHTML=sgnv+"% "+"("+dbmv+"dBm)"
}
else{
document.getElementById("s_3gmodem_signal2").innerHTML="N/A"
}
}
}
