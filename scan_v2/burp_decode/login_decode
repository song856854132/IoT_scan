function add_stasL3(str0,ary0) {// for status add L3
var L0= ary0.length;
var emls =0;
for(var z=0 ; z<L0 ; z++){
if(ary0[z] == str0 ){ return z }
}
}
if(typeof(swm_sta_vpn) != 'undefined'){  //vpn status add L3 : 379-OpenVPN Status
menus.splice(add_stasL3("m_status_vpn",menus), 1, "m_status_vpn","m_sta_vpn","m_sta_opvpn");
}
