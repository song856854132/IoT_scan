<script src="jquery.min.js"></script><script src="excanvas.min.js"></script><script src="jquery.flot.min.js"></script><script type="text/javascript" src="jquery.flot.time.min.js"></script><script type="text/javascript">
var lang="dashboard";
var Ntype = parseInt("0");
var data = [];
var data0 = [],data1 = [],data2 = [],data3 = [];
var updateInterval_arr = [1000,60000,3600000];//1 sec,1 min ,1 hour
var updateInterval = updateInterval_arr[Ntype];
var total = 30;
var number = ("0" == "0")?4:0;
var now = new Date().getTime();
var time_range_arr = [10,5,3];
var xmlfile = ["cpu_usage_sec_0.xml","cpu_usage_min_0.xml","cpu_usage_hour_0.xml"];
var xmlname_arr = ["cpu_usage0","cpu_usage1","cpu_usage2","cpu_usage3"];
var xtick_obj = {
0:[1,"second"],
1:[1,"minute"],
2:[1,"hour"]
};
var xtick_arr = xtick_obj[Ntype];
function float_change(num)
{
var num = parseInt(num);
return Math.round(num);
}
function chg_pre_time(idx,x,immediate_time,range_time){
var num = 1;
for(var a=idx;a>=0;a--){
eval("data"+x+"["+(a)+"][0] = ["+(parseInt(immediate_time)-range_time*num)+"]");
num++;
}
return false;
}
function getdata()
{
$.ajax({
url: xmlfile[Ntype],
type: 'GET',
dataType: 'xml',//texthtml
cache:false,
async:false,
error: function(xml){
},
success: function(xml){
var change_time = false;
var time = $(xml).find("time");
if(time.length>0){
for(var a=0;a<time.length;a++)
{
if(a>0){
var diff_time = (time.eq(a).text() - time.eq(a-1).text())/(updateInterval_arr[Ntype]);//get range time,ex:1s
if(diff_time!=1){
parent.trigger('now');
}
}
for(var x=0;x<number;x++)
{
if(diff_time!=1 && a>0){
change_time = true;
}
if(change_time){
change_time = chg_pre_time(a-1,x,time.eq(a).text(),updateInterval_arr[Ntype]);
}
var cpu = $(xml).find(xmlname_arr[x]);
var cpu_value = cpu.eq(a).text();
cpu_value = float_change(cpu_value.replace("%",""));
eval("data"+x+"["+(a)+"] = ["+time.eq(a).text()+" , "+cpu_value+"]")
}
}
options["xaxis"].min = get_xaxis_min();
options["xaxis"].max = get_xaxis_max();
}
redraw();
}})
}
var options = {
series: {
lines: {
show: true,
lineWidth: 1.2
}
},
xaxis: {
mode: "time",
tickSize: xtick_arr,
tickFormatter: function (v, axis) {
var date = new Date(v);
var time_value = 0;
if(Ntype == 0)
{
time_value = date.getSeconds();/*0~59*/
}
else if (Ntype == 1)
{
time_value = date.getMinutes();/*0~59*/
}
else
{
time_value = date.getHours();/*0~23*/
}
if (time_value % time_range_arr[Ntype] == 0) {
var hours = date.getHours() < 10 ? "0" + date.getHours() : date.getHours();
var minutes = date.getMinutes() < 10 ? "0" + date.getMinutes() : date.getMinutes();
var seconds = date.getSeconds() < 10 ? "0" + date.getSeconds() : date.getSeconds();
var time_format;
switch(Ntype)
{
case 0:
time_format = hours + ":" + minutes + ":" + seconds;
break;
case 1:
time_format = hours + ":" + minutes;
break;
case 2:
time_format = hours;
break;
}
return time_format;
}
else
{
return "";
}
}
},
yaxis: {
min: 0,
max: 100,
tickSize: 10,
tickFormatter: function (v, axis) {
return float_change(v) + "%";
}
}
};
function get_xaxis_min(){
return data0[0][0];
}
function get_xaxis_max(){
return data0[data0.length-1][0];
}
function redraw()
{
data = [];
$.ajax({
url: xmlfile[0],
type: 'GET',
dataType: 'xml',//texthtml
cache:false,
async:false,
error: function(xml){
},
success: function(xml){
var cpu_xml_0;
var cpu_xml_1;
var cpu_xml_2;
var cpu_xml_3;
cpu_xml_0 = $(xml).find("cpu_usage0").eq(0).text();
cpu_xml_1 = $(xml).find("cpu_usage1").eq(0).text();
cpu_xml_2 = $(xml).find("cpu_usage2").eq(0).text();
cpu_xml_3 = $(xml).find("cpu_usage3").eq(0).text();
if(cpu_xml_0!="")
{data.push({ label: "CPU 1", data: eval("data0")});}
if(cpu_xml_1!="")
{data.push({ label: "CPU 2", data: eval("data1")});}
if(cpu_xml_2!="")
{data.push({ label: "CPU 3", data: eval("data2")});}
if(cpu_xml_3!="")
{data.push({ label: "CPU 4", data: eval("data3")});}
}});
$.plot("#flotcontainer", data, options);
}
