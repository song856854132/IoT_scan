function list_arr(){
for (i=0;i<Nwans;i++){
if(ALL_INDEXV[i]!="0"){
var idx=(ALL_INDEXV[i]=="99")?0:ALL_INDEXV[i];
var add_str = "";
wiface=ALL_INDEXV[i];
sw_phy=swName_idx[wiface];
add_str += sw_phy +"|";
var opmode = (FAILOVER_ENABLEV[idx]==ALL_INDEXV[i])?1:0;
if(opmode == 0) {use_always_on_num=use_always_on_num+1;} //palways on
add_str += opmode +"|";
if(i==0) { wanopmode += opmode }
else { wanopmode += "-"+opmode }
add_str += FAILOVER_ENABLEV[idx] +"|";
wtype=ALL_WANTYPEV[idx];
if(wtype=="0xFFFFFFFF")  wtype="0x00000000";
sw_wtype=wantype[wtype];
add_str += sw_wtype +"|";
add_str += VLAN_TAGV[idx] +"|";
add_str += WAN_PVIDV[idx];
LIST_SET.push(add_str);
}else{
LIST_SET.push(" - |2|0| - |0|0");
if(i==0) wanopmode += "2";
else wanopmode += "-2"
}
}
LIST_SET.push("");
}
