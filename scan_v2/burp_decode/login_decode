function L3_button(p,s)
{
var L3_button_str="";
var link_web;
var L3_name=ThreeMenus[p].split("|");
var L3_name_show;
var L3_link=ThreeMenulk[p].split("|");
var L3_link_show;
var L3_id=TAID_L3[p].split("|");
var L3_id_show;
var str="";
str=L3_name[s];
str=str.substring(0, str.length-1);
L3_name_show=str.split("~");
str=L3_link[s];
str=str.substring(0, str.length-1);
L3_link_show=str.split("~");
str=L3_id[s];
L3_id_show=str.split("~");
if(window.word_max_length){ //for ui word_max_length in func.js
}else{
word_max_length=25;
if(_LM_ ==3 || _LM_ ==4 || _LM_ ==5)   //,,
{word_max_length=10;}
}
L3_button_str = "<tr class='handlk'><td>";
L3_button_str = L3_button_str + "<div id='L3_e_"+p+"_"+s+"' style='display:none;'>";
for(i1=0;i1<L3_name_show.length;i1++)
{
if(L3_name_show[i1].indexOf("remove_this")==-1)
{
id="MENU_"+L3_id_show[i1];
link_web = append_url(links(L3_link_show[i1]));
L3_button_str = L3_button_str + "<div id='"+id+"' style='height:25px;' class='LM2'";
if(id!=localStorage.getItem("L3_menu_id"))
{
if(L3_name_show[i1].length>word_max_length)
{L3_button_str = L3_button_str + "onMouseOver=\"changeClass('"+id+"','LM2_mouse'); L3_add_all_title('"+id+"','"+L3_name_show[i1]+"');\" ";}
else
{L3_button_str = L3_button_str + "onMouseOver=\"changeClass('"+id+"','LM2_mouse');\" ";}
L3_button_str = L3_button_str + "onMouseOut=\"changeClass('"+id+"','LM2');\" ";
}
L3_button_str = L3_button_str + "onClick=\"save_L3_menu_id('"+id+"','"+link_web+"')\">";
if(L3_name_show[i1].length>word_max_length)
{L3_name_show[i1]=L3_name_show[i1].substring(0, word_max_length)+"...";}
L3_button_str = L3_button_str + "<span class='L3_list_margin'></span>"+L3_name_show[i1]+"</div>";
}
}
L3_button_str = L3_button_str + "</div>";
L3_button_str = L3_button_str + "</td></tr>";
return L3_button_str;
}
function L3_add_all_title(L3_id, L3_name)
{
$("#"+L3_id).addClass("hint--right");
$("#"+L3_id).attr('data-hint', L3_name);
}
function displaymenu(m,click_type,L1_td_id)
{
if(!login) return;
if(m==null)
{m="1";}
var id='L2_e'+m;
if(L2_click_menu=="")
{L2_click_menu=id;}
else if(L2_click_menu!=id)
{
var L2_display_state=document.getElementById(L2_click_menu).style.display;
if(L2_display_state!="none")
{$("#"+L2_click_menu).animate({height: 'toggle'});}
L2_click_menu=id;
}
localStorage.setItem("L2_menu", m);
if(localStorage.getItem("L2_menu_id")!=null)
{changeClass(localStorage.getItem("L2_menu_id"),'LM2_mouse');}
if(localStorage.getItem("L1_menu_td_id")==null)
{localStorage.setItem("L1_menu_td_id","Status");}
else
{
L1_click_td_id=localStorage.getItem("L1_menu_td_id");
changeClass(L1_click_td_id,'L1_color');
changeClass(L1_td_id,'L1_mouse_color');
localStorage.setItem("L1_menu_td_id",L1_td_id);
}
if(m!="_m99") {}    //_m99 no set L123
if(click_type=="menu")
{$("#"+id).animate({height: 'toggle'});}
else
{
if(document.getElementById(id)!=null)
{document.getElementById(id).style.display="";}
}
change_fancybox_shade();
}
function display_L3_menu(L3_id,click_type)
{
if(!login) return;
L2_id='L2_e'+localStorage.getItem("L2_menu");
if(L3_id==null)
{
L3_id="L3_e_1_0";
localStorage.setItem("L3_menu_id","MENU_dash_network");
}
if(L3_click_menu=="")
{L3_click_menu=L3_id;}
else if(L3_click_menu!=L3_id)
{
var L3_display_state=document.getElementById(L3_click_menu).style.display;
if($("#"+L3_click_menu).children().length>1 && L3_display_state!="none")
{$("#"+L3_click_menu).animate({height: 'toggle'});}
L3_click_menu=L3_id;
}
localStorage.setItem("L3_menu", L3_id);
changeClass(localStorage.getItem("L3_menu_id"),'LM2_mouse');
if($("#"+L3_id).children().length>1)
{
if(click_type=="menu")
{$("#"+L3_id).animate({height: 'toggle'});}
else
{document.getElementById(L3_id).style.display="";}
}
change_fancybox_shade();
}
function L3_menu_link(web_link,L3_id)
{
var p;
var s;
localStorage.setItem("left_menu_1",document.getElementById("expand_left_menu_1").style.display);
localStorage.setItem("left_menu_2",document.getElementById("expand_left_menu_2").style.display);
localStorage.setItem("L3_menu_id","MENU_"+L3_id);
localStorage.setItem("L2_menu_color","0");
for(i1=0;i1<TAID_L3.length;i1++)
{
p=TAID_L3[i1].split("|");
for(j1=0;j1<p.length;j1++)
{
if(p[j1]==L3_id)
{localStorage.setItem("L2_menu_color","0");}
s=p[j1].split("~");
for(k1=0;k1<s.length;k1++)
{
if(L3_id==s[k1])
{
localStorage.setItem("L2_menu", i1);
localStorage.setItem("L3_menu", "L3_e_"+i1+"_"+j1);
break;
}
}
}
}
location.href=web_link;
}
function save_L2_menu_id(p,s,L2_id,web_link,L3_num)
{
localStorage.setItem("L2_menu_id",L2_id);
if(L3_num==1)
{
localStorage.setItem("L3_menu","L3_e_"+p+"_"+s);
localStorage.setItem("L3_menu_id","MENU_dash_network");
localStorage.setItem("left_menu_1",document.getElementById("expand_left_menu_1").style.display);
localStorage.setItem("left_menu_2",document.getElementById("expand_left_menu_2").style.display);
localStorage.setItem("L2_menu_color","0");
if(p==0)
{top.frames['main'].location.href=web_link;}
else
{location.href=web_link;}
}
else
{display_L3_menu('L3_e_'+p+'_'+s,"menu");}
}
function save_L3_menu_id(L3_id,web_link)
{
localStorage.setItem("L3_menu_id",L3_id);
localStorage.setItem("left_menu_1",document.getElementById("expand_left_menu_1").style.display);
localStorage.setItem("left_menu_2",document.getElementById("expand_left_menu_2").style.display);
localStorage.setItem("L2_menu_color","0");
location.href=web_link;
}
/* =====table(,,)===== */
function L4button_SPgTl(num, is_toggle, is_help, is_dialog, is_delete, is_toggle_id, is_show_menu, toggle_name,on_off_name)
{
var str;
str = "<ul id='icons' style='float:right'>";
if(is_toggle==1)
{
str = str + "<li class='ui-state-default ui-corner-all' id='toggle_up_"+num+"'";
str = str + "onClick=\"table_toggle('toggle_up_"+num+"','toggle_down_"+num+"','#toggle_table_"+num+"','"+toggle_name+"',"+(num-1)+",'"+is_toggle_id+"')\" ";
str = str + "onmouseover=\"title_figure_color('#toggle_up_"+num+"','1')\" ";
str = str + "onmouseout=\"title_figure_color('#toggle_up_"+num+"','0')\" ";
str = str + "onmousedown=\"drag_table_cancel()\">";
str = str + "<span class='ui-icon ui-icon-triangle-1-n'></span>";
str = str + "</li>";
str = str + "<li class='ui-state-default ui-corner-all' id='toggle_down_"+num+"'";
str = str + "onClick=\"table_toggle('toggle_up_"+num+"','toggle_down_"+num+"','#toggle_table_"+num+"','"+toggle_name+"',"+(num-1)+",'"+is_toggle_id+"')\" ";
str = str + "onmouseover=\"title_figure_color('#toggle_down_"+num+"','1')\" ";
str = str + "onmouseout=\"title_figure_color('#toggle_down_"+num+"','0')\" ";
str = str + "onmousedown=\"drag_table_cancel()\">";
str = str + "<span class='ui-icon ui-icon-triangle-1-s'></span>";
str = str + "</li>";
}
if(is_help==1)
{
str = str + "<li class='ui-state-default ui-corner-all' id='help_figure_"+num+"'";
str = str + "onClick=\"open_help_dialog("+num+")\" ";
str = str + "onmouseover=\"title_figure_color('#help_figure_"+num+"','1')\" ";
str = str + "onmouseout=\"title_figure_color('#help_figure_"+num+"','0')\" ";
str = str + "onmousedown=\"drag_table_cancel()\">";
str = str + "<span class='ui-icon ui-icon-help'></span>";
str = str + "</li>";
}
if(is_dialog==1)
{
str = str + "<li class='ui-state-default ui-corner-all' id='dialog_figure_"+num+"'";
str = str + "onClick=\"open_common_dialog()\" ";
str = str + "onmouseover=\"title_figure_color('#dialog_figure_"+num+"','1')\" ";
str = str + "onmouseout=\"title_figure_color('#dialog_figure_"+num+"','0')\" ";
str = str + "onmousedown=\"drag_table_cancel()\">";
str = str + "<span class='ui-icon ui-icon-gear'></span>";
str = str + "</li>";
}
if(is_delete==1)
{
str = str + "<li class='ui-state-default ui-corner-all' id='delete_figure_"+num+"'";
str = str + "onClick=\"table_close('"+on_off_name+"','menu_image_"+num+"','table_zone_"+num+"',"+(num-1)+")\" ";
str = str + "onmouseover=\"title_figure_color('#delete_figure_"+num+"','1')\" ";
str = str + "onmouseout=\"title_figure_color('#delete_figure_"+num+"','0')\" ";
str = str + "onmousedown=\"drag_table_cancel()\">";
str = str + "<span class='ui-icon ui-icon-closethick'></span>";
str = str + "</li>";
}
str = str + "</ul>";
return str;
}
function L4button_sub_table(num, is_toggle, is_help, is_dialog, is_delete, is_toggle_id, delete_id, disabled_name)
{
var str;
str = "<ul id='icons' style='float:right'>";
if(is_toggle==1)
{
str = str + "<li class='ui-state-default ui-corner-all' id='sub_toggle_up_"+num+"'";
str = str + "onClick=\"sub_table_toggle('sub_toggle_up_"+num+"','sub_toggle_down_"+num+"','#sub_toggle_table_"+num+"','"+is_toggle_id+"')\" ";
str = str + "onmouseover=\"title_figure_color('#sub_toggle_up_"+num+"','1')\" ";
str = str + "onmouseout=\"title_figure_color('#sub_toggle_up_"+num+"','0')\" >";
str = str + "<span class='ui-icon ui-icon-triangle-1-n'></span>";
str = str + "</li>";
str = str + "<li class='ui-state-default ui-corner-all' id='sub_toggle_down_"+num+"' style='display:none'";
str = str + "onClick=\"sub_table_toggle('sub_toggle_up_"+num+"','sub_toggle_down_"+num+"','#sub_toggle_table_"+num+"','"+is_toggle_id+"')\" ";
str = str + "onmouseover=\"title_figure_color('#sub_toggle_down_"+num+"','1')\" ";
str = str + "onmouseout=\"title_figure_color('#sub_toggle_down_"+num+"','0')\" >";
str = str + "<span class='ui-icon ui-icon-triangle-1-s'></span>";
str = str + "</li>";
}
if(is_help==1)
{
str = str + "<li class='ui-state-default ui-corner-all' id='sub_help_figure_"+num+"'";
str = str + "onClick=\"sub_open_help_dialog("+num+")\" ";
str = str + "onmouseover=\"title_figure_color('#sub_help_figure_"+num+"','1')\" ";
str = str + "onmouseout=\"title_figure_color('#sub_help_figure_"+num+"','0')\" >";
str = str + "<span class='ui-icon ui-icon-help'></span>";
str = str + "</li>";
}
if(is_dialog==1)
{
str = str + "<li class='ui-state-default ui-corner-all' id='sub_dialog_figure_"+num+"'";
str = str + "onClick=\"open_common_dialog()\" ";
str = str + "onmouseover=\"title_figure_color('#sub_dialog_figure_"+num+"','1')\" ";
str = str + "onmouseout=\"title_figure_color('#sub_dialog_figure_"+num+"','0')\" >";
str = str + "<span class='ui-icon ui-icon-gear'></span>";
str = str + "</li>";
}
if(is_delete==1)
{
str = str + "<li class='ui-state-default ui-corner-all' id='sub_delete_figure_"+num+"'";
str = str + "onClick=\"sub_table_close('"+delete_id+"','"+disabled_name+"')\" ";
str = str + "onmouseover=\"title_figure_color('#sub_delete_figure_"+num+"','1')\" ";
str = str + "onmouseout=\"title_figure_color('#sub_delete_figure_"+num+"','0')\" >";
str = str + "<span class='ui-icon ui-icon-closethick'></span>";
str = str + "</li>";
}
str = str + "</ul>";
return str;
}
function title_figure_color(id,is_over)
{
if(is_over=="1")
{$(id).addClass("ui-state-highlight");}
else
{$(id).removeClass("ui-state-highlight");}
}
function add_figure_function(sort_name,toggle_name,on_off_name)
{
chgLang();
if(sort_name=="")
{no_drag_table_function();}
else
{drag_table_function(sort_name);}
table_toggle_function(toggle_name);
open_dialog_function();
table_on_off_function(on_off_name);
global_on_off_name=on_off_name;
}
/* =====table===== */
function no_drag_table_function()
{
var sort_table=$("#drag_sort_table");
var drag_zone = document.getElementsByName("drag_zone");
var sort_table_array = new Array();
for (i = 0; i < drag_zone.length; i++)
{sort_table_array[i]=add_figure_string(i+1,drag_zone[i].innerHTML);}
$("div[name='drag_zone']").remove();
for (i = 0; i < sort_table_array.length; i++)
{sort_table.append(sort_table_array[i]);}
}
function drag_table_function(storage_name)
{
var sort_data=localStorage.getItem(storage_name);
var sort_data_array;
var start_index;
var end_index;
var sort_table=$("#drag_sort_table");
var sort_table_array = new Array();
var drag_zone = document.getElementsByName("drag_zone");
var drag_content_1;
var drag_content_2;
var drag_content_3;
if(sort_data==null)
{
sort_data="";
for (i = 0; i < drag_zone.length; i++)
{sort_data=sort_data+(i+1)+",";}
sort_data=sort_data.substring(0, sort_data.length-1);
localStorage.setItem(storage_name, sort_data);
}
else
{
sort_data_array=sort_data.split(",");
if(sort_data_array.length!=drag_zone.length)
{
sort_data="";
for (i = 0; i < drag_zone.length; i++)
{sort_data=sort_data+(i+1)+",";}
sort_data=sort_data.substring(0, sort_data.length-1);
localStorage.setItem(storage_name, sort_data);
}
}
sort_data=localStorage.getItem(storage_name);
sort_data_array=sort_data.split(",");
for (i = 0; i < sort_data_array.length; i++)
{sort_table_array[i]=add_figure_string(i+1,drag_zone[i].innerHTML);}
$("div[name='drag_zone']").remove();
for (i = 0; i < sort_data_array.length; i++)
{
n=parseInt(sort_data_array[i])-1;
sort_table.append(sort_table_array[n]);
}
if(!is_mobile_login)
{
sort_table.sortable({
revert: true,
placeholder: "table_shade",
disabled: true,
start: function(e, ui){
ui.placeholder.height(ui.item.height()-4);
start_index=ui.item.index();
},
update: function(e, ui){
end_index=ui.item.index();
sort_data=localStorage.getItem(storage_name);
sort_data_array=sort_data.split(",");
if(start_index<end_index)
{
sort_data_array.splice(end_index+1 ,0 ,sort_data_array[start_index]);
sort_data_array.splice(start_index ,1);
}
else
{
sort_data_array.splice(end_index ,0 ,sort_data_array[start_index]);
sort_data_array.splice(start_index+1 ,1);
}
sort_data="";
for (i = 0; i < sort_data_array.length; i++)
{sort_data=sort_data+sort_data_array[i]+",";}
sort_data=sort_data.substring(0, sort_data.length-1);
localStorage.setItem(storage_name, sort_data);
}
});
}
}
function add_figure_string(n,str)
{
var table_str = str;
var table_str_1;
var table_str_2;
var figure_function;
var toggle_function;
var str_data_1;
var str_data_2;
var span_position_1;
var span_position_2;
var span_num;
while (table_str.indexOf("<script")>=0 && table_str.indexOf("</script>")>=0)
{
a = table_str.indexOf("<script");
b = table_str.indexOf("</script>");
table_str = table_str.substring(0, a) + table_str.substring(b+9);
}
a = table_str.indexOf("add_pertain_function");
if(a==-1)     //add_pertain_function
{table_str = "<div id='table_zone_"+n+"'>" + table_str + "</div>";}
else
{
b=a-5;
while (table_str.substring(b, a).indexOf("<span")<0)
{b=b-1;}
table_str_1 = table_str.substring(0, b);
table_str_2 = table_str.substring(a);
a = table_str_2.indexOf("</span>");
table_str_2 = table_str_2.substring(a+7);
figure_function = document.getElementsByName("add_pertain_function");
str_data_1 = figure_function[add_pertain_function_index].innerHTML.split(",");
add_pertain_function_index = add_pertain_function_index+1;
table_str = table_str_1 + L4button_SPgTl(n,str_data_1[0],str_data_1[1],str_data_1[2],str_data_1[3],str_data_1[4],str_data_1[5],str_data_1[6],str_data_1[7]) + table_str_2;
table_str = "<div id='table_zone_"+n+"'>" + table_str + "</div>";
show_common_menu[n-1] = 0;
if(str_data_1[5]=="1")
{show_common_menu[n-1] = 1;}
while (table_str.indexOf("sub_add_pertain_function")>=0)
{
a = table_str.indexOf("sub_add_pertain_function");
b=a-5;
while (table_str.substring(b, a).indexOf("<span")<0)
{b=b-1;}
table_str_1 = table_str.substring(0, b);
table_str_2 = table_str.substring(a);
a = table_str_2.indexOf("</span>");
table_str_2 = table_str_2.substring(a+7);
figure_function = document.getElementsByName("sub_add_pertain_function");
str_data_1 = figure_function[sub_add_pertain_function_index].innerHTML.split(",");
table_str = table_str_1 + L4button_sub_table(sub_add_pertain_function_index+1,str_data_1[0],str_data_1[1],str_data_1[2],str_data_1[3],str_data_1[4],str_data_1[5],str_data_1[6]) + table_str_2;
a = table_str.indexOf("sub_add_toggle_table");
table_str_1 = table_str.substring(0, a-12);
table_str_2 = table_str.substring(a-12);
toggle_function = document.getElementsByName("sub_add_toggle_table");
str_data_2 = toggle_function[sub_add_pertain_function_index].innerHTML;
while (str_data_2.indexOf("<script")>=0 && str_data_2.indexOf("</script>")>=0)
{
a = str_data_2.indexOf("<script");
b = str_data_2.indexOf("</script>");
str_data_2 = str_data_2.substring(0, a) + str_data_2.substring(b+9);
}
for (j = 0; j < (str_data_2.split("</span>").length); j++)
{
if(j==0)
{span_position_1=table_str_2.indexOf("</span>");}
else
{span_position_1=table_str_2.indexOf("</span>",span_position_1+1);}
}
table_str_2=table_str_2.substring(span_position_1+7);
table_str = table_str_1 + "<div id='sub_toggle_table_"+(sub_add_pertain_function_index+1)+"'>" + str_data_2 + "</div>" + table_str_2;
sub_add_pertain_function_index = sub_add_pertain_function_index + 1;
}
a = table_str.indexOf("add_toggle_table");
table_str_1 = table_str.substring(0, a-12);
table_str_2 = table_str.substring(a-12);
toggle_function = document.getElementsByName("add_toggle_table");
str_data_2 = toggle_function[toggle_function_index].innerHTML;
toggle_function_index = toggle_function_index+1;
while (str_data_2.indexOf("<script")>=0 && str_data_2.indexOf("</script>")>=0)
{
a = str_data_2.indexOf("<script");
b = str_data_2.indexOf("</script>");
str_data_2 = str_data_2.substring(0, a) + str_data_2.substring(b+9);
}
for (j = 0; j < (str_data_2.split("</span>").length); j++)
{
if(j==0)
{span_position_1=table_str_2.indexOf("</span>");}
else
{span_position_1=table_str_2.indexOf("</span>",span_position_1+1);}
}
table_str_2=table_str_2.substring(span_position_1+7);
table_str = table_str_1 + "<div id='toggle_table_"+n+"'>" + str_data_2 + "</div>" + table_str_2;
}
return table_str;
}
function drag_table_enable()
{$("#drag_sort_table").sortable("enable");}
function drag_table_cancel()
{$("#drag_sort_table").sortable("disable");}
/* =====table===== */
function table_toggle_function(storage_name)
{
var toggle_data=localStorage.getItem(storage_name);
if(toggle_data==null)
{
toggle_data="";
for (i = 0; i < $("#drag_sort_table").children().length; i++)
{toggle_data=toggle_data+"1";}
localStorage.setItem(storage_name, toggle_data);
}
else
{
if(toggle_data.length!=$("#drag_sort_table").children().length)
{
toggle_data="";
for (i = 0; i < $("#drag_sort_table").children().length; i++)
{toggle_data=toggle_data+"1";}
localStorage.setItem(storage_name, toggle_data);
}
}
toggle_data=localStorage.getItem(storage_name);
for (i = 1; i <= toggle_data.length; i++)
{
if(toggle_data[i-1]=="0")
{
if(document.getElementById("toggle_table_"+i)!=null)
{document.getElementById("toggle_table_"+i).style.display = "none";}
if(document.getElementById("toggle_up_"+i)!=null)
{document.getElementById("toggle_up_"+i).style.display = "none";}
}
else
{
if(document.getElementById("toggle_down_"+i)!=null)
{document.getElementById("toggle_down_"+i).style.display = "none";}
}
}
}
function table_toggle(up_id,down_id,t_id,storage_name,n,is_toggle_id)
{
var is_toggle = 1;
var check_toggle_id = is_toggle_id.split("~");
if(is_toggle_id.indexOf("~")>0)
{
for (i = 0; i < check_toggle_id.length; i++)
{
if(document.getElementById(check_toggle_id[i])!=null)
{
if(document.getElementById(check_toggle_id[i]).style.display!="none"  && document.getElementById(up_id).style.display!="none")
{
is_toggle=0;
break;
}
}
}
}
else
{
if(document.getElementById(is_toggle_id)!=null)
{
if(document.getElementById(is_toggle_id).style.display!="none"  && document.getElementById(up_id).style.display!="none")
{is_toggle=0;}
}
}
if(is_toggle == 1)
{
var up = document.getElementById(up_id);
var down = document.getElementById(down_id);
var toggle_on_off = localStorage.getItem(storage_name);
var on_off;
$(t_id).animate({height: 'toggle'});
if(up!=null && down!=null)
{
if(down.style.display=="")
{
down.style.display="none";
up.style.display="";
on_off="1";
}
else
{
down.style.display="";
up.style.display="none";
on_off="0";
}
toggle_on_off = toggle_on_off.substring(0,n) + on_off + toggle_on_off.substring(n+1);
localStorage.setItem(storage_name, toggle_on_off);
}
}
}
function sub_table_toggle(up_id,down_id,t_id,is_toggle_id)
{
var is_toggle = 1;
var check_toggle_id = is_toggle_id.split("~");
if(is_toggle_id.indexOf("~")>0)
{
for (i = 0; i < check_toggle_id.length; i++)
{
if(document.getElementById(check_toggle_id[i])!=null)
{
if(document.getElementById(check_toggle_id[i]).style.display!="none" && document.getElementById(up_id).style.display!="none")
{
is_toggle=0;
break;
}
}
}
}
else
{
if(document.getElementById(is_toggle_id)!=null)
{
if(document.getElementById(is_toggle_id).style.display!="none"  && document.getElementById(up_id).style.display!="none")
{is_toggle=0;}
}
}
if(is_toggle == 1)
{
var up = document.getElementById(up_id);
var down = document.getElementById(down_id);
$(t_id).animate({height: 'toggle'});
if(up!=null && down!=null)
{
if(down.style.display=="")
{
down.style.display="none";
up.style.display="";
}
else
{
down.style.display="";
up.style.display="none";
}
}
}
}
/* ========== */
function open_dialog_function()
{
dw("<div id='common_dialog' title='title' style='display:none'>");
dw("<table width=100% height=80%>");
dw("<tr><td  valign='middle'>Refresh Interval <input type=text id='second_number' size=5> Second</td></tr>");
dw("</table>");
dw("<table width=100% height=20%>");
dw("<tr>");
dw("<td align='center' width=50%><input type=button id='dialog_ok_button' value='ok'></td>");
dw("<td align='center' width=50%><input type=button id='dialog_cancel_button' value='cancel'></td>");
dw("</tr>");
dw("</table>");
dw("</div>");
$(function() {
$("#common_dialog").dialog({
autoOpen: false,
