function SetCkBox(chkbox,ckv,nckv,chkid,hids,hide) //chkid=\`}(checkbox)
{
f=chkbox.form
for(cb=0;cb<f.length;cb++){if(f.elements[cb]==chkbox)break;}
f.elements[cb-1].value=(f.elements[cb].checked)?ckv:nckv
if(arguments.length==4){ //`}~|do "prim_switch" function
prim_switch(chkid);
}else if(arguments.length==6){
switch_hidden(chkid,hids,hide);}
}
function SetSelHidval(selectobjs)
{
if(typeof(selectobjs)=="object") { f=selectobjs.form }
else { f=document.forms[0]; }
for(cb=0;cb<f.length;cb++){if(f.elements[cb]==selectobjs)break;}
if (!window.ActiveXObject)
{
f.elements[cb-1].style.display="none"
f.elements[cb-1].type="text"
}
f.elements[cb-1].value=(typeof(selectobjs)=="object")? selectobjs.value : selectobjs
}
