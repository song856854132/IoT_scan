function show_L1_button(l, link) {
topsnm=TopMenus[l]
topsnm_logo=TopMenus_logo[l]
topsnm0=eval(topsnm+"[0]") //L1 name
topsnm1=(showL1Help==true || link==null)?eval(topsnm+"[1]"):link; //L1 link
L1ID=TAID_L1[l]            //L1 TAID
dw("<TR " + setTopMenus(topsnm) + " CLASS=handlk onMouseOver=\"changeClass('TA0_"+ L1ID +"','LM_mouse')\" onMouseOut=\"changeClass('TA0_"+ L1ID +"','LM')\" ONCLICK=\"displaymenu('"+ l +"','menu','"+L1ID+"')\"><TD HEIGHT=45 colspan=3 width=100% id='TA0_"+ L1ID +"' CLASS='LM' NOWRAP><IMG SRC=sp.gif WIDTH=12 HEIGHT=5 BORDER=0><IMG SRC="+topsnm_logo+".gif WIDTH=30 HEIGHT=30 ALIGN=ABSMIDDLE><IMG SRC=sp.gif WIDTH=6 HEIGHT=5 BORDER=0 ALIGN=ABSMIDDLE>")
dw("<A id='TA_"+ L1ID +"' xHREF=#>"+ topsnm0 +"</A></TD></TR>")
dw("<TR id='_m"+ l +"'><td colspan=3 width=100%><div id='L2_e"+ l +"' style='display:none;'><table border=0 align=right width=99%>")
}
