function expand_left_menu()
{
var panel = document.getElementById("panel_img");
var url=panel.src.split("/");
var name=url[url.length-1];
if(name == "panel_left-b.svg")
{panel.src = "panel_right-b.svg";}
else
{panel.src = "panel_left-b.svg";}
$("#expand_left_menu_1").animate({width: 'toggle'});
$("#expand_left_menu_2").animate({width: 'toggle'});
change_fancybox_shade();
}
function click_left_menu_2(m,L1_td_id)
{
var panel = document.getElementById("panel_img");
var url=panel.src.split("/");
var name=url[url.length-1];
if(name == "panel_left-b.svg")
{panel.src = "panel_right-b.svg";}
else
{panel.src = "panel_left-b.svg";}
$("#expand_left_menu_1").animate({width: 'toggle'});
$("#expand_left_menu_2").animate({width: 'toggle'});
if(document.getElementById("L2_e"+m).style.display=="none")
{displaymenu(m,"menu",L1_td_id);}
setTimeout("changeClass('"+L1_td_id+"','L1_mouse_color')", 2000);
change_fancybox_shade();
}
