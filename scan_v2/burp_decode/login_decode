function logout()
{
mGURL("/log/out?rd=/uir/start.htm?Nrd=2");
}
function clk(link)
{
var links=link;
if(link=="#")return link
if((link.indexOf(':')==-1)&&(link.indexOf('.')==-1)&&(link.indexOf('=')==-1))links="/"+link+".htm?rc="
return links
}
function displayL1Menu(){
m='_m'+setLMtp();
if(!login) return;
for(i=0;i<TopMenus.length;i++){
document.getElementById('_m'+i).style.display='none';
}
if(m!="_m99") {    //_m99 no set L123
document.getElementById(m).style.display='';
}
}
