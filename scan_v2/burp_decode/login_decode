function locateObj(fm,fobj,offset)
{
for(var j=0;j<fm.length;j++)
{
if(fm.elements[j]==fobj)break;
}
return (arguments.length<3)?j:eval("fm.elements[j"+offset+"]");
}
function chkundo()
{
f=document.forms[0]
f.reset();
for(i=0;i<f.length;i++) showmsg(f.elements[i],true)
}
function pattern()
{
ips="(25[0-5]|2[0-4][0-9]|[1]?[0-9][0-9]?)"
ips_0="(25[0-5]|2[0-4][0-9]|[1][0-9][0-9]|[1-9][0-9]|[1-9])"
ipss=ips+"\\."+ips+"\\."+ips+"\\."+ips
n0_32="(3[0-2]|2[0-9]|1[0-9]|[0-9])"
ipssn=ipss+"\/"+n0_32
ipssnm="^"+ipssn+"$|"+ipssn+"(\\;"+ipssn+")+"
hexs="([0-9a-fA-F]+)"
hex2="([0-9a-fA-F]{2})"
ports="([0-9]{1,5})"
numbers="([0-9]+)"
numbers2="^(0|[1-9][0-9]*)$"
hours="(2[0-3]|1[0-9]|[0-9]|0[0-9])"
minutes="([1-5][0-9]|[0-9]|0[0-9])"
smasks="(1+0+|1{8}|0{8})"
ccsmasks="(1{8})"
strs="([^\r|^\n]+)"
strsns="([^\x20]+)"
strfilename="([^\\\\/:\*?\"<>|]*)"
strfilename2="([0-9xX+\\-\\*\\/\\^\\(\\)]*)"
strfilename3="([^\\\\/\(\)\"<>']*)"
emails="([a-zA-Z0-9_\\-\\.]+\\@[a-zA-Z0-9_\\-\\.]+\\.[a-zA-Z]{2,5})"
emails2="^"+emails+"$|"+emails+"(\\;"+emails+")+|"+emails+"(\\,"+emails+")+"
inte="([1-9]|[1-9][0-9]|[1-9][0-9][0-9]|[1-9][0-9][0-9][0-9]|[1-9][0-9][0-9][0-9][0-9])"
namepass="([a-zA-Z0-9_\\ -]*)"
namepass2="([a-zA-Z0-9_\\-\\.]*)"
namepass3="([a-zA-Z0-9-]*)"
namepass4="([a-zA-Z0-9-_/]*)"
nonword="([A-Za-z0-9]+)"
strsnmp="([a-zA-Z0-9_]*)"
strsnmp1="([0-9.]*)"
strsnmp2="([a-zA-Z0-9.-]*)"
hexcolor="(^#([0-9a-fA-F]{3}){1,2}$)";
this.ip1=ips
this.ip=ipss
this.iprange=ipss+"-"+ipss
this.ipms="^"+ipss+"$|"+ipss+"(\\;"+ipss+")+"
this.subnetmask=smasks+"\\."+smasks+"\\."+smasks+"\\."+smasks
this.ccsubnetmask=ccsmasks+"\\."+ccsmasks+"\\."+ccsmasks+"\\."+smasks
this.port=ports
this.portrange=ports+"-"+ports
this.hex=hexs
this.mac=hex2+"-"+hex2+"-"+hex2+"-"+hex2+"-"+hex2+"-"+hex2
this.bmac=hex2+":"+hex2+":"+hex2+":"+hex2+":"+hex2+":"+hex2
this.macs="^"+hex2+":"+hex2+":"+hex2+":"+hex2+":"+hex2+":"+hex2+"$|"
+hex2+":"+hex2+":"+hex2+":"+hex2+":"+hex2+":"+hex2+"(\\;"+hex2+":"+hex2+":"+hex2+":"+hex2+":"+hex2+":"+hex2+")+"
this.number=numbers
this.number2=numbers2
this.time=hours+":"+minutes
this.hour=hours
this.minute=minutes
this.ctime=hours+":"+minutes+":"+minutes
this.str=strs
this.strn=strsns
this.strname=strfilename
this.strname2=strfilename2
this.strname3=strfilename3
this.email=emails
this.emailm=emails2
this.ipnms=ipssnm
this.n0_32=n0_32
this.ipnms1=ipssn
this.inte=inte
this.name=namepass
this.name2=namepass2
this.name3=namepass3
this.name4=namepass4
this.nonwords=nonword
this.strsnmps=strsnmp
this.strsnmps1=strsnmp1
this.strsnmps2=strsnmp2
this.hexcolor=hexcolor;
}
function showmsg(cobj,Result,msg)
{
sobjia=inArr(spanary,cobj)
if(!Result){if(!sobjia)spanary[spanary.length]=cobj}
else if(sobjia)spanary.splice(inArrix,1)
cobj.style.color=(Result)?"":"#FF0000"
cobj.style.backgroundColor=(Result)?"":"#FFFF99"
}
var dis_arr=[];       //diabled//
function chkForm(v)
{
var k = 0;
dis_arr.length = 0;
if(v==null){
k = spanary.length;
}else if(v==1){             //chkform_disobj() ex WAN type change
for (i = 0; i < spanary.length; i++)
{
if($(spanary[i]).is(":hidden")||spanary[i].disabled){
dis_arr.push(spanary[i]);
}else{
k+=1;
}
}
}
if(k!=0)
{
alert(GetElmCont("valChk_0","Please check the settings again!"));
try {spanary[0].focus();}
catch( err ){}
return false;
}
else return true
}
function chkform_disobj(){
for (i = 0; i < dis_arr.length; i++)
{
dis_arr[i].disabled = true;
}
return true
}
function clear_valchk_for_undo(){
if(spanary.length!=0){
for(s=0;s<spanary.length;s++){
spanary[s].style.color=""
spanary[s].style.backgroundColor=""
}
spanary.length=0 //set to no error!!
}
}
function clear_valchk(obj){
if(spanary.length!=0){
for(s=0;s<spanary.length;s++){
if(obj==spanary[s]){ //spanary
spanary[s].style.color="";
spanary[s].style.backgroundColor="";
spanary.splice(s,1);
}
}
}
}
function cvr(chkval,rs,re)
{
renumber=new RegExp("^"+pn.number+"$")
chkvalo=chkval
chkval=parseInt(chkval)
if(arguments.length<3)
{
if(rs.indexOf("-")!=-1){
v=rs.split("-");
rs=v[0];
re=v[1];
}
else {
re=rs;
rs=0;
}
}
return (renumber.test(chkvalo)&&rs<=chkval&&chkval<=re)
}
function clen(chklen,rs,re)
{
restr=new RegExp("^"+pn.str+"$")
chkleno=chklen
chklen=chklen.length
if(arguments.length<3)
{
if(String(rs).indexOf("-")!=-1){
v=rs.split("-");
rs=v[0];
re=v[1];
}
else {
re=rs;
}
}
return (restr.test(chkleno)&&rs<=chklen&&chklen<=re)
}
function inArr(arr,itm)
{
inArrix=0
for(aa=0;aa<arr.length;aa++)
{
if(arr[aa]==itm){inArrix=aa;return true;}
}
return false;
}
function add0(k1)
{
k1l=k1.length
for(kk=0;kk<(8-k1l);kk++){k1='0'+k1}
return k1
}
function turnbinary(v)
{  //ip address  binary, for subnet mask checking.
ipa=v.split(".")
ipa2=""
for(i=0;i<4;i++)
{
k=parseInt(ipa[i]).toString(2)
if(k.length<8)k=add0(k)
ipa2+=k+((i<3)?".":"")
}
return ipa2
}
/* check IP - chkI(this,'a')
1. 'a' =>  IP 0.0.0.0
1. 'b' =>  IP 0.0.0.0
1. 'c' =>  IP mask 
1. 'd' =>  IP LAN IP  subnet
1. 'r' =>  IP range
*/
function chkI(obj,type,separator)
{
objval=obj.value
reip=new RegExp("^"+pn.ip+"$")
resm=new RegExp("^"+pn.subnetmask+"$")
resmcc=new RegExp("^"+pn.ccsubnetmask+"$")
reipr=new RegExp("^"+pn.iprange+"$")
ipc=reip.test(objval)
if(type=='a') // IP 0.0.0.0
{
cmsg=GetElmCont("valChk_1","IP address is invalid.")
chkResult=ipc
}
if(type=='b') // IP 0.0.0.0
{
cmsg=GetElmCont("valChk_1","IP address is invalid.")
chkResult=ipc&&(objval!="0.0.0.0")
}
if(type=='c') // subnet mask
{
cmsg=GetElmCont("valChk_2","Subnet mask is invalid.")
chkResult=reip.test(objval)?(resm.test(turnbinary(objval))):false
}
if(type=='d') // IP LAN IP  subnet
{
lanip="192.168.0.1"
cmsg="The subnet of IP address should be equal to LAN IP address("+lanip+")."
lips=lanip.lastIndexOf(".")
ipls=new RegExp("^"+lanip.substring(0,lips+1)+pn.ip1+"$")
chkResult=ipc&&ipls.test(objval)
}
if(type=='e') //subnet mask for class C
{
cmsg=GetElmCont("valChk_2","Subnet mask is invalid.")
chkResult=reip.test(objval)?(resmcc.test(turnbinary(objval))):false
}
if(type=='f') // FQDN
{
cmsg=GetElmCont("valChk_1","FQDN is invalid.");
var fqdnRegExp = new RegExp("^([a-z0-9]+\\.)?([a-z0-9][a-z0-9-]*\\.)+[a-z]{2,6}$");
chkResult = fqdnRegExp.test(objval);
}
if(type=='m') // multiple IP, separated by "separator"
{
cmsg=GetElmCont("valChk_1","IP address is invalid.");
if(typeof(separator)=='undefined') separator = ','; // set default separator to ','
var ip_list = objval.split(separator);
chkResult = true;
for(var ip_idx = 0; ip_idx < ip_list.length; ++ip_idx) { // check each IP
if(!reip.test(ip_list[ip_idx])) {
chkResult = false; // any one of IP is invalid
break;
}
}
}
if(type=='r') // normal IP range, including single IP address. e.g. 192.168.123.1-192.168.123.254
{
cmsg=GetElmCont("valChk_3","IP address range is invalid.");
chkResult=reip.test(objval)||reipr.test(objval);
}
if(type=='u') // url: allow IP addresses to also be a valid URL
{
cmsg=GetElmCont("valChk_1","URL is invalid.");
var urlReg = /^(http(s?):\/\/)?(((www\.)?[a-zA-Z0-9\.\-\_]+(\.[a-zA-Z]{2,6})+)|(\b(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\b))(:([1-9][0-9]{0,3}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5]))?(\/[a-zA-Z0-9\_\-\s\.\/\?\%\#\&\=]*)?$/;
chkResult = urlReg.test(objval);
}
if(type=='l') { // valid IP address, including 0.0.0.0. Don't accept empty value.
cmsg=GetElmCont("valChk_4","LAN IP address is invalid.");
chkResult=ipc&&(objval!="");
}
else { // accept empty value if type is other than 'l'
chkResult=chkResult||(objval=="");
}
showmsg(obj,chkResult,cmsg);
return chkResult;
}
/* check Number - chkN(this,'a')
1. 'i' =>  ip1~2550 
1. 'p' => tcp/udp  port 1~65535 0 
1. 'q' => tcp/udp  port 1~65535"" 
1. 'r' => tcp/udp  port range port  port range
1. 'n' => tcp/udp  port range port  port( ","" ) port range  (eg.Special AP  incoming port)
1. 'j' => static/Dynamic MTU (?)
1. 'm' => PPPoE MTU (?)
1. 'l' => PPTP MTU (?)
1. 'l' => L2TP MTU (?)
1. 't' => wan type idle timeout
1. 'x' => admin idle timeout
1. 'd' => @XkrAiH]t 0Ap MTU, idle time ()
1. 'u' => User defined
*/
function chkN(obj,type,range,itemname)
{
objval=obj.value
report =new RegExp("^"+pn.port+"$")
reportr=new RegExp("^"+pn.portrange+"$")
renumber=new RegExp("^"+pn.number+"$")
renumber2=new RegExp("^"+pn.number2+"$")
inte = new RegExp("^"+pn.inte+"$");
if(arguments.length<3)
{
range=""
itemname=""
/*
valuerange
ex.<input type="text" id="vrrp_si>range,
htmlrange<span>user, range
<span><text id>+<_range>,<text>
<span id=vrrp_si_range>, <input type="text" id="vrrp_si>range
......toshi
*/
tagary=document.getElementsByTagName("span");
for(t=0;t<tagary.length;t++){
tid=tagary[t].id          //get <span id>
if(tid==obj.id+"_range"){ //ex. <span id=vrrp_si_range>(1-255)</span>
tids=GetElmCont(tid)   //get (1-255)
range=tids.substring(1,tids.length-1) //range=1-255
}
}
/*
...toshi
if((obj.id.length>0)&&(obj.id.indexOf("format : ")!=-1))
{
otls=splits(obj.id,"format : ")
itemname=otls[0]
range=(otls.length>1)?otls[1]:""
}
*/
if(itemname=="")itemname="Value"
}
if(type=='i') // ip1~2550 
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=(report.test(objval)&&cvr(objval,0,255))
}
if(type=='k') // ip1~255 0
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=((objval=="")||report.test(objval)&&cvr(objval,1,255))
}
if(type=='i_1') // ip1~254
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=((objval=="")||report.test(objval)&&cvr(objval,1,254))
}
if(type=='i_2') //iprange0~255
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=((objval=="")||report.test(objval)&&cvr(objval,0,255))
}
if(type=='i_3')
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=((objval=="")||report.test(objval)&&cvr(objval,range))
}
if(type=='o') //Number 0~65536 0 
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=((objval=="")||renumber.test(objval)&&cvr(objval,0,65536))
}
if(type=='o_1') //Number 0~65535 0 
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=((objval=="")||renumber.test(objval)&&cvr(objval,0,65535))
}
if(type=='p') //tcp/udp  port 1~65535 0 
{
cmsg=GetElmCont("valChk_6","Port is invalid.")
chkResult=(report.test(objval)&&cvr(objval,1,65535))
}
if(type=='q') //tcp/udp  port 1~65535 "" 
{
cmsg=GetElmCont("valChk_6","Port is invalid.")
chkResult=((objval=="")||cvr(objval,1,65535))
}
if(type=='f_1') //range 10~10000
{
cmsg=GetElmCont("aipslist_18","Packets/second value is 10~10000!")
chkResult=((objval=="")||cvr(objval,10,10000))
}
if(type=='r') //tcp/udp  port range port  port range
{
reportr.exec(objval)
cmsg=GetElmCont("valChk_8","Port range is invalid.")+" (Format eg. 1-65535)"
portrary=splits(objval,"-")
chkResult=((objval.indexOf("-")!=-1)?(cvr(portrary[0],"1-65535")&&cvr(portrary[1],"1-65535")&&(parseInt(portrary[0])<parseInt(portrary[1]))):(report.test(objval)&&cvr(objval,1,65535)))
}
if(type=='n') // tcp/udp  port range port  port( "," ) port range  (eg.Special AP  incoming port)
{
cmsg=GetElmCont("valChk_6","Port is invalid.")+" (Format eg. 1-65535,777)"
portrsary=splits(objval,",")
cflag=0
for(p=0;p<portrsary.length;p++)
{
portrary=splits(portrsary[p],"-")
chkResult=((portrsary[p].indexOf("-")!=-1)?(cvr(portrary[0],"1-65535")&&cvr(portrary[1],"1-65535")&&(parseInt(portrary[0])<parseInt(portrary[1]))):(report.test(portrsary[p])&&cvr(portrsary[p],1,65535)))
if(!chkResult)cflag++
}
chkResult=(cflag==0)
}
if(type=='a') //
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=(renumber.test(objval))
}
if(type=='a2') //,00
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=(renumber2.test(objval))
}
if(type=='b') //0 ,
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=((renumber.test(objval))||(objval==""))
}
/*
1. 'j' => static/Dynamic MTU  (range?)
1. 'k' => PPPoE MTU (range?)
1. 'l' => PPTP MTU  (range?)
1. 'm' => L2TP MTU  (range?)
1. 't' => wan type idle timeout  (range?)
1. 'x' => admin idle timeout (range?)
*/
if(type=='j') //static/Dynamic MTU  (range?)
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=(renumber.test(objval)&&cvr(objval,512,1500)||(objval=="0"))
}
if(type=='m') //PPPoE MTU (range?)
{
cmsg=GetElmCont("valChk_9","MTU is invalid!")+" (576<=MTU<=1492 or 0(Auto))"
chkResult=(renumber.test(objval)&&(cvr(objval,512,1492)||(objval=="0")))
}
if(type=='l') //PPTP MTU (range?)
{
cmsg=GetElmCont("valChk_9","MTU is invalid!")+" (576<=MTU<=1460 or 0(Auto))"
chkResult=(renumber.test(objval)&&(cvr(objval,512,1460)||(objval=="0")))
}
if(type=='g') //3G MTU (range 0-1500)
{
cmsg=GetElmCont("valChk_9","MTU is invalid!")+" (0<=MTU<=1500 or 0(Auto))"
chkResult=(renumber.test(objval)&&(cvr(objval,0,1500)||(objval=="0")))
}
if(type=='h') //TLS Renegotiation Time (range 0-86400)
{
cmsg=GetElmCont("valChk_9","TLS Renegotiation Time is invalid!")+" (0-86400)"
chkResult=(renumber.test(objval)&&(cvr(objval,0,86400)||(objval=="0")))
}
if(type=='v') //Connection Retry (range -1~86400)
{
cmsg=GetElmCont("valChk_9","Connection Retry is invalid!")+" (-1~86400)"
nnn=("-1"==objval)
chkResult=(nnn||(renumber.test(objval)&&(cvr(objval,0,86400)||(objval=="0"))))
}
/*
if(type=='m') //L2TP MTU (range?)
{
cmsg="MTU is invalid! (576<=MTU<=1460)"
chkResult=(renumber.test(objval)&&cvr(objval,576,1460)&&(objval!="0"))
}
*/
if(type=='t') //wan type idle timeout  (range?)
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=(renumber.test(objval)&&cvr(objval,30,300)&&(objval!="0"))
}
if(type=='x') //admin idle timeout (range?)
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=(renumber.test(objval)&&cvr(objval,30,300)&&(objval!="0"))
}
if(type=='c') // 0(?)
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=(renumber.test(objval)&&cvr(objval,range)&&(objval!="0"))
}
if(type=='d') // 0 MTU, idle time ()
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=(renumber.test(objval)&&cvr(objval,range))
}
if(type=='y') //wan type idle timeout  (range? : Min)
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=(renumber.test(objval)&&cvr(objval,1,65521)||(objval=="0"))
}
if(type=='e') // 0
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=(renumber.test(objval))
}
if(type=='u') //User defined
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=((objval=="")||renumber.test(objval)&&cvr(objval,range))
}
if(type=='u2') //User defined ,00
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=((objval=="")||renumber2.test(objval)&&cvr(objval,range))
}
if(type=='f') //other function check
{//execute function (range)
func=range
}
if(type=='inr') //for Interval 0~xxxxx range x~xxxxx 0 by kent
{
cmsg="Value is invalid."
chkResult=((objval=="")||inte.test(objval)&&cvr(objval,range))
}
if(type=='in') //for Interval 0~xxxxx 0 by kent
{
cmsg="Value is invalid."
chkResult=((objval=="")||inte.test(objval))
}
if(range!=""&&range.indexOf("(")==-1)range="("+range
if(range!=""&&range.indexOf(")")==-1)range+=")"
if(itemname!="") cmsg=itemname+" is invalid. "+range
showmsg(obj,chkResult,cmsg)
return chkResult
}
/* check Time - chkT(this,'a')
1. 'h' => (0-23)
1. 'm' => /(0-59)
1. 's' => :(12:34)
1. 'c' => ::(1:23:45)
*/
function chkT(obj,type)
{
objval=obj.value
rehour = new RegExp("^"+pn.hour+"$")
reminute = new RegExp("^"+pn.minute+"$")
retime = new RegExp("^"+pn.time+"$")
rectime = new RegExp("^"+pn.ctime+"$")
if(type=='h') //(0-23)
{
cmsg=GetElmCont("valChk_10","Hour is invalid.")
chkResult=rehour.test(objval)
}
if(type=='m') ///(0-59)
{
cmsg=GetElmCont("valChk_11","Time is invalid.")
chkResult=reminute.test(objval)
}
if(type=='s') //:(12:34)
{
cmsg=GetElmCont("valChk_11","Time is invalid.")
chkResult=retime.test(objval)
}
if(type=='c') //::(1:23:45)
{
cmsg=GetElmCont("valChk_11","Time is invalid.")
chkResult=rectime.test(objval)
}
showmsg(obj,chkResult,cmsg)
return chkResult
}
/* check Hex - chkH(this,'a')
1. 'm' =>  MAC
1. 'w' =>  Hex (WEP key)
1. 'p' =>  Hex (Preshared Key)
*/
function chkH(obj,type)
{
objval=obj.value
rehex = new RegExp("^"+pn.hex+"$")
remac = new RegExp("^"+pn.mac+"$")
if(type=='m') // MAC
{
cmsg=GetElmCont("valChk_12","MAC address is invalid.")
chkResult=remac.test(objval)
}
if(type=='w') // Hex (WEP key)
{
cmsg=GetElmCont("valChk_13","WEP key is invalid.")
chkResult=rehex.test(objval)
}
if(type=='p') // Port range
{
cmsg=GetElmCont("valChk_14","Preshared Key is invalid.")
chkResult=rehex.test(objval)
}
chkResult=chkResult||(objval=="")
showmsg(obj,chkResult,cmsg)
return chkResult
}
function chkB(obj,type)
{
objval=obj.value
rebmac = new RegExp("^"+pn.bmac+"$")
remac = new RegExp("^"+pn.mac+"$")
if(type=='a') // MAC
{
cmsg=GetElmCont("valChk_12","MAC address is invalid.")
chkResult=rebmac.test(objval)
}
if(type=='b') // MAC , ":"
{
cmsg=GetElmCont("valChk_12","MAC address is invalid.")
chkResult=remac.test(objval)
}
if(type=='c') // MAC,00:00:00:00:00:00
{
cmsg=GetElmCont("valChk_12","MAC address is invalid.")
chkResult=rebmac.test(objval)&&(objval!="00:00:00:00:00:00")
}
if(type == 'd') //":""-"
{
cmsg=GetElmCont("valChk_12","MAC address is invalid.")
chkResult=rebmac.test(objval)||remac.test(objval)&&(objval!="00:00:00:00:00:00")
}
chkResult=chkResult||(objval=="")
showmsg(obj,chkResult,cmsg)
return chkResult
}
/* check String - chkS(this,'a')
1. 'a' => 
1. 'b' => 
1. 'e' => email 
1. 'm' =>  email  , ';' 
1. 'r' => tcp/udp  port range port  port range
1. 'n' => tcp/udp  port range port  port( "," ) port range  (eg .Special AP  incoming port)
1. 't' => SMS
1. 'sw' => special word
*/
function chkS(obj,type,seat)
{
objval=obj.value
restr = new RegExp("^"+pn.str+"$");
restrn = new RegExp("^"+pn.strn+"$");
restrname = new RegExp("^"+pn.strname+"$");
restrname2 = new RegExp("^"+pn.strname2+"$");
restrname3 = new RegExp("^"+pn.strname3+"$");
reemail = new RegExp("^"+pn.email+"$");
reemails =new RegExp("^"+pn.emailm+"$");
report =new RegExp("^"+pn.port+"$");
reportr=new RegExp("^"+pn.portrange+"$");
rehex = new RegExp("^"+pn.hex+"$");
reip=new RegExp("^"+pn.ip+"$");
reipr=new RegExp("^"+pn.iprange+"$");
reipms=new RegExp("^"+pn.ipms+"$");
reipssnm=new RegExp("^"+pn.ipnms+"$");
reipssnm1=new RegExp("^"+pn.ipnms1+"$");
reipssnm2=new RegExp("^"+pn.ipnms2+"$");
rebmac = new RegExp("^"+pn.bmac+"$");
remacs = new RegExp("^"+pn.macs+"$");
rename = new RegExp("^"+pn.name+"$");
rename3 = new RegExp("^"+pn.name3+"$");
rename4 = new RegExp("^"+pn.name4+"$");
renonwords = new RegExp("^"+pn.nonwords+"$");
restrsnmps = new RegExp("^"+pn.strsnmps+"$");
restrsnmps1 = new RegExp("^"+pn.strsnmps1+"$");
restrsnmps2 = new RegExp("^"+pn.strsnmps2+"$");
renumber = new RegExp("^"+pn.number+"$");
recolor = new RegExp(pn.hexcolor);
ipc=reip.test(objval)
var chkResult;
if(type=='a') //
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=restrn.test(objval)
}
if(type=='b') //
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=restr.test(objval)
}
if(type=='c') //Strings, can't duplicate with other fields
{
itemnames=new Array(
"Blind Call Transfer"
,"Attended Call Transfer"
,"Anonymous Call Enable"
,"Anonymous Call Disable"
,"Anonymous Call Per Call Basis"
,"DND Enable"
,"DND Disable"
,"Call Forwarding"
,"Call Return"
)
chkResult=true
for(items=0;items<9;items++)
{
cmsg=GetElmCont("none_5","This value and ")+itemnames[items]+GetElmCont("none_5"," are the same!")
ckobj=document.getElementById("it"+items)
if(ckobj==obj) continue;
if(ckobj.value==obj.value)
{
chkResult=false;
break;
}
}
}
if(type=='color') // accept Hex color or color names  e.g. #fff, #00ff00, red
{
cmsg = "Color is invalid.";
var colorNameArr = Object.keys(colorsDict);
chkResult = recolor.test(objval); // check for Hex color
if(!chkResult) { // check for color names
for(var i=0; i<colorNameArr.length; ++i) {
if(colorNameArr[i]==objval.toLowerCase()) {
chkResult = true;
break;
}
}
}
}
if(type=='d') //ssid, can not be empty
{
cmsg=GetElmCont("wpa_52","SSID can not be empty!")
chkResult=(objval!="")
}
if(type=='e') // email 
{
cmsg=GetElmCont("valChk_15","Email is invalid.")
chkResult=reemail.test(objval)
}
if(type=='f') //Find a non-word character
{
cmsg="Value is invalid."
chkResult=(renonwords.test(objval))
}
if(type=='f_1') //,,"_"
{
cmsg="Value is invalid."
chkResult=(restrsnmps.test(objval))
}
if(type=='f_2') //
{
cmsg="Value is invalid."
chkResult=(renumber.test(objval)&&cvr(objval,"1-2080768")&&(objval!="0"))
}
if(type=='f_3') //"."
{
cmsg="Value is invalid."
chkResult=(restrsnmps1.test(objval))
}
if(type=='f_4') // IP 0.0.0.0
{
cmsg=GetElmCont("valChk_1","IP address is invalid.")
chkResult=ipc&&(objval!="0.0.0.0")
}
if(type=='f_5') //IP(192.168.56.23,yahoo.com.tw) 0.0.0.0
{
cmsg=GetElmCont("valChk_1","IP address is invalid.");
chkResult=(restrsnmps2.test(objval));
}
if(type=='j') //type,42949672965
{
renumber=new RegExp("^"+pn.number+"$");
cmsg="Value is invalid.";
toint = parseInt(objval,10);
chkResult = ((renumber.test(objval))&&(toint <= 42949672965)) ;
/*alert("del chkS(j)="+((renumber.test(objval))&&(toint <= 42949672965))
+","+(renumber.test(objval))
+","+(toint <= 42949672965)
)
*/
}
if(type=='t') //SMS
{
cc=0;
cmsg=GetElmCont("valChk_20","The length of Text message is too long.")
var num_str = "";
for(p=0;p<objval.length;p++)
{
strc = objval.charCodeAt(p);
if (!(strc>=32&&strc<=126)){cc=70;break;}
else cc=160;
}
if(objval.length > cc)
{
var num = parseInt(objval.length/cc);
cc = cc*(num+1);
num_str = " ("+(num+1)+")";
}
SetElmCont("inputtextnum",objval.length+"/"+cc+num_str)
chkResult=(objval.length<=cc)
}
if(type=='i') // IP range( IP )
{
cmsg=GetElmCont("valChk_3","IP address range is invalid.")
chkResult=reip.test(objval)||reipr.test(objval)
}
if(type=='i1') // ip , ';' 
{
cmsg=GetElmCont("valChk_3","IP address range is invalid.")
chkResult=reipms.test(objval)
}
if(type=='s') // subnet , ';' 
{
cmsg=GetElmCont("valChk_2","Subnet is invalid.")
chkResult=reipssnm.test(objval)
}
if(type=='v') //IP/Subnet
{
cmsg=GetElmCont("valChk_2","Setting is invalid. (Format eg. IP/netmask.)")
chkResult=reipssnm1.test(objval)
}
if(type=='m') // email  , ';' 
{
cmsg=GetElmCont("valChk_15","Email is invalid. (Format eg. Email-A;Email-B;Email-C... Or Email-A,Email-B,Email-C...)")
chkResult=reemails.test(objval)
}
if(type=='r') //tcp/udp  port range port  port range
{
reportr.exec(objval)
cmsg=GetElmCont("valChk_6","Port is invalid.")+" (Format eg. 1-65535)"
portrary=splits(objval,"-")
chkResult=((objval.indexOf("-")!=-1)?(cvr(portrary[0],"1-65535")&&cvr(portrary[1],"1-65535")&&(parseInt(portrary[0])<parseInt(portrary[1]))):(report.test(objval)&&cvr(objval,1,65535)))
}
if(type=='n') // tcp/udp  port range port  port( "," ) port range  (eg.Special AP  incoming port)
{
cmsg=GetElmCont("valChk_6","Port is invalid.")+" (Format eg. 1-65535,777)"
portrsary=splits(objval,",")
cflag=0
for(p=0;p<portrsary.length;p++)
{
portrary=splits(portrsary[p],"-")
chkResult=((portrsary[p].indexOf("-")!=-1)?(cvr(portrary[0],"1-65535")&&cvr(portrary[1],"1-65535")&&(parseInt(portrary[0])<parseInt(portrary[1]))):(report.test(portrsary[p])&&cvr(portrsary[p],1,65535)))
if(!chkResult)cflag++
}
chkResult=(cflag==0)
}
if(type=='n1') // port( ";" )
{
cmsg=GetElmCont("valChk_6","Port is invalid.")+" (Format eg. 1-65535,777)"
portrsary=splits(objval,";")
cflag=0
for(p=0;p<portrsary.length;p++)
{
portrary=splits(portrsary[p],"-")
chkResult=(report.test(portrsary[p])&&cvr(portrsary[p],1,65535))
if(!chkResult)cflag++
}
chkResult=(cflag==0)
}
if(type=='w') // Hex (WEP key)
{//seat for hexascllkey text....liwei
if(seat){
if(typeof(seat) == "number"){ktype=obj.form.elements[locateObj(obj.form,obj)-seat].value;}
else {ktype=document.getElementById(seat).value;}
}
else ktype=obj.form.elements[locateObj(obj.form,obj)-1].value
cmsg=GetElmCont("valChk_13","WEP key is invalid.")+"  \n "+((ktype==0)?GetElmCont("valChk_16","Please input 10 or 26 hexadecimal digits in WEP Key field!!"):GetElmCont("valChk_17","Please input 5 or 13 ASCII characters in WEP Key field!!"))
wkeylen=obj.value.length
chkResult=(objval!="")
&&(((ktype=="0")&&(clen(objval,10)||clen(objval,26))&&rehex.test(objval))
||((ktype=="1")&&(clen(objval, 5)||clen(objval,13))&&restr.test(objval)))
}
if(type=='p') // Hex (Preshared Key)
{
cmsg="Preshared Key is invalid."
pkeylen=obj.value.length
if((pkeylen>64)||(pkeylen<8))
{
cmsg="Please input 64 hexadecimal digits or 8~63 ASCII character in Preshared Key field!!"
chkResult=false; //fix ......liwei
}
else if(pkeylen==64)
{
cmsg="Please input 64 hexadecimal digits in Preshared Key field!!"
chkResult=rehex.test(objval)
}
else  //ASCII
{
chkResult=(objval!="")
}
}
if(type=='x') //vnp Preshared Key 2  256
{
cmsg="Preshared Key is invalid."
pkeylen=obj.value.length
if(pkeylen < 2)
{
cmsg = "Preshared Key 2~256";
chkResult = false;
}
else
{
chkResult = (objval!="");
}
}
if(type=='x1') //user
{
cmsg=GetElmCont("valChk_5","Value is invalid.The value length is"+seat)
if(seat.search("-")==-1) chkResult=!(objval.length < seat);
else
{
var arr = seat.split("-");
chkResult=!(objval.length < arr[0] || objval.length > arr[1]);
}
}
if(type=='g') // MAC
{
cmsg=GetElmCont("valChk_12","MAC address is invalid.")
chkResult=rebmac.test(objval)
}
if(type=='g1') // MAC  , ';' 
{
cmsg=GetElmCont("valChk_12","MAC address is invalid.")
chkResult=remacs.test(objval)
}
if(type=='h') //0001~FFFF for IPv6 Address Range
{
cmsg="IPv6 Address Range is invalid."
chkResult=(rehex.test(objval))&&(objval!="0")&&(objval!="00")&&(objval!="000")&&(objval!="0000")
}
if(type=='sw') //
{
cmsg="Only A~Z a~z 0~9 - and _."
chkResult=(rename.test(objval));
}
if(type=='sw1') // + Only number
{
cmsg="Only A~Z a~z 0~9 - and _"
renumber=new RegExp("^"+pn.number+"$");
toint = parseInt(objval,10);
/*alert("del !renumber="+(!renumber.test(objval))
+","+(rename.test(objval))
+","+((rename.test(objval))&&(!renumber.test(objval)))
)*/
chkResult=((rename.test(objval))&&(!renumber.test(objval)));
}
if(type=='sw2') // + 
{
cmsg="Only A~Z a~z 0~9 - and _."
chkResult=((rename.test(objval)) && (restrn.test(objval)));
}
if(type=='sw3') //(\ / : * ? " < > |) + 
{
cmsg="Beside '\','/',':','*','?','\','<','>','|'"
chkResult=restrname.test(objval);
}
if(type=='sw4') //(+ - * / ^ ( )) + 
{
cmsg="Only '+','-','*','/','^','(',')','numbers'"
chkResult=restrname2.test(objval);
}
if(type=='sw5') //
{
cmsg="Only A~Z a~z 0~9 and -"
chkResult=(rename3.test(objval));
}
if(type=='sw6') //(-,_,/) + 
{
cmsg="Only A~Z a~z 0~9 - and _ and /"
chkResult=((rename4.test(objval)) && (restrn.test(objval)));
}
if(type=='k') // pppoe  ()<>\/"'
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=restrname3.test(objval)
}
if(type=='q') //  Hex (0~9 a-f A-F)
{
cmsg=GetElmCont("valChk_5","Value is invalid.")
chkResult=rehex.test(objval)
}
if((type!='w')&&(type!='p')&&(type!='h'))
chkResult=chkResult||(objval=="")
showmsg(obj,chkResult,cmsg)
return chkResult
}
function splits(strs,splitby)
{
if(strs.indexOf(splitby)==-1)
ary=new Array(strs)
else ary=strs.split(splitby)
return ary
}
