function LM() { // show left menu
var ck_loginL = (typeof(left_menu_login) != "undefined");
if(ck_loginL && left_menu_login && !login) {
LM_loginL();
return;
}
LP=setLMtp();
LS=setLMts();
dw("<TABLE BORDER=0 WIDTH=200 HEIGHT='100%' CELLPADDING=0 CELLSPACING=0 align=center>")
dw("<TR><TD HEIGHT=10 colspan=3></TD></TR>")
for (l=0;l<TopMenus.length;l++){
var found_first_L2 = false; // use this flag to record whether found the first L2 button or not
sm=SecMenus[l];
sms=sm.split("~");
smu=SecMenulk[l];
smus=smu.split("~");
sml2=TAID_L2[l];         //L2
sml2s=sml2.split("~");   //L2 TAID
for (k=0;k<sms.length;k++){
if(sms[k]=="") sms.splice(k,1);
if(smus[k]=="") smus.splice(k,1);
}
var L2_button_str = "";
for(i=0;i<sms.length;i++){
showl2=(sms[i].indexOf("remove_this")!=-1)? "style='display:none'" : ""
c_link=smus[i]
L2ID=sml2s[i] //L2 TAID
if(c_link.indexOf("remove_this")!=-1){ //L3comporeomve, L2link
smv=SecMenuv[l];
try{    //L2smvs=smv.split("|")try
smvs=smv.split("|");
smvss=smvs[i].split(",")
for(a=2;a<smvss.length;a++){
if(eval(smvss[a]+"[1]").indexOf("remove_this")==-1){ //L2link
c_link=eval(smvss[a]+"[1]");
c_link=c_link.substring(0,c_link.length-1) //remove "~"
break;
}
}
}catch(e){}
}
if(setTopMenus(TopMenus[l])=="")
{
var L3_name=ThreeMenus[l].split("|");
var L3_name_show;
var str;
var L3_num=0;
str=L3_name[i];
str=str.substring(0, str.length-1);
L3_name_show=str.split("~");
for(i1=0;i1<L3_name_show.length;i1++)
{
if(L3_name_show[i1].indexOf("remove_this")==-1)
{L3_num=L3_num+1;}
}
}
if(l==0) sloc="top.frames['main'].location" //wizardmGURLGURL....liwei
else sloc="location"
var L2_link = clk(c_link); // L2 url link
var L2_click_event="ONCLICK=\"display_L3_menu('L3_e_"+l+"_"+i+"')";
if(L3_num==1)
{L2_click_event="ONCLICK=\""+sloc+"='" + L2_link +"'";}
/*
