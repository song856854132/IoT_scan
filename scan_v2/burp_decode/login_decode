var isIE = (function(){
var undef,
v = 3,
div = document.createElement('div'),
all = div.getElementsByTagName('i');
while (
div.innerHTML = '<!--[if gt IE ' + (++v) + ']><i></i><![endif]-->',
all[0]
);
return v > 4 ? v : undef;
}());
function getCookie(cname) {
var name = cname + "=";
var ca = document.cookie.split(';');
for(var i=0; i<ca.length; i++) {
var c = ca[i];
while (c.charAt(0)==' ') c = c.substring(1);
if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
}
return "";
}
function get_csrf_token(){
var csrf_cookie_name = "CSRFTOKEN";
return getCookie(csrf_cookie_name);
}
function append_url(url) { // we can modify this function to append paramters to url
url += (url.indexOf("?")==-1) ? "?" : "&";
var t = new Date();
return url+'ZT='+t.getTime()+'&csrftok='+get_csrf_token();
}
function GURL(x) {
window.location = append_url(x);
}
function sGURL(x) { // We can use this function to send ALTER or modify CSIDs in secret.
top.Hide.location = append_url(x);
}
function mGURL2(x) {
if(top.frames['main']!=null)top.frames['main'].location = append_url(x);
else location = append_url(x);
}
function mGURL(x) {
if(top.frames['main']!=null) top.frames['main'].location = append_url(x);
else top.location = append_url(x);
}
function makesure(p,l,k) {
if (confirm(p)) GURL(l);
else if(typeof(k)!="undefined") eval(k);
}
function dw(str){document.write(str+"\n")}
function SetCkBox(chkbox,ckv,nckv,chkid,hids,hide) //chkid=\`}(checkbox)
{
f=chkbox.form
for(cb=0;cb<f.length;cb++){if(f.elements[cb]==chkbox)break;}
f.elements[cb-1].value=(f.elements[cb].checked)?ckv:nckv
if(arguments.length==4){ //`}~|do "prim_switch" function
prim_switch(chkid);
}else if(arguments.length==6){
switch_hidden(chkid,hids,hide);}
}
function SetSelHidval(selectobjs)
{
if(typeof(selectobjs)=="object") { f=selectobjs.form }
else { f=document.forms[0]; }
for(cb=0;cb<f.length;cb++){if(f.elements[cb]==selectobjs)break;}
if (!window.ActiveXObject)
{
f.elements[cb-1].style.display="none"
f.elements[cb-1].type="text"
}
f.elements[cb-1].value=(typeof(selectobjs)=="object")? selectobjs.value : selectobjs
}
