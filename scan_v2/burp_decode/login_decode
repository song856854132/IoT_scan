is_table_4_open=0;
}
else if(swm_POEStatus==false || swm_poe==false)
{
document.getElementById("table_4").style.display="none";
document.getElementById("table_on_off_4").style.display="none";
menu_enable_number=menu_enable_number-1;
device_dashboard_on_off = device_dashboard_on_off.substring(0,3) + "0" + device_dashboard_on_off.substring(4);
is_table_4_open=0;
}
localStorage.setItem("device_dashboard_on_off", device_dashboard_on_off);
menu_line.style.height=28*menu_enable_number;
is_open_menu=0;
for (i = 0; i < device_dashboard_on_off.length; i++)
{
if(i==1 &&  is_table_2_open==0)
{}
else if(i==3 &&  is_table_4_open==0)
{}
else
{
if(device_dashboard_on_off[i]=="0")
{is_open_menu=1;}
}
}
if(is_open_menu==1)
{
menu.style.display="";
menu_line.style.display="";
}
}
