}
//===================	ASReader.htm	======================================================^^^^^^^^===============
//===================	ASReader.htm	=========================================ModeReaderChange====================
//	idx = 1-16;
//	isele = 1:A, 2:B, 3:C, 4:D, 5:No function ;
//	inx	= 0:door, 1:Parking, 4:Elevator, 9:no function
function SelectFuncReader(idx,isele,ina,inb,inc,ind)
{
	var s;
	var v = new Array(' ',' ',' ',' ',' ');

	if(isele == 1)			v[1]=' selected';	else if(isele == 2)		v[2]=' selected';	else if(isele == 3)		v[3]=' selected';
	else 					v[4]=' selected';

	s='<nobr><select id=FuncReader'+idx+' name=FuncReader'+idx+' size=1>';

	if(ina == 0)
		s+='<option value=1'+v[1]+'>Door 1';
	else if(ina == 1)
		s+='<option value=1'+v[1]+'>Gate 1';
	else if(ina == 4)
		s+='<option value=1'+v[1]+'>Relay 1';

	if(inb == 0)
		s+='<option value=2'+v[2]+'>Door 2';
	else if(inb == 1)
		s+='<option value=2'+v[2]+'>Gate 2';
	else if(inb == 4)
		s+='<option value=2'+v[2]+'>Relay 2';

	if(inc == 0)
		s+='<option value=3'+v[3]+'>Door 3';
	else if(inc == 1)
		s+='<option value=3'+v[3]+'>Gate 3';
	else if(inc == 4)
		s+='<option value=3'+v[3]+'>Relay 3';

	if(ind == 0)
		s+='<option value=4'+v[4]+'>Door 4';
	else if(ind == 1)
		s+='<option value=4'+v[4]+'>Gate 4';
	else if(ind == 4)
		s+='<option value=4'+v[4]+'>Relay 4';

	s+='</select></nobr>';
    document.write(s);
}
