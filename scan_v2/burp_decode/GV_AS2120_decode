    else if(time == 40) v40 = ' selected';  else if(time == 41) v41 = ' selected';  else if(time == 42) v42 = ' selected';
    else if(time == 43) v43 = ' selected';  else if(time == 44) v44 = ' selected';  else if(time == 45) v45 = ' selected';
