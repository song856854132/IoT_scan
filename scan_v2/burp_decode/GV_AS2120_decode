function ip_to_number(addr) {
    var ip = addr.match(/^(\d+)\.(\d+)\.(\d+)\.(\d+)$/);
    if(ip) {
        return (+ip[1]<<24) + (+ip[2]<<16) + (+ip[3]<<8) + (+ip[4]);
    }

    return null;
}

function Input_CheckIfValidate(lan_ip, lan_sub, wl_ip, wl_sub)
{
    if ((ip_to_number(lan_ip) & ip_to_number(lan_sub)) == (ip_to_number(wl_ip) & ip_to_number(wl_sub))) {
        if (lan_sub != wl_sub) {
            alert("Same domain with different subnet mask!");
            return false;
        }
    }
    return true;
}

function Input_CheckIp(szObjId)
{
    var iIindex = 0;
    var iStart = 0, iEnd = 0;
    var szIpAddr,szTmp,iTmp;
    var ObjIp = document.getElementById(szObjId);
    
    szIpAddr = ObjIp.value;
    
    for(iIindex=0;iIindex<4;iIindex++) {
        if(iIindex<3) {
            iEnd = szIpAddr.indexOf(".",iStart);
            if(iEnd == -1) {
                alert(GetXML("IDS_VS_Chk_Invalid_IP"));
                ObjIp.value="";
                return false;
            }
            szTmp = szIpAddr.substring(iStart,iEnd);
        }
        else {
            szTmp = szIpAddr.substring(iStart,szIpAddr.length);
        }
        
        iTmp = szTmp.valueOf();
        if(iTmp<0 || iTmp>255) {
            alert(GetXML("IDS_VS_Chk_Invalid_IP"));
            ObjIp.value="";
            return false;
        } 
        iStart = iEnd+1;
    }
    
    return true;
}

function Input_CheckIpMsk(szObjId)
{
    var szIpAddr;
    var splitString,szIpMsk,szMsk;
    var ObjIp = document.getElementById(szObjId);
    szIpMsk = ObjIp.value;
    if(szIpMsk.indexOf("/") == -1)
        szIpAddr = szIpMsk;    
    else {
        splitString = szIpMsk.split("/");
        szIpAddr = splitString[0];
           szMsk = splitString[1];
    }

    //check ipv4
    var ipv4=/^((25[0-5]|2[0-4]\d|[01]?\d\d?)\.){3}(25[0-5]|2[0-4]\d|[01]?\d\d?)$/;
    if(ipv4.test(szIpAddr))
    {
      if(szIpMsk.indexOf("/") != -1) {
        if( szMsk < 1 || szMsk > 32 || isNaN(szMsk)) {
            alert(GetXML("IDS_VS_Chk_Invalid_NM"));
            return false;
        }
      }
      return true;
    }

    //check ipv6
    if (/^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$|^(([a-zA-Z]|[a-zA-Z][a-zA-Z0-9\-]*[a-zA-Z0-9])\.)*([A-Za-z]|[A-Za-z][A-Za-z0-9\-]*[A-Za-z0-9])$|^\s*((([0-9A-Fa-f]{1,4}:){7}([0-9A-Fa-f]{1,4}|:))|(([0-9A-Fa-f]{1,4}:){6}(:[0-9A-Fa-f]{1,4}|((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){5}(((:[0-9A-Fa-f]{1,4}){1,2})|:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){4}(((:[0-9A-Fa-f]{1,4}){1,3})|((:[0-9A-Fa-f]{1,4})?:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){3}(((:[0-9A-Fa-f]{1,4}){1,4})|((:[0-9A-Fa-f]{1,4}){0,2}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){2}(((:[0-9A-Fa-f]{1,4}){1,5})|((:[0-9A-Fa-f]{1,4}){0,3}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){1}(((:[0-9A-Fa-f]{1,4}){1,6})|((:[0-9A-Fa-f]{1,4}){0,4}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(:(((:[0-9A-Fa-f]{1,4}){1,7})|((:[0-9A-Fa-f]{1,4}){0,5}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:)))(%.+)?\s*$/.test(szIpAddr))
    {
      if(szIpMsk.indexOf("/") != -1) {
        if( szMsk < 1 || szMsk > 32 || isNaN(szMsk)) {
            alert(GetXML("IDS_VS_Chk_Invalid_NM"));
            return false;
        }
      }
      return true;
    }

    alert(GetXML("IDS_VS_Chk_Invalid_IP"));
    return false;
}

