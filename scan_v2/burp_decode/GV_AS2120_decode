<script language=JavaScript>
var va_machname = 'ASController';
var va_IpAddress = '192.168.50.200';
var va_SubnetMask = '255.255.255.0';
var va_DefaultGateway = '192.168.50.1';
var va_DNServer = '192.168.50.1';
var va_EnableDHCP = 0;
var va_SendtoServer = 2;
var va_ServerAddress = '192.168.0.10';
var va_MyName = 'user';
var va_ddns = 3;
var va_HostName = 'user.gvdip.com';
var va_UserName = '';
var va_DDNSPasswd = '';
var va_ConnectStatus = '';
</script>
<script language=JavaScript>
function Chklimit()
{
	if(va_ddns == 1|| va_ddns == 3)	
		document.CF.Txt_HostName.disabled = true;
	else
		document.CF.Txt_HostName.disabled = false;
}
function GeoReg()
{
	window.open("http://ns.gvdip.com/register.aspx","register");	
}
function byDDNS()
{
	if(document.CF.DDNS.value == 2)
	{
        document.CF.Txt_HostName.value = "";
	}
    else if(document.CF.DDNS.value == 1)
	{
	    document.CF.Txt_HostName.value = va_UserName+".dipmap.com" ;
	}
    else if(document.CF.DDNS.value == 3)
    {
	    document.CF.Txt_HostName.value = va_UserName+".gvdip.com";
    }
    document.CF.Txt_HostName.disabled = true;
}

function SubmitCheck()
{
	if(document.CF.IP_IpAddress[0].value > 255 || document.CF.IP_IpAddress[1].value > 255 || document.CF.IP_IpAddress[2].value > 255 || document.CF.IP_IpAddress[3].value > 255)
	{
		alert('IP Address value error!\nPlease input again');
		return false;
	}
	if(document.CF.IP_SubnetMask[0].value > 255 || document.CF.IP_SubnetMask[1].value > 255 || document.CF.IP_SubnetMask[2].value > 255 || document.CF.IP_SubnetMask[3].value > 255)
	{
		alert('Subnet Mask value error!\nPlease input again');
		return false;
	}
	if(document.CF.IP_DefaultGateway[0].value > 255 || document.CF.IP_DefaultGateway[1].value > 255 || document.CF.IP_DefaultGateway[2].value > 255 || document.CF.IP_DefaultGateway[3].value > 255)
	{
		alert('Default Gateway value error!\nPlease input again');
		return false;
	}
	if(document.CF.IP_ServerAddress[0].value > 255 || document.CF.IP_ServerAddress[1].value > 255 || document.CF.IP_ServerAddress[2].value > 255 || document.CF.IP_ServerAddress[3].value > 255)
	{
		alert('Server IP value error!\nPlease input again');
		return false;
	}
	document.CF.Txt_HostName.disabled = false;
	return true;
}
function ChkDDNSMode(va,ddnsmode)
{
	if(va == 1)					//	Not Send
	{
		if(ddnsmode == 2)
			document.write('<input type=radio name=WR_SendtoServer value=2 checked>Disable');
		else					
			document.write('<input type=radio name=WR_SendtoServer value=2 >Disable');
	}
	else if(va == 2)			//	LocalDDNS
	{
		if(ddnsmode == 1)
			document.write('<input type=radio name=WR_SendtoServer value=1 checked>Enable&nbsp;LocalDDNS');
		else
			document.write('<input type=radio name=WR_SendtoServer value=1 >Enable&nbsp;LocalDDNS');
	}
	else						//	DDNS
	{
		if(ddnsmode == 0)
			document.write('<input type=radio name=WR_SendtoServer value=0 checked>Enable&nbsp;DDNS');
		else
			document.write('<input type=radio name=WR_SendtoServer value=0 >Enable&nbsp;DDNS');
	}
}
</script>
</head>

<body bgcolor=#ffffff>
<p class="type1" >Network Configuration</p>
