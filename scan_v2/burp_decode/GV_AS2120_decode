function Draw3DES(intype, sValue)
{
	var s;
	if(intype == 1)
		s='<input type=password name=Txt_3Des1 value="'+sValue+'" size=30 maxlength=8 class='+sClassInputText+' onClick="desZero(1);">';
	else if(intype == 2)
		s='<input type=password name=Txt_3Des2 value="'+sValue+'" size=30 maxlength=8 class='+sClassInputText+' onClick="desZero(2);">';
	else
		s='<input type=password name=Txt_3Des3 value="'+sValue+'" size=30 maxlength=8 class='+sClassInputText+' onClick="desZero(3);">';
	document.write(s);
}

