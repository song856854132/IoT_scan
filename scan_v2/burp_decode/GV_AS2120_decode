		v2 = ' selected';

	s='<nobr>';
	s+='<select name=Anti size=1><option value=0'+v1+'>Disable<option value=1'+v2+'>Enable</select></nobr>'
    document.write(s);
}

function DrawIpBox(sFormName, sIpName, sValue)
{
	var i,s,v;
	v=sValue.split('.',4);
	for(i=0;i<4;i++)
	{
		if(i<v.length) v[i]=TrimString(v[i]).substr(0,3);
		else v[i]='';
	}
	s='<nobr>';
	for(i=0;i<4;i++)
	{
		s+='<input name=IP_'+sIpName+' value="'+v[i]+'" type=text size=3 maxlength=3 class='+sClassInputText+' onkeypress="return IpKeyPress(\''+sFormName+'\', \''+sIpName+'\', '+i+', event);" onkeydown="return IpKeyDown(\''+sFormName+'\', \''+sIpName+'\', '+i+', event);" onkeyup="return IpKeyUp(\''+sFormName+'\', \''+sIpName+'\', '+i+', event);" onfocus="this.select();" onblur="return IpBlur(this);">';
		if(i!=3) s+='.';
	}
	s+='</nobr>';
	document.write(s);
}

