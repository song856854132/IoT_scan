function Hidden_OutputPart(value)
{
	var s;
	s = ' ';
	s='<input name=OutputPart value="'+value+'"type=hidden size=5 maxlength=3 class='+sClassInputText+' >';
	document.write(s);
}
//===================	ASOutput.htm	=============================================================================
function InterConfig()
{
	var a,b,i;

	for(i=1;i<=16;i++){
		a = document.getElementById('InterOutMode'+i);
		if(a.selectedIndex==0){															//	No Function
			if(i<=9){
				b = document.getElementById('test10'+i);	b.style.display = "none";
				b = document.getElementById('test20'+i);	b.style.display = "none";
				b = document.getElementById('test30'+i);	b.style.display = "none";
				b = document.getElementById('Fu10'+i);		b.style.display = "none";
				b = document.getElementById('Fu20'+i);		b.style.display = "none";
				b = document.getElementById('Ev10'+i);		b.style.display = "none";
				b = document.getElementById('Ev20'+i);		b.style.display = "none";
				b = document.getElementById('Logic10'+i);	b.style.display = "none";
				b = document.getElementById('Logic20'+i);	b.style.display = "none";
				b = document.getElementById('Logic30'+i);	b.style.display = "none";
				b = document.getElementById('ExdIn10'+i);	b.style.display = "none";
				b = document.getElementById('ExdIn20'+i);	b.style.display = "none";
				b = document.getElementById('Reset0'+i);	b.style.display = "none";
			}
			else{
				b = document.getElementById('test1'+i);		b.style.display = "none";
				b = document.getElementById('test2'+i);		b.style.display = "none";
				b = document.getElementById('test3'+i);		b.style.display = "none";
				b = document.getElementById('Fu1'+i);		b.style.display = "none";
				b = document.getElementById('Fu2'+i);		b.style.display = "none";
				b = document.getElementById('Ev1'+i);		b.style.display = "none";
				b = document.getElementById('Ev2'+i);		b.style.display = "none";
				b = document.getElementById('Logic1'+i);	b.style.display = "none";
				b = document.getElementById('Logic2'+i);	b.style.display = "none";
				b = document.getElementById('Logic3'+i);	b.style.display = "none";
				b = document.getElementById('ExdIn1'+i);	b.style.display = "none";
				b = document.getElementById('ExdIn2'+i);	b.style.display = "none";
				b = document.getElementById('Reset'+i);		b.style.display = "none";
			}
		}
		else if((a.selectedIndex>=1) && (a.selectedIndex<=5)){							//	Door/Gate/Relay 1..4 ,Index=5 is All Door
			if(i<=9){
				b = document.getElementById('test10'+i);	b.style.display = "none";
				b = document.getElementById('test30'+i);	b.style.display = "none";
				b = document.getElementById('Fu10'+i);		b.style.display = "none";
				b = document.getElementById('Fu20'+i);		b.style.display = "none";
				b = document.getElementById('Ev10'+i);		b.style.display = "none";
				b = document.getElementById('Ev20'+i);		b.style.display = "none";
				b = document.getElementById('Logic10'+i);	b.style.display = "none";
				b = document.getElementById('Logic20'+i);	b.style.display = "none";
				b = document.getElementById('Logic30'+i);	b.style.display = "none";
				b = document.getElementById('ExdIn10'+i);	b.style.display = "none";
				b = document.getElementById('ExdIn20'+i);	b.style.display = "none";
				b = document.getElementById('Reset0'+i);	b.style.display = "none";
			}
			else{
				b = document.getElementById('test1'+i);		b.style.display = "none";
				b = document.getElementById('test3'+i);		b.style.display = "none";
				b = document.getElementById('Fu1'+i);		b.style.display = "none";
				b = document.getElementById('Fu2'+i);		b.style.display = "none";
				b = document.getElementById('Ev1'+i);		b.style.display = "none";
				b = document.getElementById('Ev2'+i);		b.style.display = "none";
				b = document.getElementById('Logic1'+i);	b.style.display = "none";
				b = document.getElementById('Logic2'+i);	b.style.display = "none";
				b = document.getElementById('Logic3'+i);	b.style.display = "none";
				b = document.getElementById('ExdIn1'+i);	b.style.display = "none";
				b = document.getElementById('ExdIn2'+i);	b.style.display = "none";
				b = document.getElementById('Reset'+i);		b.style.display = "none";
			}
		}
		else if(a.selectedIndex>=6){				//	Normal, Toggle, Pulse Mode
			if(i<=9){
				b = document.getElementById('test20'+i);	b.style.display = "none";
			}
			else{
				b = document.getElementById('test2'+i);		b.style.display = "none";
			}
			if(a.selectedIndex != 8){
				if(i<=9){
					b = document.getElementById('test10'+i);	b.style.display = "none";
				}
				else{
					b = document.getElementById('test1'+i);		b.style.display = "none";
				}
			}
		}
	}
}
//===================	ASOutput.htm	======================================================^^^^^^^^===============
//===================	ASOutput.htm	=============================================================================
function ChkInterOutMode(idx,inx)
{
	var a;
	a = document.getElementById('InterOutMode'+idx);

	if(a.selectedIndex == 0){
		if(idx<=9){
			b = document.getElementById('test10'+idx);	b.style.display = "none";
			b = document.getElementById('test20'+idx);	b.style.display = "none";
			b = document.getElementById('test30'+idx);	b.style.display = "none";
			b = document.getElementById('Fu10'+idx);	b.style.display = "none";
			b = document.getElementById('Fu20'+idx);	b.style.display = "none";
			b = document.getElementById('Ev10'+idx);	b.style.display = "none";
			b = document.getElementById('Ev20'+idx);	b.style.display = "none";
			b = document.getElementById('Logic10'+idx);	b.style.display = "none";
			b = document.getElementById('Logic20'+idx);	b.style.display = "none";
			b = document.getElementById('Logic30'+idx);	b.style.display = "none";
			b = document.getElementById('ExdIn10'+idx);	b.style.display = "none";
			b = document.getElementById('ExdIn20'+idx);	b.style.display = "none";
			b = document.getElementById('Reset0'+idx);	b.style.display = "none";
		}
		else{
			b = document.getElementById('test1'+idx);	b.style.display = "none";
			b = document.getElementById('test2'+idx);	b.style.display = "none";
			b = document.getElementById('test3'+idx);	b.style.display = "none";
			b = document.getElementById('Fu1'+idx);		b.style.display = "none";
			b = document.getElementById('Fu2'+idx);		b.style.display = "none";
			b = document.getElementById('Ev1'+idx);		b.style.display = "none";
			b = document.getElementById('Ev2'+idx);		b.style.display = "none";
			b = document.getElementById('Logic1'+idx);	b.style.display = "none";
			b = document.getElementById('Logic2'+idx);	b.style.display = "none";
			b = document.getElementById('Logic3'+idx);	b.style.display = "none";
			b = document.getElementById('ExdIn1'+idx);	b.style.display = "none";
			b = document.getElementById('ExdIn2'+idx);	b.style.display = "none";
			b = document.getElementById('Reset'+idx);	b.style.display = "none";
		}
	}
	else if((a.selectedIndex>=1) && (a.selectedIndex<=5)){
		if(idx<=9){
			b = document.getElementById('test10'+idx);	b.style.display = "none";
			b = document.getElementById('test30'+idx);	b.style.display = "none";
			b = document.getElementById('Fu10'+idx);	b.style.display = "none";
			b = document.getElementById('Fu20'+idx);	b.style.display = "none";
			b = document.getElementById('Ev10'+idx);	b.style.display = "none";
			b = document.getElementById('Ev20'+idx);	b.style.display = "none";
			b = document.getElementById('Logic10'+idx);	b.style.display = "none";
			b = document.getElementById('Logic20'+idx);	b.style.display = "none";
			b = document.getElementById('Logic30'+idx);	b.style.display = "none";
			b = document.getElementById('ExdIn10'+idx);	b.style.display = "none";
			b = document.getElementById('ExdIn20'+idx);	b.style.display = "none";
			b = document.getElementById('test20'+idx);	b.style.display = "inline";
			b = document.getElementById('Reset0'+idx);	b.style.display = "none";
		}
		else{
			b = document.getElementById('test1'+idx);	b.style.display = "none";
			b = document.getElementById('test3'+idx);	b.style.display = "none";
			b = document.getElementById('Fu1'+idx);		b.style.display = "none";
			b = document.getElementById('Fu2'+idx);		b.style.display = "none";
			b = document.getElementById('Ev1'+idx);		b.style.display = "none";
			b = document.getElementById('Ev2'+idx);		b.style.display = "none";
			b = document.getElementById('Logic1'+idx);	b.style.display = "none";
			b = document.getElementById('Logic2'+idx);	b.style.display = "none";
			b = document.getElementById('Logic3'+idx);	b.style.display = "none";
			b = document.getElementById('ExdIn1'+idx);	b.style.display = "none";
			b = document.getElementById('ExdIn2'+idx);	b.style.display = "none";
			b = document.getElementById('test2'+idx);	b.style.display = "inline";
			b = document.getElementById('Reset'+idx);	b.style.display = "none";
		}

		var NewSe01=document.createElement('option');
		var NewSe02=document.createElement('option');
		var NewSe03=document.createElement('option');
		var NewSe04=document.createElement('option');
		var NewSe05=document.createElement('option');
		var NewSe06=document.createElement('option');
		var NewSe07=document.createElement('option');
		var NewSe08=document.createElement('option');
		var NewSe09=document.createElement('option');
		var NewSe10=document.createElement('option');
		
		var c=document.getElementById('EventOutput'+idx);
		for(i=c.length-1;i>=0;i--)
			c.remove(i);
		if(a.selectedIndex<5){	//About Door1..4 Function
			NewSe01.text = "Electric Lock";		NewSe01.value = 1;		c.add(NewSe01);
			NewSe02.text = "Event Alarm";		NewSe02.value = 2;		c.add(NewSe02);
			NewSe03.text = "Alarm Keyswitch";	NewSe03.value = 3;		c.add(NewSe03);
			NewSe04.text = "Entry Card";		NewSe04.value = 4;		c.add(NewSe04);
			NewSe05.text = "Exit Card";			NewSe05.value = 5;		c.add(NewSe05);
			if(vafu[a.selectedIndex-1]==0){						//	Door
				NewSe06.text = "Unlock Alarm";	NewSe06.value = 6;		c.add(NewSe06);
			}
			else if(vafu[a.selectedIndex-1]==1){				//	Parking
				NewSe06.text = "Open Alarm";	NewSe06.value = 6;		c.add(NewSe06);
			}
			else{												//	Elevator
			}
			/*NewSe07.text = "Beeper";			NewSe07.value = 7;		c.add(NewSe07);
			NewSe08.text = "Green LED";			NewSe08.value = 8;		c.add(NewSe08);
			NewSe09.text = "Red LED";			NewSe09.value = 9;		c.add(NewSe09);
			NewSe10.text = "Invert Red LED";	NewSe10.value = 10;		c.add(NewSe10);*/
		}else{
			NewSe01.text = "Area Occupied";		NewSe01.value = 1;		c.add(NewSe01);
		}
	}
	else if(a.selectedIndex>=6){			//	Normal, Toggle, Pulse Mode
		if(idx<=9){
			b = document.getElementById('test10'+idx);	b.style.display = "block";
			b = document.getElementById('test20'+idx);	b.style.display = "none";
			b = document.getElementById('test30'+idx);	b.style.display = "block";
			b = document.getElementById('Fu10'+idx);	b.style.display = "block";
			b = document.getElementById('Fu20'+idx);	b.style.display = "block";
			b = document.getElementById('Ev10'+idx);	b.style.display = "block";
			b = document.getElementById('Ev20'+idx);	b.style.display = "block";
			b = document.getElementById('Logic10'+idx);	b.style.display = "block";
			b = document.getElementById('Logic20'+idx);	b.style.display = "block";
			b = document.getElementById('Logic30'+idx);	b.style.display = "block";
			b = document.getElementById('ExdIn10'+idx);	b.style.display = "block";
			b = document.getElementById('ExdIn20'+idx);	b.style.display = "block";
			b = document.getElementById('Reset0'+idx);	b.style.display = "block";
		}
		else{
			b = document.getElementById('test1'+idx);	b.style.display = "block";
			b = document.getElementById('test2'+idx);	b.style.display = "none";
			b = document.getElementById('test3'+idx);	b.style.display = "block";
			b = document.getElementById('Fu1'+idx);		b.style.display = "block";
			b = document.getElementById('Fu2'+idx);		b.style.display = "block";
			b = document.getElementById('Ev1'+idx);		b.style.display = "block";
			b = document.getElementById('Ev2'+idx);		b.style.display = "block";
			b = document.getElementById('Logic1'+idx);	b.style.display = "block";
			b = document.getElementById('Logic2'+idx);	b.style.display = "block";
			b = document.getElementById('Logic3'+idx);	b.style.display = "block";
			b = document.getElementById('ExdIn1'+idx);	b.style.display = "block";
			b = document.getElementById('ExdIn2'+idx);	b.style.display = "block";
			b = document.getElementById('Reset'+idx);	b.style.display = "block";
		}
		if(a.selectedIndex != 8){
			if(idx<=9){
				b = document.getElementById('test10'+idx);	b.style.display = "none";
			}
			else{
				b = document.getElementById('test1'+idx);	b.style.display = "none";
			}
		}
	}
}
//===================	ASOutput.htm	======================================================^^^^^^^^===============
//===================	ASOutput.htm	==========================================ChkInterOutMode====================
function InterOutMode(idx,ivalue,inx)
{
	var s;
	var v = new Array(' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ');

	if(ivalue == 1)			v[1]=' selected';	else if(ivalue == 2)	v[2]=' selected';	else if(ivalue == 3)	v[3]=' selected';
	else if(ivalue == 4)	v[4]=' selected';	else if(ivalue == 5)	v[5]=' selected';	else if(ivalue == 6)	v[6]=' selected';
	else if(ivalue == 7)	v[7]=' selected';	else if(ivalue == 8)	v[8]=' selected';	else if(ivalue == 9)	v[9]=' selected';
	else if(ivalue == 10)	v[10]=' selected';	else 					v[0]=' selected';

	s='<nobr><select style="font-size:10pt" id=InterOutMode'+idx+' name=InterOutMode'+idx+' size=1 onchange="return ChkInterOutMode('+idx+','+inx+')">';
	s+='<option value=0'+v[0]+'> No Function';
	if(inx[0] == 0)
		s+='<option value=1'+v[1]+'>Door 1';
	else if(inx[0] == 1)
		s+='<option value=1'+v[1]+'>Gate 1';
	else if(inx[0] == 4)
		s+='<option value=1'+v[1]+'>Relay 1';

	if(inx[1] == 0)
		s+='<option value=2'+v[2]+'>Door 2';
	else if(inx[1] == 1)
		s+='<option value=2'+v[2]+'>Gate 2';
	else if(inx[1] == 4)
		s+='<option value=2'+v[2]+'>Relay 2';

	if(inx[2] == 0)
		s+='<option value=3'+v[3]+'>Door 3';
	else if(inx[2] == 1)
		s+='<option value=3'+v[3]+'>Gate 3';
	else if(inx[2] == 4)
		s+='<option value=3'+v[3]+'>Relay 3';

	if(inx[3] == 0)
		s+='<option value=4'+v[4]+'>Door 4';
	else if(inx[3] == 1)
		s+='<option value=4'+v[4]+'>Gate 4';
	else if(inx[3] == 4)
		s+='<option value=4'+v[4]+'>Relay 4';

	s+='<option value=5'+v[5]+'> All Zone';	
	s+='<option value=6'+v[6]+'> Normal';
	s+='<option value=7'+v[7]+'> Toggle';
	s+='<option value=8'+v[8]+'> Pulse';
	s+='</select></nobr>';
    document.write(s);

}
//===================	ASOutput.htm	======================================================^^^^^^^^===============
//===================	ASOutput.htm	=============================================================================
//	idx = 1-16
//	ivalue:	0: no function, 1:choice 1, 2:choice 2, 3:choice 3, 4:choice 4
//	i2:	[0,1,4] 		which one, 0:door, 1:Parking, 4:Elevator
function SelectEventOutput(idx,ivalue,usele,i2)
{
	var s,v0,v1,v2,v3,v4,v5,v6,v7;

	v0=' ';	v1=' ';	v2=' ';	v3=' ';	v4=' ';

	if(ivalue == 1)			v1 = ' selected';	else if(ivalue == 2)	v2 = ' selected';	else if(ivalue == 3)	v3 = ' selected';
	else if(ivalue == 4)	v4 = ' selected';	else if(ivalue == 5)    v5 = ' selected';   else if(ivalue == 6)    v6 = ' selected';
	else					v0 = ' selected';

	s='<nobr><select id=EventOutput'+idx+' name=EventOutput'+idx+' size=1>';

	if((usele == 0) || (usele >= 6)){	//	No Function, Normal Output
		s+='<option value=0 selected> ';
	}else if((usele >= 1) && (usele <=4)){
		s+='<option value=1'+v1+'>Electric Lock';
		s+='<option value=2'+v2+'>Event Alarm';
		s+='<option value=3'+v3+'>Alarm Keyswitch';
		s+='<option value=4'+v4+'>Entry Card';
		s+='<option value=5'+v5+'>Exit Card';
		if( i2[usele-1] == 0 ){							//	Door
			s+='<option value=6'+v6+'>Unlock Alarm';
		}else if(i2[usele-1] == 1){						//	Car
			s+='<option value=6'+v6+'>Open Alarm';
		}
		else{											//	Elevator
		}
		/*s+='<option value=7'+v7+'>Beeper';
		s+='<option value=8'+v8+'>Green LED';
		s+='<option value=9'+v9+'>Red LED';
		s+='<option value=10'+v10+'>Invert Red LED';*/
	}else if(usele == 5){
		s+='<option value=1'+v1+'>Area Occupied';
	}
	s+='</select></nobr>';
    document.write(s);
}
//===================	ASOutput.htm	======================================================^^^^^^^^===============
//===================	ASOutput.htm	=============================================================================
//	infunc : 0:door,	1:Parking,	4:Elevator,	9:No Function
function FuChange(idx,infunc)
{
	var i,j;
	var NewSe00=document.createElement('option');
	var NewSe01=document.createElement('option');
	var NewSe02=document.createElement('option');
	var NewSe03=document.createElement('option');
	var NewSe04=document.createElement('option');
	var NewSe05=document.createElement('option');
	var NewSe06=document.createElement('option');
	var NewSe07=document.createElement('option');
	var NewSe08=document.createElement('option');
	var NewSe09=document.createElement('option');
	var NewSe10=document.createElement('option');
	
	var a = document.getElementById('Fu'+idx);
	var b = document.getElementById('Ev'+idx);
	for(j=b.length-1;j>=0;j--)
		b.remove(j);

	NewSe00.text = " ";					NewSe00.value = 0;
	NewSe01.text = "Access Granted";	NewSe01.value = 1;
	NewSe02.text = "Access Denied";		NewSe02.value = 2;
	NewSe03.text = "Fire Alarm";		NewSe03.value = 3;
	NewSe04.text = "Tamper";			NewSe04.value = 4;
	NewSe05.text = "Duress Event";		NewSe05.value = 5;
	NewSe06.text = "Held Open";			NewSe06.value = 6;
	NewSe07.text = "Forced Open";		NewSe07.value = 7;
	NewSe08.text = "Exit Button";		NewSe08.value = 8;
	NewSe09.value = 9;
	NewSe10.text = "ARM Mode";			NewSe10.value = 10;
	
	if(a.selectedIndex == 0){
		b.add(NewSe00);
		b.disabled = true;
	}
	else{
		b.disabled = false;
		b.add(NewSe01);
		b.add(NewSe02);
		b.add(NewSe03);
		b.add(NewSe04);
		b.add(NewSe05);
		if(infunc[a.selectedIndex-1]==0){	//	Door
			b.add(NewSe06);
			b.add(NewSe07);
			b.add(NewSe08);
			NewSe09.text = "Door Contact";
			b.add(NewSe09);
			b.add(NewSe10);
		}
		else if(infunc[a.selectedIndex-1]==1){	//	Parking
			b.add(NewSe06);
			b.add(NewSe07);
			b.add(NewSe08);
			NewSe09.text = "Car Detection";
			b.add(NewSe09);
			b.add(NewSe10);
		}
		//else{									//	4:Elevator
		//}
	}
	b.selectedIndex = 0;
}
//===================	ASOutput.htm	======================================================^^^^^^^^===============
//===================	ASOutput.htm	============================================FuChange=========================
//	inx : 0:door,	1:Parking,	4:Elevator,	9:No Function
function SelectFu(idx,isele,inx)
{
	var s;
	var v = new Array(' ',' ',' ',' ',' ',' ',' ');

	if(isele == 1)			v[1]=' selected';	else if(isele == 2)		v[2]=' selected';	else if(isele == 3)		v[3]=' selected';
	else if(isele == 4)		v[4]=' selected';	else if(isele == 5)		v[5]=' selected';	else 					v[0]=' selected';

	s='<nobr>';
	s+='<select style="font-size:8pt" id=Fu'+idx+' name=Fu'+idx+' size=1 onchange="return FuChange('+idx+',vafu)"';

	s+='><option value=0'+v[0]+'>None';

	if(inx[0] == 0)
		s+='<option value=1'+v[1]+'>Door 1';
	else if(inx[0] == 1)
		s+='<option value=1'+v[1]+'>Gate 1';
	else if(inx[0] == 4)
		s+='<option value=1'+v[1]+'>Relay 1';

	if(inx[1] == 0)
		s+='<option value=2'+v[2]+'>Door 2';
	else if(inx[1] == 1)
		s+='<option value=2'+v[2]+'>Gate 2';
	else if(inx[1] == 4)
		s+='<option value=2'+v[2]+'>Relay 2';

	if(inx[2] == 0)
		s+='<option value=3'+v[3]+'>Door 3';
	else if(inx[2] == 1)
		s+='<option value=3'+v[3]+'>Gate 3';
	else if(inx[2] == 4)
		s+='<option value=3'+v[3]+'>Relay 3';

	if(inx[3] == 0)
		s+='<option value=4'+v[4]+'>Door 4';
	else if(inx[3] == 1)
		s+='<option value=4'+v[4]+'>Gate 4';
	else if(inx[3] == 4)
		s+='<option value=4'+v[4]+'>Relay 4';

	s+='</select></nobr>';
    document.write(s);
}
//===================	ASOutput.htm	======================================================^^^^^^^^===============
//===================	ASOutput.htm	=============================================================================
function SelectLogic(idx,ivalue)
{
	var s,v0,v1;

	v0= ' ';	v1= ' ';

	if(ivalue == 0)	v0=' selected';	else	v1=' selected';

	s='<nobr><select style="font-size:8pt" id=Logic'+idx+' name=Logic'+idx+' size=1>';
	s+='<option value=0'+v0+'>AND';
	s+='<option value=1'+v1+'>OR';
	s+='</select></nobr>';
    document.write(s);
}
//===================	ASOutput.htm	======================================================^^^^^^^^===============
//===================	ASOutput.htm	========	ASExd.htm	=====================================================
function SelectINnormal(idx,inum,normalin,imysele)
{
	var s,i,j;
	var v = new Array(' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ');

	if(imysele == 1)		v[1]=' selected';	else if(imysele == 2)	v[2]=' selected';	else if(imysele == 3)	v[3]=' selected';
	else if(imysele == 4)	v[4]=' selected';	else if(imysele == 5)	v[5]=' selected';	else if(imysele == 6)	v[6]=' selected';
	else if(imysele == 7)	v[7]=' selected';	else if(imysele == 8)	v[8]=' selected';	else if(imysele == 9)	v[9]=' selected';
	else if(imysele == 10)	v[10]=' selected';	else if(imysele == 11)	v[11]=' selected';	else if(imysele == 12)	v[12]=' selected';
	else if(imysele == 13)	v[13]=' selected';	else if(imysele == 14)	v[14]=' selected';	else if(imysele == 15)	v[15]=' selected';
	else if(imysele == 16)	v[16]=' selected';	else if(imysele == 17)	v[17]=' selected';	else if(imysele == 18)	v[18]=' selected';
	else if(imysele == 19)	v[19]=' selected';	else if(imysele == 20)	v[20]=' selected';	else if(imysele == 21)	v[21]=' selected';
	else if(imysele == 22)	v[22]=' selected';	else if(imysele == 23)	v[23]=' selected';	else if(imysele == 24)	v[24]=' selected';
	else if(imysele == 25)	v[25]=' selected';	else if(imysele == 26)	v[26]=' selected';	else if(imysele == 27)	v[27]=' selected';
	else if(imysele == 28)	v[28]=' selected';	else if(imysele == 29)	v[29]=' selected';	else if(imysele == 30)	v[30]=' selected';
	else if(imysele == 31)	v[31]=' selected';	else if(imysele == 32)	v[32]=' selected';	
	else 					v[0]=' selected';

	s='<nobr><select style="font-size:8pt" id=ExdIn'+idx+' name=ExdIn'+idx+' size=1>';
	s+='<option value=0'+v[0]+'>None';

 	for(i=1;i<9;i++){
		if(normalin[i-1] == 1)
			s+='<option value='+i+v[i]+'>Input 0'+i;
	}
	
	/*for(i=10;i<=inum;i++){
		if(normalin[i-1] == 1)
		s+='<option value='+i+v[i]+'>Input '+i;
	}*/

	i=inum+1;
	s+='<option value='+i+v[i]+'>Wiegand A';
	i = i+1;
	s+='<option value='+i+v[i]+'>Wiegand B';
	i = i+1;
	s+='<option value='+i+v[i]+'>Wiegand C';
	i = i+1;
	s+='<option value='+i+v[i]+'>Wiegand D';
	
	for(i=inum+1+8,j=0;i<=(inum+8+8);i++,j++)
		s+='<option value='+i+v[i]+'>Reader ID0'+j;

	s+='</select></nobr>';
    document.write(s);
}
//===================	ASOutput.htm	======================================================^^^^^^^^===============
//===================	ASOutput.htm	========	ASExd.htm	=====================================================
function SelectIN(idx,inum,imysele)
{
	var s,i,j;
	var v = new Array(' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ');

	if(imysele == 1)		v[1]=' selected';	else if(imysele == 2)	v[2]=' selected';	else if(imysele == 3)	v[3]=' selected';
	else if(imysele == 4)	v[4]=' selected';	else if(imysele == 5)	v[5]=' selected';	else if(imysele == 6)	v[6]=' selected';
	else if(imysele == 7)	v[7]=' selected';	else if(imysele == 8)	v[8]=' selected';	else if(imysele == 9)	v[9]=' selected';
	else if(imysele == 10)	v[10]=' selected';	else if(imysele == 11)	v[11]=' selected';	else if(imysele == 12)	v[12]=' selected';
	else if(imysele == 13)	v[13]=' selected';	else if(imysele == 14)	v[14]=' selected';	else if(imysele == 15)	v[15]=' selected';
	else if(imysele == 16)	v[16]=' selected';	else if(imysele == 17)	v[17]=' selected';	else if(imysele == 18)	v[18]=' selected';
	else if(imysele == 19)	v[19]=' selected';	else if(imysele == 20)	v[20]=' selected';	else if(imysele == 21)	v[21]=' selected';
	else if(imysele == 22)	v[22]=' selected';	else if(imysele == 23)	v[23]=' selected';	else if(imysele == 24)	v[24]=' selected';
	else if(imysele == 25)	v[25]=' selected';	else if(imysele == 26)	v[26]=' selected';	else if(imysele == 27)	v[27]=' selected';
	else if(imysele == 28)	v[28]=' selected';	else if(imysele == 29)	v[29]=' selected';	else if(imysele == 30)	v[30]=' selected';
	else if(imysele == 31)	v[31]=' selected';	else if(imysele == 32)	v[32]=' selected';	
	else 					v[0]=' selected';

	s='<nobr><select style="font-size:8pt" id=ExdIn'+idx+' name=ExdIn'+idx+' size=1>';
	s+='<option value=0'+v[0]+'>None';

	if(inum <= 9){
		for(i=1;i<=inum;i++)
			s+='<option value='+i+v[i]+'>Input 0'+i;
	}
	else{
		for(i=1;i<=9;i++)
			s+='<option value='+i+v[i]+'>Input 0'+i;
		for(i=10;i<=inum;i++)
			s+='<option value='+i+v[i]+'>Input '+i;
	}
	i = inum+1;
	s+='<option value='+i+v[i]+'>Wiegand A';
	i = i+1;
	s+='<option value='+i+v[i]+'>Wiegand B';
	i = i+1;
	s+='<option value='+i+v[i]+'>Wiegand C';
	i = i+1;
	s+='<option value='+i+v[i]+'>Wiegand D';
	i = i+1;
	s+='<option value='+i+v[i]+'>Wiegand E';
	i = i+1;
	s+='<option value='+i+v[i]+'>Wiegand F';
	i = i+1;
	s+='<option value='+i+v[i]+'>Wiegand G';
	i = i+1;
	s+='<option value='+i+v[i]+'>Wiegand H';


	for(i=inum+1+8,j=0;i<=(inum+8+8);i++,j++)
		s+='<option value='+i+v[i]+'>Reader 0'+j;

	s+='</select></nobr>';
    document.write(s);
}
//===================	ASOutput.htm	======================================================^^^^^^^^===============
//===================	ASOutput.htm	=============================================================================
//	inx : 0:door,	1:Parking,	4:Elevator,	9:No Function
function SelectEv(idx,ifusele,inx,imysele)
{
	var s;
	var v = new Array(' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ');

	if(imysele == 1)		v[1]=' selected';	else if(imysele == 2)	v[2]=' selected';	else if(imysele == 3)	v[3]=' selected';
	else if(imysele == 4)	v[4]=' selected';	else if(imysele == 5)	v[5]=' selected';	else if(imysele == 6)	v[6]=' selected';
	else if(imysele == 7)	v[7]=' selected';	else if(imysele == 8)	v[8]=' selected';	else if(imysele == 9)	v[9]=' selected';
	else if(imysele == 10)	v[10]=' selected';	else if(imysele == 11)	v[11]=' selected';	else if(imysele == 12)	v[12]=' selected';
	else if(imysele == 13)	v[13]=' selected';	else 					v[0]=' selected';

	s='<nobr>';
	s+='<select style="font-size:8pt" id=Ev'+idx+' name=Ev'+idx+' size=1 >';

	if(ifusele == 0)						//	No Function
		s+='<option value=0 selected> ';
	else
	{
		s+='<option value=1'+v[1]+'>Access Granted';
		s+='<option value=2'+v[2]+'>Access Denied';
		s+='<option value=3'+v[3]+'>Fire Alarm';
		s+='<option value=4'+v[4]+'>Tamper';
		s+='<option value=5'+v[5]+'>Duress';

		if(inx[ifusele-1] == 0){	//	Door
			s+='<option value=6'+v[6]+'>Held Open';
			s+='<option value=7'+v[7]+'>Forced Open';
			s+='<option value=8'+v[8]+'>Exit Button';
			s+='<option value=9'+v[9]+'>Door Contact';
			s+='<option value=10'+v[10]+'>ARM Mode';
		}else{						//	Parking
			s+='<option value=6'+v[6]+'>Held Open';
			s+='<option value=7'+v[7]+'>Forced Open';
			s+='<option value=8'+v[8]+'>Exit Button';
			s+='<option value=9'+v[9]+'>Car Detection';
			s+='<option value=10'+v[10]+'>ARM Mode';
		}
	}
	s+='</select></nobr>';
    document.write(s);

	if(ifusele == 0){
		var b = document.getElementById('Ev'+idx);
		b.disabled = true;
	}
}

/*********************************************************************************/
//ASReader.htm & ExdReader.htm used
/*********************************************************************************/
function ReaderStatus(inone)
{
	var s;

	if(inone == 1)
		s = '<img src="ok.png" width="40" height="25" alt="">';
	else if(inone == 2)
		s = '<img src="fail.png" width="40" height="25" alt="">';
	else
		s=' ';
	document.write(s);
}
function FingerStatus(inone)
{
	var s;

	if(inone == 1)
		s = '<img src="ok.png" width="40" height="25" alt="">';
	else if(inone == 2)
		s = '<img src="fail.png" width="40" height="25" alt="">';
	else if(inone == 11)
		s = '<img src="ok.png" width="40" height="25" alt="">';
	else
		s=' ';
	document.write(s);
}
//===================	ASReader.htm	=============================================================================
//	inx	= 0:door, 1:Parking, 4:Elevator, 9:no function
function ModeReaderChange(idx,inx)
{
	var i;
	var NewSe01=document.createElement('option');
	var NewSe02=document.createElement('option');
	var NewSe03=document.createElement('option');


	var a = document.getElementById('ModeReader'+idx);
	var Fi = document.getElementById('FuncReader'+idx);

	for(i=a.length-1;i>=0;i--)
		a.remove(i);

	NewSe01.text = " ";						NewSe01.value = 0;
	NewSe02.text = "Door/Gate Entry";		NewSe02.value = 1;
	NewSe03.text = "Door/Gate Exit";		NewSe03.value = 2;

	if(Fi.selectedIndex == 0)
		a.add(NewSe01);
	else
	{
		a.add(NewSe02);
		a.add(NewSe03);
	}
}
//===================	ASReader.htm	======================================================^^^^^^^^===============
function SelectFilterMode(idx,ivalue)
{
	var s,v0,v1;

	v0=' ';v1=' ';

	if(ivalue == 0)			v0=' selected';
	else					v1=' selected';

	s='<nobr><select id=ASReader'+idx+' name=ASReader'+idx+' size=1>';

	s+='<option value=0'+v0+'>Disable ';
	s+='<option value=1'+v1+'>Enable';
	s+='</select></nobr>';
    document.write(s);
}
//===================	ASReader.htm	=============================================================================
function ASReader(idx,ione)
{
	var s,v0,v1,v2,v3,v4,v5,v6,v7,v8;

	v0=' ';
	v1=' ';v2=' ';	v3=' ';	v4=' ';
	v5=' ';v6=' ';	v7=' ';	v8=' ';

	if(ione == 0)			v0=' selected';
	else if(ione == 1)		v1=' selected';
	else if(ione == 2)		v2=' selected';
	else if(ione == 3)		v3=' selected';
	else if(ione == 4)		v4=' selected';
	else if(ione == 5)		v5=' selected';
	else if(ione == 6)		v6=' selected';
	else if(ione == 7)		v7=' selected';
	else 					v8=' selected';

	s='<nobr><select id=ASReader'+idx+' name=ASReader'+idx+' size=1>';

	s+='<option value=0'+v0+'>No Function';
	s+='<option value=1'+v1+'>Door/Gate 1 Entry';
	s+='<option value=2'+v2+'>Door/Gate 1 Exit';
	s+='<option value=3'+v3+'>Door/Gate 2 Entry';
	s+='<option value=4'+v4+'>Door/Gate 2 Exit';
	s+='<option value=5'+v5+'>Door/Gate 3 Entry';
	s+='<option value=6'+v6+'>Door/Gate 3 Exit';
	s+='<option value=7'+v7+'>Door/Gate 4 Entry';
	s+='<option value=8'+v8+'>Door/Gate 4 Exit';
	s+='</select></nobr>';
    document.write(s);
}
//===================	ASReader.htm	======================================================^^^^^^^^===============
//===================	ASReader.htm	=========================================ModeReaderChange====================
//	idx = 1-16;
//	isele = 1:A, 2:B, 3:C, 4:D, 5:No function ;
//	inx	= 0:door, 1:Parking, 4:Elevator, 9:no function
function SelectFuncReader(idx,isele,ina,inb,inc,ind)
{
	var s;
	var v = new Array(' ',' ',' ',' ',' ');

	if(isele == 1)			v[1]=' selected';	else if(isele == 2)		v[2]=' selected';	else if(isele == 3)		v[3]=' selected';
	else 					v[4]=' selected';

	s='<nobr><select id=FuncReader'+idx+' name=FuncReader'+idx+' size=1>';

	if(ina == 0)
		s+='<option value=1'+v[1]+'>Door 1';
	else if(ina == 1)
		s+='<option value=1'+v[1]+'>Gate 1';
	else if(ina == 4)
		s+='<option value=1'+v[1]+'>Relay 1';

	if(inb == 0)
		s+='<option value=2'+v[2]+'>Door 2';
	else if(inb == 1)
		s+='<option value=2'+v[2]+'>Gate 2';
	else if(inb == 4)
		s+='<option value=2'+v[2]+'>Relay 2';

	if(inc == 0)
		s+='<option value=3'+v[3]+'>Door 3';
	else if(inc == 1)
		s+='<option value=3'+v[3]+'>Gate 3';
	else if(inc == 4)
		s+='<option value=3'+v[3]+'>Relay 3';

	if(ind == 0)
		s+='<option value=4'+v[4]+'>Door 4';
	else if(ind == 1)
		s+='<option value=4'+v[4]+'>Gate 4';
	else if(ind == 4)
		s+='<option value=4'+v[4]+'>Relay 4';

	s+='</select></nobr>';
    document.write(s);
}
//===================	ASReader.htm	======================================================^^^^^^^^===============
//===================	ASReader.htm	=============================================================================
//	ivalue:	0: no function, 1:choice 1, 2:choice 2, 3:choice 3, 4:choice 4
//	i2:	[0,1,4] 		which one, 0:door, 1:Parking, 4:Elevator
function SelectModeReader(idx,ivalue,i2)
{
	var s,v0,v1,v2;

	v0=' ';v1=' ';	v2=' ';

	if(ivalue == 1)			v1=' selected';
	else					v2=' selected';

	s='<nobr><select id=ModeReader'+idx+' name=ModeReader'+idx+' size=1>';

	s+='<option value=1'+v1+'>Door/Gate Entry';
	s+='<option value=2'+v2+'>Door/Gate Exit';
	s+='</select></nobr>';
    document.write(s);
}
//===================	ASReader.htm	======================================================^^^^^^^^===============
//===================	ASReader.htm	=============================================================================
function SelectReadMode(ivalue)
{
	var s,v0,v1;

	v0=' ';v1=' ';

	if(ivalue == 0)			v0=' selected';
	else					v1=' selected';

	s='<nobr><select id=ReadMode name=ReadMode size=1>';

	s+='<option value=0'+v0+'>Read UID';
	s+='<option value=1'+v1+'>Read GID';
	s+='</select></nobr>';
    document.write(s);
}
//===================	ASReader.htm	======================================================^^^^^^^^===============
//===================	ExdReader.htm	=============================================================================
function ExdBox(idx,En)
{
	var s,v;

	if(En == 1)
		v = ' checked';
	else
		v = ' ';

	s='<nobr>';
	s+='<input name=ExdBox'+idx+' type=checkbox value=1 onclick="return ChkExdBox('+idx+')"'+v+'></nobr>';
	document.write(s);
}
//===================	ExdReader.htm	======================================================^^^^^^^^===============
//===================	ExdReader.htm	=============================================================================
function ExdReader(idx,ione)
{
	var s,v0,v1,v2,v3,v4,v5,v6,v7,v8;

	v0=' ';
	v1=' ';v2=' ';	v3=' ';	v4=' ';
	v5=' ';v6=' ';	v7=' ';	v8=' ';

	if(ione == 0)			v0=' selected';
	else if(ione == 1)		v1=' selected';
	else if(ione == 2)		v2=' selected';
	else if(ione == 3)		v3=' selected';
	else if(ione == 4)		v4=' selected';
	else if(ione == 5)		v5=' selected';
	else if(ione == 6)		v6=' selected';
	else if(ione == 7)		v7=' selected';
	else 					v8=' selected';

	s='<nobr><select id=ExdReader'+idx+' name=ExdReader'+idx+' size=1>';

	s+='<option value=0'+v0+'>No Function';
	s+='<option value=1'+v1+'>Door/Gate 1 Entry';
	s+='<option value=2'+v2+'>Door/Gate 1 Exit';
	s+='<option value=3'+v3+'>Door/Gate 2 Entry';
	s+='<option value=4'+v4+'>Door/Gate 2 Exit';
	s+='<option value=5'+v5+'>Door/Gate 3 Entry';
	s+='<option value=6'+v6+'>Door/Gate 3 Exit';
	s+='<option value=7'+v7+'>Door/Gate 4 Entry';
	s+='<option value=8'+v8+'>Door/Gate 4 Exit';
	s+='</select></nobr>';
    document.write(s);
}
//===================	ExdReader.htm	======================================================^^^^^^^^===============
//===================	ExdReader.htm	=============================================================================
function SubExd()
{
	var a,b,c,d,i,j;
	var finger = new Array(' ',' ',' ',' ',' ',' ',' ',' ');

	for(i=0;i<8;i++)
		finger[i] = 0;

	for(i=8;i<17;i++)
	{
		a = document.getElementById('ExdBox'+i);
		if(a.checked == true)
		{
			b = document.getElementById('ExdReader'+i);
			finger[i-8] = b.selectedIndex;
		}
	}

	i = 0;
	c = document.getElementById('Txt_Finger01');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 01 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger02');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 02 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger03');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 03 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger04');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 04 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger05');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 05 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger06');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 06 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger07');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 07 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger08');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 08 serial number must between 0 - 4294967295");
	}

	if(i != 0)
		return false;

	for(j=0;j<8;j++)
	{
		if(finger[j]!=0)
		{
			for(i=0;i<8;i++)
			{
				if(finger[j]==finger[i] && i!=j)
				{
					alert("One Door Entry/Exit only Support One GV-Finger\n");
                	return false;
				}
			}
		}

	}

	return true;
}
//===================	ExdReader.htm	======================================================^^^^^^^^===============
//===================	ExdReader.htm	=============================================================================
function ChkExdBox(idx)
{
	var i,a,sum;
	var k=0;

	sum = 0;
	for(i=1;i<=16;i++)
	{
		a = document.getElementById('ExdBox'+i);
		if(a.checked == true)
			sum++;
		if(sum >= 9)
		{
			a = document.getElementById('ExdBox'+idx);
			alert("Only Support Total eight Extend Devices");
			a.checked = false;
			return;
		}
	}

}
//===================	ExdReader.htm	======================================================^^^^^^^^===============
//===================	ASExd1.htm		=============================================================================
function DetectExd(idx)
{
	var s,v1,v2,v3,v4;

	v1=' ';	v2=' ';	v3=' ';	v4=' ';

	if(idx == 1)
		v1=' selected';
	else if(idx == 2)
		v2=' selected';
	else if(idx == 3)
		v3=' selected';
	else
		v4=' selected';

	s='<nobr><select id=DetectExd name=DetectExd size=1>';
	s+='<option value=1'+v1+'> 01';
	s+='<option value=2'+v2+'> 02';
	s+='<option value=3'+v3+'> 03';
	s+='<option value=4'+v4+'> 04';
	s+='</select></nobr>';
	document.write(s);

	var a = document.getElementById('DetectExd');
	a.style.display = "none";
}

//===================	ASExd1.htm  	======================================================^^^^^^^^===============
//===================	ASPare.htm		=============================================================================
function Interlock(value)
{
	var s,v0,v1;

	v0=' '; v1=' ';

	if(value == 0)
		v0=' selected';
	else
		v1=' selected';

	s='<nobr>';
	s+='<select name=Interlock size=1><option value=0'+v0+'>NO<option value=1'+v1+'>YES</select></nobr>';
	document.write(s);
}

