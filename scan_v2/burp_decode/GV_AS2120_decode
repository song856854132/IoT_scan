    else if(time == 58) v58 = ' selected';  else if(time == 59) v59 = ' selected';  else v0 = ' selected';

	s='<nobr>';
    s+='<select name=Min'+index+' size=1 > <option value=0'+v0+'>0  <option value=1'+v1+'>1 <option value=2'+v2+'>2 <option value=3'+v3+'>3 <option value=4'+v4+'>4 <option value=5'+v5+'>5 <option value=6'+v6+'>6 <option value=7'+v7+'>7 <option value=8'+v8+'>8 <option value=9'+v9+'>9 <option value=10'+v10+'>10 <option value=11'+v11+'>11 <option value=12'+v12+'>12 <option value=13'+v13+'>13 <option value=14'+v14+'>14 <option value=15'+v15+'>15 <option value=16'+v16+'>16 <option value=17'+v17+'>17 <option value=18'+v18+'>18 <option value=19'+v19+'>19 <option value=20'+v20+'>20 <option value=21'+v21+'>21 <option value=22'+v22+'>22 <option value=23'+v23+'>23 <option value=24'+v24+'>24 <option value=25'+v25+'>25 <option value=26'+v26+'>26 <option value=27'+v27+'>27 <option value=28'+v28+'>28 <option value=29'+v29+'>29 <option value=30'+v30+'>30 <option value=31'+v31+'>31 <option value=32'+v32+'>32 <option value=33'+v33+'>33 <option value=34'+v34+'>34 <option value=35'+v35+'>35 <option value=36'+v36+'>36 <option value=37'+v37+'>37 <option value=38'+v38+'>38 <option value=39'+v39+'>39 <option value=40'+v40+'>40 <option value=41'+v41+'>41 <option value=42'+v42+'>42 <option value=43'+v43+'>43 <option value=44'+v44+'>44 <option value=45'+v45+'>45 <option value=46'+v46+'>46 <option value=47'+v47+'>47 <option value=48'+v48+'>48 <option value=49'+v49+'>49 <option value=50'+v50+'>50 <option value=51'+v51+'>51 <option value=52'+v52+'>52 <option value=53'+v53+'>53 <option value=54'+v54+'>54 <option value=55'+v55+'>55 <option value=56'+v56+'>56 <option value=57'+v57+'>57 <option value=58'+v58+'>58 <option value=59'+v59+'>59  </select></nobr>'
    document.write(s);


}

function Draw3DES(intype, sValue)
{
	var s;
	if(intype == 1)
		s='<input type=password name=Txt_3Des1 value="'+sValue+'" size=30 maxlength=8 class='+sClassInputText+' onClick="desZero(1);">';
	else if(intype == 2)
		s='<input type=password name=Txt_3Des2 value="'+sValue+'" size=30 maxlength=8 class='+sClassInputText+' onClick="desZero(2);">';
	else
		s='<input type=password name=Txt_3Des3 value="'+sValue+'" size=30 maxlength=8 class='+sClassInputText+' onClick="desZero(3);">';
	document.write(s);
}

function Syear(index, time)
{
    var s,v2,v3,v4,v5,v6,v7,v8,v9,v10,v11,v12,v13,v14,v15,v16,v17,v18,v19,v20,v21,v22,v23,v24,v25,v26,v27,v28,v29,v30,v31,v32,v33,v34,v35,v36,v37,v38,v39,v40,v41,v42,v43,v44;

    v2 = ' ';   v3 = ' ';   v4 = ' ';   v5 = ' ';   v6 = ' ';   v7 = ' ';   v8 = ' ';   v9 = ' ';   v10 = ' ';
    v11 = ' ';  v12 = ' ';  v13 = ' ';  v14 = ' ';  v15 = ' ';  v16 = ' ';  v17 = ' ';  v18 = ' ';  v19 = ' ';  v20 = ' ';
