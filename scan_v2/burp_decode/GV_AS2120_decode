<script language=JavaScript>
var admin_pw_level = 0;
var g_password_level_threshold = 1; // 0: Weak, 1: Normal, 2: Great

function PW_strength_reflash(level)
{
  var strength_h = document.getElementById('sPWAdmStrengthH');
  var strength_m = document.getElementById('sPWAdmStrengthM');
  var strength_l = document.getElementById('sPWAdmStrengthL');

  switch(level)
    {
      case 3:
          strength_h.style.display = "";
          strength_h.style.color = "#33cc00";
          strength_m.style.display = "none";
          strength_l.style.display = "none";
          break;
      case 2:
          strength_h.style.display = "none";
          strength_m.style.display = "";
          strength_m.style.color = "#ff9900";
          strength_l.style.display = "none";
          break;
      case 1:
          strength_h.style.display = "none";
          strength_m.style.display = "none";
          strength_l.style.display = "";
          strength_l.style.color = "#ff0000";
          break;
      default:
          strength_h.style.display = "none";
          strength_m.style.display = "none";
          strength_l.style.display = "none";
          break;
    }
}

function submmit_status_refresh ()
{
  var FORM;
  //var password;
  //var password_retype;
  var status = false;

    { FORM = document.forms[0]; }

    if (FORM.Txt_LoginName.value == "")
      { status = true; }
    //else if (FORM.passwordOld.value == "")
    //  { status = true; }
    else if (FORM.Txt_AdminPasswd.value == "")
      { status = true; }
    else if (FORM.Txt_AdminPasswd.value != FORM.Txt_ReAdminPasswd.value)
      { status = true; }
    else if (admin_pw_level < g_password_level_threshold)
      { status = true; }

  FORM.Submit.disabled = status;
}

function checkMatch()
{
  var FORM;
  var passwordRetypeMsg;
  //var password_retype;

    FORM = document.forms[0];
    passwordRetypeMsg = document.getElementById('admin_passwordRetypeMsg');

  if (FORM.Txt_AdminPasswd.value == FORM.Txt_ReAdminPasswd.value) {
    passwordRetypeMsg.style.display = "none";
    passwordRetypeMsg.style.color   = "#ff0000";
  } else {
    passwordRetypeMsg.style.display = "";
  }
  submmit_status_refresh ();
}

function username_check (f)
{
  f.Txt_LoginName.value = INPUT_bFilterHTMLtag (f.Txt_LoginName.value, 'Login Name');
  submmit_status_refresh ();
}

function PWStrength(f)
{
  f.Txt_AdminPasswd.value = INPUT_bFilterHTMLtag (f.Txt_AdminPasswd.value, 'New Password');

  pwd = f.Txt_AdminPasswd.value;

  if (pwd == null || pwd == '')
     {
        PW_strength_reflash(0);
        return;
     }

  S_level = PW_check_strong(pwd); // 0: Weak, 1: Normal, 2: Great
  if (S_level < 0)
    { S_level = 0; }
  PW_strength_reflash(S_level + 1);

    { admin_pw_level = S_level; }

  checkMatch ();
}

var	ZeroChar = '';
function OnbeforeSubmit(f)
{
    if(!INPUT_bCheckHTMLtag(f.Txt_LoginName.value, 'Login Name') )
        return false;
    if(!INPUT_bCheckHTMLtag(f.Txt_AdminPasswd.value, 'New Password') )
        return false;
    if(!INPUT_bCheckEmpty(f.Txt_ReAdminPasswd.value))
        return false;
    if(f.Txt_AdminPasswd.value != f.Txt_ReAdminPasswd.value) {
        alert('The passwords do not match.');
        return false;
    }
	if(f.Txt_AdminPasswd.value.length<=5) {
		alert('The password must be longer than 6 characters.');
		return false;
	}
	if(f.Txt_LoginName.value == 'admin' && f.Txt_AdminPasswd.value == 'admin') {
		alert('Login Name/Password cannot be default setting.');
		return false;
	}
	return true;
}

</script>

</head>

