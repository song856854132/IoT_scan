	s='<input name=ControlType_'+num+' value="'+value+'"type=hidden size=5 maxlength=3 class='+sClassInputText+' >';
	document.write(s);
	s='<input name=w0x1c'+num+' value="'+w0x1c+'"type=hidden size=5 maxlength=10 class='+sClassInputText+' >';
	document.write(s);
}

