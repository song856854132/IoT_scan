	var2 = ' ';
	var3 = ' ';
	var4 = ' ';
	var5 = ' ';
	var6 = ' ';
	var7 = ' ';
	var8 = ' ';

	if(sMask == 1)
		var1 = ' selected';
	else if(sMask == 2)
		var2 = ' selected';
	else if(sMask == 3)
		var3 = ' selected';
	else if(sMask == 4)
		var4 = ' selected';
	else if(sMask == 5)
		var5 = ' selected';
	else if(sMask == 6)
