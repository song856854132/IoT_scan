<script language=JavaScript>
function SubmitCheck()
{
	if(document.CF.Default.value=='cancel'){
		document.CF.Default.value='Default';
		return false;
	}

	if(document.CF.Format.value=='cancel'){
		document.CF.Format.value='Format';
		return false;
	}

	if(document.CF.Unmount.value=='cancel'){
		document.CF.Unmount.value='Unmount';
		return false;
	}

	if(document.CF.Check.value=='cancel'){
		document.CF.Check.value='Check';
		return false;
	}
	
	if ( GetNum('CF', 'DevicePort') < 1025 )
	{
		alert('Device Port must bigger than 1024\nPlease input again!');
		return false;
	}

	if( GetNum('CF', 'DevicePort') > 65535 )
	{
		alert('Device Port must less than 65536\nPlease input again!');
		return false;
	}
	return true;
}
</script>
