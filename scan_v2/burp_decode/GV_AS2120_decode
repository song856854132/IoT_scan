function Select_AddOptionLst(szObjId, arrItem)
{
    var iIindex = 0;

    for(iIindex=0;iIindex<arrItem[0].length;iIindex++) {
        Select_AddOption(szObjId, arrItem[0][iIindex], arrItem[1][iIindex]);
    }
}

function Select_DelOptionLst(szObjId, arrItem)
{
    var iIindex = 0;

    for(iIindex=0;iIindex<arrItem[1].length;iIindex++) {
        Select_DelOption(szObjId, arrItem[1][iIindex]);
    }
}

function Select_AddOption(szObjId, szText, szValue)
{
    var ObjSel = document.getElementById(szObjId);
    var iCount = ObjSel.options.length;
    
    ObjSel.options[iCount] = new Option(szText, szValue);
}

function Select_DelOption(szObjId, szValue)
{
    var ObjSel = document.getElementById(szObjId);
    var iIindex = 0;

    for(iIindex=0;iIindex<ObjSel.length;iIindex++) {
        if(ObjSel.options[iIindex].value == szValue) {
            ObjSel.options[iIindex] = null;
            return;
        }
    }
}

function Select_SelectAll(szObjId)
{
    var iIindex = 0;
    var ObjSel = document.getElementById(szObjId);

    for(iIindex=0;iIindex<ObjSel.length;iIindex++) {
        ObjSel.options[iIindex].selected = true;
    }
}

function Select_UnSelectAll(szObjId)
{
    var iIindex = 0;
    var ObjSel = document.getElementById(szObjId);

    for(iIindex=0;iIindex<ObjSel.length;iIindex++) {
        ObjSel.options[iIindex].selected = false;
    }
}

function Select_SetSelect(szObjId, szValue)
{
    var iIindex = 0;
    var ObjSel = document.getElementById(szObjId);

    for(iIindex=0;iIindex<ObjSel.length;iIindex++) {
        if(ObjSel.options[iIindex].value == szValue) {
            ObjSel.options[iIindex].selected = true;
            return;
        }
    }
}

function Select_MovOption(szObjIdFm, szObjIdTo)
{    
	var iIindex = 0;
	var ObjSelFm = document.getElementById(szObjIdFm);
	var ObjSelTo = document.getElementById(szObjIdTo);
    var iCount = ObjSelFm.options.length;
       
    for(iIindex=0;iIindex<iCount;iIindex++) {
        if(ObjSelFm.options[iIindex].selected) {
            Select_AddOption(szObjIdTo, ObjSelFm.options[iIindex].text, ObjSelFm.options[iIindex].value);
            Select_SetSelect(szObjIdTo, ObjSelFm.options[iIindex].value);
            Select_DelOption(szObjIdFm, ObjSelFm.options[iIindex].value);
            iCount--;
            iIindex--;
        }
    }    
}

function Select_OnChangeSingle(ObjSel)
{
    var iIindex=0;
    var szSelectText;
    var szSelectValue;
    
    for(iIindex=0;iIindex<ObjSel.options.length;iIindex++) {
        if(ObjSel.options[iIindex].selected == true)
            break;
    }

    szSelectText  = ObjSel.options[iIindex].text;
    szSelectValue = ObjSel.options[iIindex].value;

    Select_OnChangeSingle_CallBack(ObjSel.id, szSelectText, szSelectValue);  
}

function Select_OnChangeMulti(ObjSel)
{
    var iIindex = 0;
    var iSelCount = 0;
    var arrSelectText  = new Array();
    var arrSelectValue = new Array();
    
    for(iIindex=0;iIindex<ObjSel.options.length;iIindex++) {
        if(ObjSel.options[iIindex].selected == true) {
            arrSelectText[iSelCount] = ObjSel.options[iIindex].text;
            arrSelectValue[iSelCount] = ObjSel.options[iIindex].value;
            iSelCount++;
        }    
    }

    Select_OnChangeMulti_CallBack(ObjSel.id, iSelCount, arrSelectText, arrSelectValue);
}

function Table_AddRow(ObjTbl, arrColor, arrAlign, arrItem)
{
    var iIindex = 0;
    var lastRow = ObjTbl.rows.length;
    var iteration = lastRow;
    var ObjTr = ObjTbl.insertRow(lastRow);
   
    for(iIindex=0;iIindex<arrItem.length;iIindex++) {
        var ObjTd = ObjTr.insertCell(iIindex);
        ObjTd.bgColor   = arrColor[iIindex];
        ObjTd.align     = arrAlign[iIindex];
        ObjTd.innerHTML = arrItem[iIindex];
    }
}

function INPUT_bCheckString(szValue)
{
    myString = new String(szValue);

    if(myString.indexOf("\\")>=0) {
        alert(GetXML("IDS_VS_Chk_No_BS"));
        return false;
    }

    return true;
}

function INPUT_bCheckEmpty(szValue)
{
    myString = new String(szValue);

    if(szValue == "") {
        alert(GetXML("IDS_VS_Chk_No_Empty"));
        return false;
    }

    if(myString.indexOf("\\")>=0) {
        alert(GetXML("IDS_VS_Chk_No_BS"));
        return false;
    }

    return true;
}

function INPUT_bCheckHTMLtag(source_value, source)
{
    var txt = new RegExp("[\@\#\$\%\&\*\(\`\~\|\)\;\'\"\<\.\>\?\/\\s]");
    var special_character_set = '@ # $ % & * ( ` ~ | ) \ ; \' \" < . > ? /';

    if (txt.test(source_value)) {
	//alert('String cannot include: ' + txt);
	alert('\' ' + source + ' \' can NOT include the following special characters ' + special_character_set + ' or whitespace.'); 
	return false;
    }

    return true;
}

function INPUT_bFilterHTMLtag (source_value, source)
{
    var ret_str = source_value;
    if (!INPUT_bCheckHTMLtag (source_value, source)) {
	ret_str = source_value.replace(/[\@\#\$\%\&\*\(\`\~\|\)\;\'\"\<\.\>\?\/\\s]/gi, '');
    }
    return ret_str;
}

function INPUT_bCheckYearDate(szDay)
{
    myString = new String(szDay);
    splitString = myString.split("/");
    myYear  = new String(splitString[0]);
    myMonth = new String(splitString[1]);
    myDay   = new String(splitString[2]);

    if(szDay == "") {
        //alert("The fields can't be empty.");
        return false;
    }
            
    if( myYear < 2000 || myYear > 2037 || isNaN(myYear) || myYear.length!=4 ) {
        alert(GetXML("IDS_VS_Chk_Invalid_Year"));
        return false;
    }

    if( myMonth < 1 || myMonth > 12 || isNaN(myMonth) || myMonth.length!=2) {
        alert(GetXML("IDS_VS_Chk_Invalid_Mon"));
        return false;
    }

    if( myDay < 1 || myDay > 31 || isNaN(myDay) || myDay.length!=2 ) {
        alert(GetXML("IDS_VS_Chk_Invalid_Day"));
        return false;
    }

    if (myMonth == 2)
      {
        if( myDay < 1 || myDay > 29 || isNaN(myDay)) {
            alert(GetXML("IDS_VS_Chk_Invalid_Day"));
            return false;
        }
      }

    return true;
}

function INPUT_bCheckTime(szTime)
{
    myString = new String(szTime);
    splitString = myString.split(":");
    myHour  = new String(splitString[0]);
    myMins  = new String(splitString[1]);
    mySecs  = new String(splitString[2]);

    if(szTime == "") {
        alert(GetXML("IDS_VS_Chk_No_Empty"));
        return false;
    }
            
    if( myHour < 0 || myHour > 24 || isNaN(myHour)) {
        alert(GetXML("IDS_VS_Chk_Invalid_Hour"));
        return false;
    }

    if( myMins < 0 || myMins > 60 || isNaN(myMins)) {
        alert(GetXML("IDS_VS_Chk_Invalid_Min"));
        return false;
    }

    if( mySecs < 0 || mySecs > 60 || isNaN(mySecs)) {
        alert(GetXML("IDS_VS_Chk_Invalid_Sec"));
        return false;
    }

    return true;
}

function INPUT_bCheckDstDateTime(szDay)
{
    myString = new String(szDay);
    splitString = myString.split("/");
    myMonth = new String(splitString[0]);
    myDay   = new String(splitString[1]);
    myHour  = new String(splitString[2]);
    myMins  = new String(splitString[3]);

    if(szDay == "") {
        alert(GetXML("IDS_VS_Chk_No_Empty"));
        return false;
    }
            
    if( myMonth < 1 || myMonth > 12 || isNaN(myMonth)) {
        alert(GetXML("IDS_VS_Chk_Invalid_Mon"));
        return false;
    }

    if( myDay < 1 || myDay > 31 || isNaN(myDay)) {
        alert(GetXML("IDS_VS_Chk_Invalid_Day"));
        return false;
    }

    if( myHour < 0 || myHour > 24 || isNaN(myHour)) {
        alert(GetXML("IDS_VS_Chk_Invalid_Hour"));
        return false;
    }

    if( myMins < 0 || myMins > 60 || isNaN(myMins)) {
        alert(GetXML("IDS_VS_Chk_Invalid_Min"));
        return false;
    }

    return true;
}


