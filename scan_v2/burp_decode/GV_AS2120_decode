//===================	ExdReader.htm	======================================================^^^^^^^^===============
//===================	ExdReader.htm	=============================================================================
function SubExd()
{
	var a,b,c,d,i,j;
	var finger = new Array(' ',' ',' ',' ',' ',' ',' ',' ');

	for(i=0;i<8;i++)
		finger[i] = 0;

	for(i=8;i<17;i++)
	{
		a = document.getElementById('ExdBox'+i);
		if(a.checked == true)
		{
			b = document.getElementById('ExdReader'+i);
			finger[i-8] = b.selectedIndex;
		}
	}

	i = 0;
	c = document.getElementById('Txt_Finger01');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 01 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger02');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 02 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger03');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 03 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger04');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 04 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger05');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 05 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger06');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 06 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger07');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 07 serial number must between 0 - 4294967295");
	}
	c = document.getElementById('Txt_Finger08');
	if(c.value>4294967295){
		i++;
		alert("GeoFinger 08 serial number must between 0 - 4294967295");
	}

	if(i != 0)
		return false;

	for(j=0;j<8;j++)
	{
		if(finger[j]!=0)
		{
			for(i=0;i<8;i++)
			{
				if(finger[j]==finger[i] && i!=j)
				{
					alert("One Door Entry/Exit only Support One GV-Finger\n");
                	return false;
				}
			}
		}

	}

	return true;
}
//===================	ExdReader.htm	======================================================^^^^^^^^===============
//===================	ExdReader.htm	=============================================================================
function ChkExdBox(idx)
{
	var i,a,sum;
	var k=0;

	sum = 0;
	for(i=1;i<=16;i++)
	{
		a = document.getElementById('ExdBox'+i);
		if(a.checked == true)
			sum++;
		if(sum >= 9)
		{
			a = document.getElementById('ExdBox'+idx);
			alert("Only Support Total eight Extend Devices");
			a.checked = false;
			return;
		}
	}

}
