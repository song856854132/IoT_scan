function bytzone()
{
	if((document.CF.Tzone.selectedIndex == 0) || (document.CF.Tzone.selectedIndex == 25) )
	{
        document.CF.Min3.disabled = true;
		document.CF.Min3.value = 0;
    }
	else
	{
        document.CF.Min3.disabled = false;
		document.CF.Min3.value = 0;
	}
}
function TZone(time)
{
    var s,v1,v2,v3,v4,v5,v6,v7,v8,v9,v10,v11,v12,v13,v14,v15,v16,v17,v18,v19,v20,v21,v22,v23,v24,v25,v26;

	v1 = ' ';   v2 = ' ';   v3 = ' ';   v4 = ' ';   v5 = ' ';   v6 = ' ';   v7 = ' ';   v8 = ' ';   v9 = ' ';	v10 = ' ';
	v11 = ' ';  v12 = ' ';  v13 = ' ';  v14 = ' ';  v15 = ' ';  v16 = ' ';  v17 = ' ';  v18 = ' ';  v19 = ' ';  v20 = ' ';
	v21 = ' ';  v22 = ' ';  v23 = ' ';	v24 = ' ';	v25 = ' ';	v26 = ' ';

    if(time == 1)  v1 = ' selected';   else if(time == 2)  v2 = ' selected';
	else if(time == 3)  v3 = ' selected'; else if(time == 4)  v4 = ' selected';   else if(time == 5)  v5 = ' selected';
	else if(time == 6)  v6 = ' selected'; else if(time == 7)  v7 = ' selected';   else if(time == 8)  v8 = ' selected';
	else if(time == 9)  v9 = ' selected'; else if(time == 10) v10 = ' selected';  else if(time == 11) v11 = ' selected';
	else if(time == 12) v12 = ' selected'; else if(time == 13) v13 = ' selected';  else if(time == 14) v14 = ' selected';
	else if(time == 15) v15 = ' selected'; else if(time == 16) v16 = ' selected';  else if(time == 17) v17 = ' selected';
	else if(time == 18) v18 = ' selected'; else if(time == 19) v19 = ' selected';  else if(time == 20) v20 = ' selected';
	else if(time == 21) v21 = ' selected'; else if(time == 22) v22 = ' selected';  else if(time == 23) v23 = ' selected';
	else if(time == 24) v24 = ' selected'; else if(time == 25) v25 = ' selected';  else v26 = ' selected';

	s='<nobr>';
	s+='<select name=Tzone size=1 onchange="return bytzone()"> <option value=1'+v1+'>-12 <option value=2'+v2+'>-11 <option value=3'+v3+'>-10 <option value=4'+v4+'>-9 <option value=5'+v5+'>-8 <option value=6'+v6+'>-7 <option value=7'+v7+'>-6 <option value=8'+v8+'>-5 <option value=9'+v9+'>-4 <option value=10'+v10+'>-3 <option value=11'+v11+'>-2 <option value=12'+v12+'>-1 <option value=13'+v13+'>0 <option value=14'+v14+'>+1 <option value=15'+v15+'>+2 <option value=16'+v16+'>+3 <option value=17'+v17+'>+4 <option value=18'+v18+'>+5 <option value=19'+v19+'>+6 <option value=20'+v20+'>+7 <option value=21'+v21+'>+8 <option value=22'+v22+'>+9 <option value=23'+v23+'>+10 <option value=24'+v24+'>+11 <option value=25'+v25+'>+12 <option value=26'+v26+'>+13 </select></nobr>'
    document.write(s);
}

function ShowTZ(tzh,tzm)
{
	var s;

	s='<nobr>';
	if(tzh>=13)	//	+
	{
		if(tzm<10)
			s+='+'+(tzh-13)+':0'+tzm+'</nobr>';
		else
			s+='+'+(tzh-13)+':'+tzm+'</nobr>';
	}
	else
	{
		if(tzm<10)
			s+='-'+(13-tzh)+':0'+tzm+'</nobr>';
		else
			s+='-'+(13-tzh)+':'+tzm+'</nobr>';
	}
	document.write(s);
}

