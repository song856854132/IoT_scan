/*********************************************************************************/
//ASSet.htm used
/*********************************************************************************/
function ShowTitle(num,in1,in2)
{
	var s,a,b;

	s = ' ';	a= ' ';	b=' ';

	if(in2 != 0)
	{
		if(in1 == 1)	a = 'Door 1';	else if(in1 == 2)	a = 'Gate 1';	else	a = 'Relay 1';
		if(in2 == 1)	b = 'Door 2';	else if(in2 == 2)	b = 'Gate 2';	else	b = 'Relay 2';
		if(num == 2)
			s = '<tr><td width=180><h4>'+a+'</h4></td><td width=200></td><td width=150><h4>'+b+'</h4></td></tr>';
	}
	else
		s = '<tr><td width=200></td><td width=370></td><td width=50></td><td width=50></td></tr>';

	document.write(s);
}
function ShowTitle2(num,in1,in2)
{
	var s,a,b;

	s = ' ';	a= ' ';	b=' ';

	if(in2 != 0)
	{
		if(in1 == 1)	a = 'Door 3';	else if(in1 == 2)	a = 'Gate 3';	else	a = 'Relay 3';
		if(in2 == 1)	b = 'Door 4';	else if(in2 == 2)	b = 'Gate 4';	else	b = 'Relay 4';
		if(num == 2)
			s = '<tr><td width=180><h4>'+a+'</h4></td><td width=200></td><td width=150><h4>'+b+'</h4></td></tr>';
	}
	else
		s = '<tr><td width=200></td><td width=370></td><td width=50></td><td width=50></td></tr>';

	document.write(s);
}
function ShowEvent(num1,num2,num)
{
	var s;

	s = ' ';

		if(num == 3)	//	Elevator
		{
			if(num2 == 1)
				s = 'Relay On Time';
			else if(num2 == 2)
				s = 'Fire Action';
			else if(num2 == 3)
				s = 'Alarm Continuous Time';
		}
		else if(num == 2)	//	Parking
		{
			if(num2 == 1)
				s = 'Anti-passback';
			else if(num2 == 2)
				s = 'Relay On Time';
			else if(num2 == 3)
				s = 'Held Open Time';
			else if(num2 == 4)
				s = 'Fire Action';
			else if(num2 == 5)
				s = 'Alarm Continuous Time';
		}
		else if(num == 1)	//	Door
		{
			if(num2 == 1)
				s = 'Anti-passback';
			else if(num2 == 2)
				s = 'Lock Reset Time';
			else if(num2 == 3)
				s = 'Held Open Time';
			else if(num2 == 4)
				s = 'Fire Action';
			else if(num2 == 5)
				s = 'Alarm Continuous Time';
		}
	document.write(s);
}

//	num1 mean A or B;	num2 means 1-5 index;	num means which function, 1:door,2:parking,3:elevator
function SelectEvent(num1,num2,num,value)
{
	var s,v0,v1,v2;

	v0=' '; v1=' '; v2=' ';
	s = ' ';

	if(num == 3)			//	elevator
	{
		if(num2 == 1)		//	Relay ON Time
			s='<input name=Event'+num1+num2+' value="'+value+'"type=text size=5 maxlength=3 class='+sClassInputText+' onfocus="this.select();" onkeyup="isnum(this);"> (1~600)';
		else if(num2 == 3)	//	Alarm Continuous Time
			s='<input name=Event'+num1+num2+' value="'+value+'"type=text size=5 maxlength=3 class='+sClassInputText+' onfocus="this.select();" onkeyup="isnum(this);"> (1~10)';
		else if(num2 == 2)
		{
			if(value == 0)
				v0=' selected';
			else if(value == 1)
				v1=' selected';
			else
				v2=' selected';
			s='<nobr>';
			s+='<select name=Event'+num1+num2+' size=1><option value=0'+v0+'>Unchange<option value=1'+v1+'>Unlock<option value=2'+v2+'>Lock</select></nobr>';
		}
	}
	else
	{
		if((num == 2) || (num == 1))	//	Parking or Door
		{
			if(num2 == 1)			//	Anti-passback
			{
				if(value == 0)
					v0=' selected';
				else
					v1=' selected';
				s='<nobr>';
				s+='<select name=Event'+num1+num2+' size=1><option value=0'+v0+'>NO<option value=1'+v1+'>YES</select></nobr>';
			}
			else if(num2 == 2)		//	Lock Reset Time
			{
				s='<input name=Event'+num1+num2+' value="'+value+'" type=text size=5 maxlength=3 class='+sClassInputText+' onfocus="this.select();" onkeyup="isnum(this);"> (1~600)';
			}
			else if(num2 == 5)		//	Alarm Continuous Time
				s='<input name=Event'+num1+num2+' value="'+value+'" type=text size=5 maxlength=3 class='+sClassInputText+' onfocus="this.select();" onkeyup="isnum(this);"> (1~10)';
			else if(num2 == 3)		//	Held Open Time
			{
				s='<input name=Event'+num1+num2+' value="'+value+'" type=text size=5 maxlength=4 class='+sClassInputText+' onfocus="this.select();" onkeyup="isnum(this);"> (5~9999)';
			}
			else if(num2 == 4)
			{
				if(value == 0)
					v0=' selected';
				else if(value == 1)
					v1=' selected';
				else
					v2=' selected';
				s='<nobr>';
				s+='<select name=Event'+num1+num2+' size=1><option value=0'+v0+'>Unchange<option value=1'+v1+'>Unlock<option value=2'+v2+'>Lock</select></nobr>';
			}
		}
	}
	document.write(s);
}

function ShowAlarm(num,indata)
{
	var s;

	if(indata == 3)
	{
		if(num == 1)
			s = 'Fire Alarm';
		else if(num == 2)
			s = 'Access Denied';
		else if(num == 3)
			s = 'Tamper';
		else
			s = ' ';
		document.write(s);
	}
	else if((indata == 2) || (indata == 1))
	{
		if(num == 1)
			s = 'Held Open';
		else if(num == 2 && indata==1)
			s = 'Forced Open';
		else if(num == 3)
			s = 'Fire Alarm';
		else if(num == 4)
			s = 'Access Denied';
		else if(num == 5)
			s = 'Tamper';
		else if(num == 6 && indata==1)
			s = 'Tailgating';
		else
			s = ' ';
		document.write(s);
	}
}

function SelectAlarm(num1,num2,able,value)
{
	var s,v0,v1;

	v0=' '; v1=' ';

	if(value == 0)
		v0=' selected';
	else
		v1=' selected';

	if(able == 3)
	{
		if(num2 <= 3)
		{
			s='<nobr>';
			s+='<select name=Alarm'+num1+num2+' size=1><option value=0'+v0+'>NO<option value=1'+v1+'>YES</select></nobr>';
			document.write(s);
		}
	}
	else if((able == 2) && (num2 == 2))
	{
		s='<input name=Alarm'+num1+num2+' value="'+value+'"type=hidden size=5 maxlength=3 class='+sClassInputText+' >';
		document.write(s);
	}
	else if((able == 2) && (num2 == 6))
	{
		s='<input name=Alarm'+num1+num2+' value="'+value+'"type=hidden size=5 maxlength=3 class='+sClassInputText+' >';
		document.write(s);
	}
	else if((able == 2) || (able == 1))
	{
		s='<nobr>';
		s+='<select name=Alarm'+num1+num2+' size=1><option value=0'+v0+'>NO<option value=1'+v1+'>YES</select></nobr>';
		document.write(s);
	}
}

function ShowComm(num,indata)
{
	var s;

	if(indata != 0)
	{
		if(num == 1)
			s = 'Common Password';
		else
			s = 'Confirm Password';
		document.write(s);
	}
}
function MDrawTxtBox(num,sTxtName, sValue, iLength, iMaxLength, iType)
{
	var s;
	if(num != 0)
	{
		s='<input type='+(iType==1?'password':'text')+' name=Txt_'+sTxtName+' value="'+sValue+'" size='+iLength+' maxlength='+iMaxLength+' class='+sClassInputText+' onfocus="this.select();">';
		document.write(s);
	}
}
function Hidden_ControlType(num,value,w0x1c)
{
	var s;
	s = ' ';
	s='<input name=ControlType_'+num+' value="'+value+'"type=hidden size=5 maxlength=3 class='+sClassInputText+' >';
	document.write(s);
	s='<input name=w0x1c'+num+' value="'+w0x1c+'"type=hidden size=5 maxlength=10 class='+sClassInputText+' >';
	document.write(s);
}

/*********************************************************************************/
//ASInput.htm used
/*********************************************************************************/
function ODDrawTxtBox(sFormName, sTxtName, sValue, iLength, iMaxLength, iType)
{
	var s;
	s='<input style="font-size:9pt" type='+(iType==1?'password':'text')+' name=Txt_'+sTxtName+' value="'+sValue+'" size='+iLength+' maxlength='+iMaxLength+' class='+sClassInputText+' onfocus="this.select();">';
	document.write(s);
}

function NCNO(num,indata)
{
	var s,v0,v1;

	v0=' '; v1=' ';

	if(indata == 0)
		v0 = ' selected';
	else
		v1 = ' selected';
	s='<nobr>';
	s+='<select name=InType'+num+' size=1><option value=0'+v0+'>NC<option value=1'+v1+'>NO</select></nobr>';
	document.write(s);
}
//-------------------------------------------------------------
//	idx = 1-16;
//	isele = 0: No Function 1:A, 2:B, 3:C, 4:D, 5:Normal Input ;
//	inx	= 0:door, 1:Parking, 4:Elevator, 9:no function
//------------------------------------------------------------
function SelectFuncInput(idx,isele,inx)
{
	var s;
	var v = new Array(' ',' ',' ',' ',' ',' ');

	if(isele == 1)			v[1]=' selected';	else if(isele == 2)		v[2]=' selected';	else if(isele == 3)		v[3]=' selected';
	else if(isele == 4)		v[4]=' selected';	else if(isele == 5)		v[5]=' selected';	else 					v[0]=' selected';

	s='<nobr>';
	s+='<select id=FuncInput'+idx+' name=FuncInput'+idx+' size=1 onchange="return EventInputChange('+idx+',vain[0],vain[1],vain[2],vain[3])">';

	s+='<option value=0'+v[0]+'>Normal Input';

	if(inx[0] == 0)
		s+='<option value=1'+v[1]+'>Door 1';
	else if(inx[0] == 1)
		s+='<option value=1'+v[1]+'>Gate 1';
	else if(inx[0] == 4)
		s+='<option value=1'+v[1]+'>Relay 1';

	if(inx[1] == 0)
		s+='<option value=2'+v[2]+'>Door 2';
	else if(inx[1] == 1)
		s+='<option value=2'+v[2]+'>Gate 2';
	else if(inx[1] == 4)
		s+='<option value=2'+v[2]+'>Relay 2';

	if(inx[2] == 0)
		s+='<option value=3'+v[3]+'>Door 3';
	else if(inx[2] == 1)
		s+='<option value=3'+v[3]+'>Gate 3';
	else if(inx[2] == 4)
		s+='<option value=3'+v[3]+'>Relay 3';

	if(inx[3] == 0)
		s+='<option value=4'+v[4]+'>Door 4';
	else if(inx[3] == 1)
		s+='<option value=4'+v[4]+'>Gate 4';
	else if(inx[3] == 4)
		s+='<option value=4'+v[4]+'>Relay 4';

	s+='<option value=5'+v[5]+'>All Zone';
	
	s+='</select></nobr>';
    document.write(s);

}
//------------------------------------------------------------------------
//	ivalue:	0: no function, 1:choice 1, 2:choice 2, 3:choice 3, 4:choice 4
//	i2:	[0,1,4] 		which one, 0:door, 1:Parking, 4:Elevator
//------------------------------------------------------------------------
function SelectEventInput(idx,ivalue,ifu,i2)
{
	var s,v0,v1,v2,v3,v4,v5;

	v0=' ';	v1=' ';	v2=' ';	v3=' ';	v4=' ';v5=' ';

	if(ivalue == 1)			v1 = ' selected';	else if(ivalue == 2)	v2 = ' selected';	else if(ivalue == 3)	v3 = ' selected';
	else if(ivalue == 4)	v4 = ' selected';	else if(ivalue == 5)	v5 = ' selected';	else					v0 = ' selected';

	s='<nobr><select id=EventInput'+idx+' name=EventInput'+idx+' size=1>';

	if((i2 == 0) )		//	No function or normal input
		s+='<option value=0'+v0+'> ';
	else if( (i2 == 5) ){
		s+='<option value=1'+v1+'>Fire Sensor';
	}
	else{
		if(ifu[i2-1] == 0){
			s+='<option value=1'+v1+'>Fire Sensor';
			s+='<option value=2'+v2+'>Tamper Sensor';
			s+='<option value=3'+v3+'>Exit Button';
			s+='<option value=4'+v4+'>Door Contact';
			s+='<option value=5'+v5+'>Armed status ';
		}
		else if(ifu[i2-1] == 1){
			s+='<option value=1'+v1+'>Fire Sensor';
			s+='<option value=2'+v2+'>Tamper Sensor';
			s+='<option value=3'+v3+'>Exit Button';
			s+='<option value=4'+v4+'>Car Detection';
			s+='<option value=5'+v5+'>Armed status';
		}
		else if(ifu[i2-1] == 4){
			s+='<option value=1'+v1+'>Fire Sensor';
			s+='<option value=2'+v2+'>Tamper Sensor';
		}
	}
	s+='</select></nobr>';
    document.write(s);

	var a = document.getElementById('EventInput'+idx);
	var Fi = document.getElementById('FuncInput'+idx);

	if(Fi.selectedIndex == 0)
		a.style.display = "none";
	else
		a.style.display = "inline";

}
//-------------------------------------------------------
//	inx	= 0:door, 1:Parking, 4:Elevator, 9:no function
//-------------------------------------------------------
function EventInputChange(idx,ina,inb,inc,ind)
{
	var i;
	var NewSe02=document.createElement('option');
	var NewSe03=document.createElement('option');
	var NewSe04=document.createElement('option');
	var NewSe05=document.createElement('option');
	var NewSe06=document.createElement('option');

	var a = document.getElementById('EventInput'+idx);
	var b = document.getElementById('LatchInput'+idx);
	var Fi = document.getElementById('FuncInput'+idx);

	for(i=a.length-1;i>=0;i--)
		a.remove(i);

	for(i=b.length-1;i>=0;i--)
		b.remove(i);

	NewSe02.text = "Fire Sensor";	NewSe02.value = 1;
	NewSe03.text = "Tamper Sensor";	NewSe03.value = 2;
	NewSe04.text = "Exit Button";	NewSe04.value = 3;
	NewSe06.text = "Armed status";	NewSe06.value = 5;
	if(Fi.selectedIndex == 1)
	{
		a.style.display = "inline";
		b.style.display = "none";
		if(ina == 0)	//	Door
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Door Contact";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(ina == 1)	//	Car
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Car Detection";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(ina == 4)
		{
			a.add(NewSe02);						a.add(NewSe03);
		}
	}
	else if(Fi.selectedIndex == 2)
	{
		a.style.display = "inline";
		b.style.display = "none";
		if(inb == 0)	//	Door
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Door Contact";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(inb == 1)	//	Car
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Car Detection";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(inb == 4)
		{
			a.add(NewSe02);						a.add(NewSe03);
		}
	}
	else if(Fi.selectedIndex == 3)
	{
		a.style.display = "inline";
		b.style.display = "none";
		if(inc == 0)	//	Door
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Door Contact";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(inc == 1)	//	Car
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Car Detection";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(inc == 4)
		{
			a.add(NewSe02);						a.add(NewSe03);
		}
	}
	else if(Fi.selectedIndex == 4)
	{
		a.style.display = "inline";
		b.style.display = "none";
		if(ind == 0)	//	Door
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Door Contact";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(ind == 1)	//	Car
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Car Detection";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(ind == 4)
		{
			a.add(NewSe02);						a.add(NewSe03);
		}
	}
	else if(Fi.selectedIndex == 5)
	{
		a.style.display = "inline";
		b.style.display = "none";
		a.add(NewSe02);
	}
	else		//		0
	{
		NewSe02.text = "Latch Disable";		NewSe02.value = 0;
		NewSe03.text = "Latch Enable";		NewSe03.value = 1;
		b.add(NewSe02);
		b.add(NewSe03);
		a.style.display = "none";
		b.style.display = "inline";
	}
}

function InnerLatch(idx, fuuc, En)
{
	var s,v0,v1;

	v0=' ';	v1=' ';

	if(En == 1)			v1 = ' selected';
	else				v0 = ' selected';

	s='<nobr><select id=LatchInput'+idx+' name=LatchInput'+idx+' size=1><option value=0'+v0+'>Latch Disable';
	s+='<option value=1'+v1+'>Latch Enable</nobr>';
	document.write(s);

	var a = document.getElementById('LatchInput'+idx);
	var Fi = document.getElementById('FuncInput'+idx);

	if(Fi.selectedIndex == 0)
		a.style.display = "inline";
	else
		a.style.display = "none";
}

