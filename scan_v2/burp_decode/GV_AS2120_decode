	else if(ione == 1)		v1=' selected';
	else if(ione == 2)		v2=' selected';
	else if(ione == 3)		v3=' selected';
	else if(ione == 4)		v4=' selected';
	else if(ione == 5)		v5=' selected';
	else if(ione == 6)		v6=' selected';
	else if(ione == 7)		v7=' selected';
	else 					v8=' selected';

	s='<nobr><select id=ASReader'+idx+' name=ASReader'+idx+' size=1>';

	s+='<option value=0'+v0+'>No Function';
	s+='<option value=1'+v1+'>Door/Gate 1 Entry';
	s+='<option value=2'+v2+'>Door/Gate 1 Exit';
	s+='<option value=3'+v3+'>Door/Gate 2 Entry';
	s+='<option value=4'+v4+'>Door/Gate 2 Exit';
	s+='<option value=5'+v5+'>Door/Gate 3 Entry';
	s+='<option value=6'+v6+'>Door/Gate 3 Exit';
	s+='<option value=7'+v7+'>Door/Gate 4 Entry';
	s+='<option value=8'+v8+'>Door/Gate 4 Exit';
