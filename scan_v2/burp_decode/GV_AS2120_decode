function DrawIpBox(sFormName, sIpName, sValue)
{
	var i,s,v;
	v=sValue.split('.',4);
	for(i=0;i<4;i++)
	{
		if(i<v.length) v[i]=TrimString(v[i]).substr(0,3);
		else v[i]='';
	}
	s='<nobr>';
	for(i=0;i<4;i++)
	{
		s+='<input name=IP_'+sIpName+' value="'+v[i]+'" type=text size=3 maxlength=3 class='+sClassInputText+' onkeypress="return IpKeyPress(\''+sFormName+'\', \''+sIpName+'\', '+i+', event);" onkeydown="return IpKeyDown(\''+sFormName+'\', \''+sIpName+'\', '+i+', event);" onkeyup="return IpKeyUp(\''+sFormName+'\', \''+sIpName+'\', '+i+', event);" onfocus="this.select();" onblur="return IpBlur(this);">';
		if(i!=3) s+='.';
	}
	s+='</nobr>';
	document.write(s);
}

function GetIp(sFormName, sIpName)
{
	var i,s,s1,o;
	o = document[sFormName]['IP_'+sIpName];
	for(s='',i=0;i<4;i++)
	{
		s1=TrimString(o[i].value);
		if(s1.length==0) return '';
		s1=parseInt(s1, 10);
		if(isNaN(s1)) return '';
		s+=s1;
		if(i!=3) s+='.';
	}
	return s;
}

function SetEditMask(sFormName, sIpName, sMask)
{
	var i,o;
	o = document[sFormName]['IP_'+sIpName];

	for(i=0;i<4;i++)
	{
		if(i<sMask.length && sMask.charAt(i)=='0')
		{
			if (o[i].style)
				o[i].style.color='gray';
			o[i].readOnly = true;
		}
		else
		{
			if (o[i].style)
				o[i].style.color='';
			o[i].readOnly = false;
		}
	}
}

function IpKeyUp(sFormName, sIpName, iWhich, event)
{
	var key;
	if (iBrowser == 0) key = event.keyCode;
	else if(iBrowser == 1) key = event.which;
	else return true;

	if(key>=48 && key<=57 && document[sFormName]['IP_'+sIpName][iWhich].value.length==3)
		IpBlur(document[sFormName]['IP_'+sIpName][iWhich]);
}

function IpKeyPress(sFormName, sIpName, iWhich, event)
{
	var key;
	if (iBrowser == 0) key = event.keyCode;
	else if(iBrowser == 1) key = event.which;
	else return true;

	if(key>=48 && key<=57 || key==0) return true;
	else if(key==8 && iBrowser==1)
	{
		if(iWhich>0 && document[sFormName]['IP_'+sIpName][iWhich].value.length==0)
		{
			document[sFormName]['IP_'+sIpName][iWhich-1].focus();
			return false;
		}
		else return true;
	}
	if((key==46 || key==32)&& iWhich<3)
	{
		document[sFormName]['IP_'+sIpName][iWhich+1].focus();
		return false;
	}
	return false;
}

function IpKeyDown(sFormName, sIpName, iWhich, event)
{
	var key;
	if (iBrowser == 0) key = event.keyCode;
	else if(iBrowser == 1) key = event.which;
	else return true;

	if(key==37 && iWhich>0)
		document[sFormName]['IP_'+sIpName][iWhich-1].focus();
	else if(key==39 && iWhich<3)
		document[sFormName]['IP_'+sIpName][iWhich+1].focus();
	else if(key==8 && iBrowser==0)
	{
		if(iWhich>0 && document[sFormName]['IP_'+sIpName][iWhich].value.length==0)
			document[sFormName]['IP_'+sIpName][iWhich-1].focus();
	}
	return true;
}

function IpBlur(o)
{
	if(TrimString(o.value).length==0)
	{
		o.value='';
		return;
	}
	var i;
	i = parseInt(o.value, 10);
	if(isNaN(i)) o.value='';
	else if(i<=0) o.value=0;
	else o.value=i;
}

function TrimString(s)
{
	var i,j;
	for(i=0;i<s.length;i++)
		if(s.charCodeAt(i)>32) break;
	for(j=s.length-1;j>=0;j--)
		if(s.charCodeAt(j)>32) break;
	if(i>j) return '';
	else return s.substring(i,j+1);
}

function DrawNumBox(sFormName, sNumName, iValue, iLength, iMaxLength)
{
	var s;
	s='<input name=Num_'+sNumName+' value="'+iValue+'" type=text size='+iLength+' maxlength='+iMaxLength+' class='+sClassInputText+' onfocus="this.select();" onkeypress="return NumKeyPress(this, event);">';
	document.write(s);
}

function DrawIpNumBox(sFormName, sIpName, iValue)
{
	var s;
	s='<input name=Num_'+sIpName+' value="'+iValue+'" type=text size=3 maxlength=3 class='+sClassInputText+' onfocus="this.select();" onkeypress="return NumKeyPress(this, event);" onkeyup="return IpNumKeyUp(this, event);" onblur="return IpBlur(this);">';
	document.write(s);
}

function IpNumKeyUp(o, event)
{
	var key;
	if (iBrowser == 0) key = event.keyCode;
	else if(iBrowser == 1) key = event.which;
	else return true;

	if(key>=48 && key<=57 && o.value.length==3)
		IpBlur(o);
}

function GetNum(sFormName, sNumName)
{
	var rt;
	rt=parseInt(document[sFormName]['Num_'+sNumName].value, 10);
	if(isNaN(rt)) return '';
	else return rt;
}

function NumKeyPress(o, event)
{
	var key;
	if (iBrowser == 0) key = event.keyCode;
	else if(iBrowser == 1) key = event.which;
	else return true;

	if(key>=48 && key<=57 || key==0 || key==8)
		return true;
	else return false;
}

function DrawTxtBox(sFormName, sTxtName, sValue, iLength, iMaxLength, iType)
{
	var s;
	s='<input type='+(iType==1?'password':'text')+' name=Txt_'+sTxtName+' value="'+sValue+'" size='+iLength+' maxlength='+iMaxLength+' class='+sClassInputText+' onfocus="this.select();">';
	document.write(s);
}

function GetTxt(sFormName, sTxtName)
{
	return document[sFormName]['Txt_'+sTxtName].value;
}

function IP2Uint(sIP)
{
	var i, aIP;
	if (sIP.length == 0) return 0;
	var aIP = sIP.split('.', 4);
	if (aIP.length != 4) return 0;
	for (i=0; i<4; i++) aIP[i] = aIP[i] - 0;
	return ((aIP[0]*256+aIP[1])*256+aIP[2])*256+aIP[3];
}

function GetRadio(sFormName, sRadioName)
{

	for (i=0; i<document[sFormName][sRadioName].length; i++)
		if (document[sFormName][sRadioName][i].checked) return document[sFormName][sRadioName][i].value;
	return 'on';
}
