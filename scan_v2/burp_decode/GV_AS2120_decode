		</tr>
		<tr>
			<td align=center><h4>Device Name:</h4></td>
			<td><script>DrawTxtBox('CF','MyName',va_MyName,30,32,0);</script></td>
		</tr>
		<tr>
			<td><h4><script>ChkDDNSMode(3,va_SendtoServer);</script></h4></td>
			<td><script>DrawDDNSselect(va_ddns);</script><input type="button" value="GeoVision DDNS" onClick="return GeoReg();"></td>
		</tr>
		<tr>
			<td align=center><h4>Host Name:</h4></td>
			<td><script>DrawTxtBox('CF','HostName',va_HostName,30,50,0);Chklimit();</script></td>
		</tr>
		<tr>
			<td align=center><h4>User Name:</h4></td>
			<td><script>DrawTxtBox('CF','UserName',va_UserName,30,32,0);</script></td>
		</tr>
		<tr>
			<td align=center><h4>Password:</h4></td>
			<td><script>DrawTxtBox('CF','DDNSPasswd',va_DDNSPasswd,30,32,1);</script></td>
		</tr>
		<tr>
			<td><h4>Status:</h4></td>
			<td><h4><script>document.write(va_ConnectStatus);</script></h4></td>
		</tr>
	</table>

