function ShowTitle(num,in1,in2)
{
	var s,a,b;

	s = ' ';	a= ' ';	b=' ';

	if(in2 != 0)
	{
		if(in1 == 1)	a = 'Door 1';	else if(in1 == 2)	a = 'Gate 1';	else	a = 'Relay 1';
		if(in2 == 1)	b = 'Door 2';	else if(in2 == 2)	b = 'Gate 2';	else	b = 'Relay 2';
		if(num == 2)
			s = '<tr><td width=180><h4>'+a+'</h4></td><td width=200></td><td width=150><h4>'+b+'</h4></td></tr>';
	}
	else
		s = '<tr><td width=200></td><td width=370></td><td width=50></td><td width=50></td></tr>';

	document.write(s);
}
function ShowTitle2(num,in1,in2)
{
	var s,a,b;

	s = ' ';	a= ' ';	b=' ';

	if(in2 != 0)
	{
		if(in1 == 1)	a = 'Door 3';	else if(in1 == 2)	a = 'Gate 3';	else	a = 'Relay 3';
		if(in2 == 1)	b = 'Door 4';	else if(in2 == 2)	b = 'Gate 4';	else	b = 'Relay 4';
		if(num == 2)
			s = '<tr><td width=180><h4>'+a+'</h4></td><td width=200></td><td width=150><h4>'+b+'</h4></td></tr>';
	}
	else
		s = '<tr><td width=200></td><td width=370></td><td width=50></td><td width=50></td></tr>';

	document.write(s);
}
function ShowEvent(num1,num2,num)
{
	var s;

	s = ' ';

		if(num == 3)	//	Elevator
		{
			if(num2 == 1)
				s = 'Relay On Time';
			else if(num2 == 2)
				s = 'Fire Action';
			else if(num2 == 3)
				s = 'Alarm Continuous Time';
		}
		else if(num == 2)	//	Parking
		{
			if(num2 == 1)
				s = 'Anti-passback';
			else if(num2 == 2)
				s = 'Relay On Time';
			else if(num2 == 3)
				s = 'Held Open Time';
			else if(num2 == 4)
				s = 'Fire Action';
			else if(num2 == 5)
				s = 'Alarm Continuous Time';
		}
		else if(num == 1)	//	Door
		{
			if(num2 == 1)
				s = 'Anti-passback';
			else if(num2 == 2)
				s = 'Lock Reset Time';
			else if(num2 == 3)
				s = 'Held Open Time';
			else if(num2 == 4)
				s = 'Fire Action';
			else if(num2 == 5)
				s = 'Alarm Continuous Time';
		}
	document.write(s);
}

//	num1 mean A or B;	num2 means 1-5 index;	num means which function, 1:door,2:parking,3:elevator
function SelectEvent(num1,num2,num,value)
{
	var s,v0,v1,v2;

	v0=' '; v1=' '; v2=' ';
	s = ' ';

	if(num == 3)			//	elevator
	{
		if(num2 == 1)		//	Relay ON Time
			s='<input name=Event'+num1+num2+' value="'+value+'"type=text size=5 maxlength=3 class='+sClassInputText+' onfocus="this.select();" onkeyup="isnum(this);"> (1~600)';
		else if(num2 == 3)	//	Alarm Continuous Time
			s='<input name=Event'+num1+num2+' value="'+value+'"type=text size=5 maxlength=3 class='+sClassInputText+' onfocus="this.select();" onkeyup="isnum(this);"> (1~10)';
		else if(num2 == 2)
		{
			if(value == 0)
				v0=' selected';
			else if(value == 1)
				v1=' selected';
			else
				v2=' selected';
			s='<nobr>';
			s+='<select name=Event'+num1+num2+' size=1><option value=0'+v0+'>Unchange<option value=1'+v1+'>Unlock<option value=2'+v2+'>Lock</select></nobr>';
		}
	}
	else
	{
		if((num == 2) || (num == 1))	//	Parking or Door
		{
			if(num2 == 1)			//	Anti-passback
			{
				if(value == 0)
					v0=' selected';
				else
					v1=' selected';
				s='<nobr>';
				s+='<select name=Event'+num1+num2+' size=1><option value=0'+v0+'>NO<option value=1'+v1+'>YES</select></nobr>';
			}
			else if(num2 == 2)		//	Lock Reset Time
			{
				s='<input name=Event'+num1+num2+' value="'+value+'" type=text size=5 maxlength=3 class='+sClassInputText+' onfocus="this.select();" onkeyup="isnum(this);"> (1~600)';
			}
			else if(num2 == 5)		//	Alarm Continuous Time
				s='<input name=Event'+num1+num2+' value="'+value+'" type=text size=5 maxlength=3 class='+sClassInputText+' onfocus="this.select();" onkeyup="isnum(this);"> (1~10)';
			else if(num2 == 3)		//	Held Open Time
			{
				s='<input name=Event'+num1+num2+' value="'+value+'" type=text size=5 maxlength=4 class='+sClassInputText+' onfocus="this.select();" onkeyup="isnum(this);"> (5~9999)';
			}
			else if(num2 == 4)
			{
				if(value == 0)
					v0=' selected';
				else if(value == 1)
					v1=' selected';
				else
					v2=' selected';
				s='<nobr>';
				s+='<select name=Event'+num1+num2+' size=1><option value=0'+v0+'>Unchange<option value=1'+v1+'>Unlock<option value=2'+v2+'>Lock</select></nobr>';
			}
		}
	}
	document.write(s);
}

function ShowAlarm(num,indata)
{
	var s;

	if(indata == 3)
	{
		if(num == 1)
			s = 'Fire Alarm';
		else if(num == 2)
			s = 'Access Denied';
		else if(num == 3)
			s = 'Tamper';
		else
			s = ' ';
		document.write(s);
	}
	else if((indata == 2) || (indata == 1))
	{
		if(num == 1)
			s = 'Held Open';
		else if(num == 2 && indata==1)
			s = 'Forced Open';
		else if(num == 3)
			s = 'Fire Alarm';
		else if(num == 4)
			s = 'Access Denied';
		else if(num == 5)
			s = 'Tamper';
		else if(num == 6 && indata==1)
			s = 'Tailgating';
		else
			s = ' ';
		document.write(s);
	}
}

function SelectAlarm(num1,num2,able,value)
{
	var s,v0,v1;

	v0=' '; v1=' ';

	if(value == 0)
		v0=' selected';
	else
		v1=' selected';

	if(able == 3)
	{
		if(num2 <= 3)
		{
			s='<nobr>';
			s+='<select name=Alarm'+num1+num2+' size=1><option value=0'+v0+'>NO<option value=1'+v1+'>YES</select></nobr>';
			document.write(s);
		}
	}
	else if((able == 2) && (num2 == 2))
	{
		s='<input name=Alarm'+num1+num2+' value="'+value+'"type=hidden size=5 maxlength=3 class='+sClassInputText+' >';
		document.write(s);
	}
	else if((able == 2) && (num2 == 6))
	{
		s='<input name=Alarm'+num1+num2+' value="'+value+'"type=hidden size=5 maxlength=3 class='+sClassInputText+' >';
		document.write(s);
	}
	else if((able == 2) || (able == 1))
	{
		s='<nobr>';
		s+='<select name=Alarm'+num1+num2+' size=1><option value=0'+v0+'>NO<option value=1'+v1+'>YES</select></nobr>';
		document.write(s);
	}
}

