var Debug = false;
var iBrowser = 0;
var fVer = 0;

if (navigator.appName.toUpperCase() == 'MICROSOFT INTERNET EXPLORER') iBrowser = 0;
else if (navigator.appName.toUpperCase() == 'NETSCAPE') iBrowser = 1;
else if (navigator.appName.toUpperCase() == 'KONQUEROR') iBrowser = 2;
else iBrowser = 3;
fVer = parseFloat(navigator.appVersion);

var sClassInputText = null;
if (fVer < 5.0 && iBrowser == 1) 
	sClassInputText = 'InputTextNoAlign';
else 
	sClassInputText = 'InputText';

function DrawButtom(inum)
{
	var	s;
	if(inum == 1)
	{
		s='<input type=submit value=Submit name=Submit style="font-family: Verdana; font-weight:bold"><input type=reset value=Cancel name=Cancel style="font-family: Verdana; font-weight:bold">';
		document.write(s);
	}
}

function OutputSelect(index,num)
{
	var s,v1,v2;
	
	v1='';
	v2='';
	
	if(num == 4)
		v1 = ' selected';
	else
		v2 = ' selected';

    s='<nobr>';
    s+='<select name=OutputNum'+index+' size=1 > <option value=4'+v1+'>4 <option value=16'+v2+'>16 </select></nobr>'
    document.write(s);
}

