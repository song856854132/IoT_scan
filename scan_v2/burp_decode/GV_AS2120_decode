	s+='<select name="DDNS" size="1" onchange="return byDDNS()"><option value=1'+v1+'>GeoVision DDNS</option><option value=3'+v3+'>GeoVision DDNS V2</option><option value=2'+v2+'>DynDNS.org</option></select></nobr>'
    document.write(s);

}

function DrawRadio(sNumName, sMask)
{
	var s,v1,v2;

	v1 = ' ';	
	v2 = ' ';

	if(sMask == 1)
		v1 = ' checked';
	else
		v2 = ' checked';

	s='<nobr>';
	s+='<input type=radio name=WR_'+sNumName+' value=1'+v1+'>Enable<input type=radio name=WR_'+sNumName+' value=0'+v2+'>Disable</nobr>';

	document.write(s);
}

function DrawSelectBox(sFormName, sNumName, sMask)
{
	var s,var1,var2,var3,var4,var5,var6,var7,var8;
	
	var1 = ' ';
	var2 = ' ';
	var3 = ' ';
	var4 = ' ';
	var5 = ' ';
	var6 = ' ';
	var7 = ' ';
	var8 = ' ';

	if(sMask == 1)
		var1 = ' selected';
	else if(sMask == 2)
		var2 = ' selected';
	else if(sMask == 3)
		var3 = ' selected';
	else if(sMask == 4)
		var4 = ' selected';
	else if(sMask == 5)
		var5 = ' selected';
	else if(sMask == 6)
		var6 = ' selected';
	else if(sMask == 7)
		var7 = ' selected';
	else 
		var8 = ' selected';
	
	s='<nobr>';
	s+='<select name=Num_'+sNumName+' size=1><option value=1'+var1+'>1200<option value=2'+var2+'>2400<option value=3'+var3+'>4800<option value=4'+var4+'>9600<option value=5'+var5+'>19200<option value=6'+var6+'>38400<option value=7'+var7+'>57600<option value=8'+var8+'>115200</select></nobr>'
	document.write(s);
}

function DrawIpBox(sFormName, sIpName, sValue)
{
	var i,s,v;
	v=sValue.split('.',4);
	for(i=0;i<4;i++)
	{
		if(i<v.length) v[i]=TrimString(v[i]).substr(0,3);
		else v[i]='';
	}
	s='<nobr>';
	for(i=0;i<4;i++)
	{
		s+='<input name=IP_'+sIpName+' value="'+v[i]+'" type=text size=3 maxlength=3 class='+sClassInputText+' onkeypress="return IpKeyPress(\''+sFormName+'\', \''+sIpName+'\', '+i+', event);" onkeydown="return IpKeyDown(\''+sFormName+'\', \''+sIpName+'\', '+i+', event);" onkeyup="return IpKeyUp(\''+sFormName+'\', \''+sIpName+'\', '+i+', event);" onfocus="this.select();" onblur="return IpBlur(this);">';
		if(i!=3) s+='.';
	}
	s+='</nobr>';
	document.write(s);
}

function GetIp(sFormName, sIpName)
{
	var i,s,s1,o;
	o = document[sFormName]['IP_'+sIpName];
	for(s='',i=0;i<4;i++)
	{
		s1=TrimString(o[i].value);
		if(s1.length==0) return '';
		s1=parseInt(s1, 10);
		if(isNaN(s1)) return '';
		s+=s1;
		if(i!=3) s+='.';
	}
	return s;
}

function SetEditMask(sFormName, sIpName, sMask)
{
	var i,o;
	o = document[sFormName]['IP_'+sIpName];

	for(i=0;i<4;i++)
	{
		if(i<sMask.length && sMask.charAt(i)=='0')
		{
			if (o[i].style)
				o[i].style.color='gray';
			o[i].readOnly = true;
		}
		else
		{
			if (o[i].style)
				o[i].style.color='';
			o[i].readOnly = false;
		}
	}
}

function IpKeyUp(sFormName, sIpName, iWhich, event)
{
	var key;
	if (iBrowser == 0) key = event.keyCode;
	else if(iBrowser == 1) key = event.which;
	else return true;

	if(key>=48 && key<=57 && document[sFormName]['IP_'+sIpName][iWhich].value.length==3)
		IpBlur(document[sFormName]['IP_'+sIpName][iWhich]);
}

function IpKeyPress(sFormName, sIpName, iWhich, event)
{
	var key;
	if (iBrowser == 0) key = event.keyCode;
	else if(iBrowser == 1) key = event.which;
	else return true;

	if(key>=48 && key<=57 || key==0) return true;
	else if(key==8 && iBrowser==1)
	{
		if(iWhich>0 && document[sFormName]['IP_'+sIpName][iWhich].value.length==0)
		{
			document[sFormName]['IP_'+sIpName][iWhich-1].focus();
			return false;
		}
		else return true;
	}
	if((key==46 || key==32)&& iWhich<3)
	{
		document[sFormName]['IP_'+sIpName][iWhich+1].focus();
		return false;
	}
	return false;
}

function IpKeyDown(sFormName, sIpName, iWhich, event)
{
	var key;
	if (iBrowser == 0) key = event.keyCode;
	else if(iBrowser == 1) key = event.which;
	else return true;

	if(key==37 && iWhich>0)
		document[sFormName]['IP_'+sIpName][iWhich-1].focus();
	else if(key==39 && iWhich<3)
		document[sFormName]['IP_'+sIpName][iWhich+1].focus();
	else if(key==8 && iBrowser==0)
	{
		if(iWhich>0 && document[sFormName]['IP_'+sIpName][iWhich].value.length==0)
			document[sFormName]['IP_'+sIpName][iWhich-1].focus();
	}
	return true;
}

function IpBlur(o)
{
	if(TrimString(o.value).length==0)
	{
		o.value='';
		return;
	}
	var i;
	i = parseInt(o.value, 10);
	if(isNaN(i)) o.value='';
	else if(i<=0) o.value=0;
	else o.value=i;
}

function TrimString(s)
{
	var i,j;
	for(i=0;i<s.length;i++)
		if(s.charCodeAt(i)>32) break;
	for(j=s.length-1;j>=0;j--)
		if(s.charCodeAt(j)>32) break;
	if(i>j) return '';
	else return s.substring(i,j+1);
}

function DrawNumBox(sFormName, sNumName, iValue, iLength, iMaxLength)
{
	var s;
	s='<input name=Num_'+sNumName+' value="'+iValue+'" type=text size='+iLength+' maxlength='+iMaxLength+' class='+sClassInputText+' onfocus="this.select();" onkeypress="return NumKeyPress(this, event);">';
	document.write(s);
}

function DrawIpNumBox(sFormName, sIpName, iValue)
{
	var s;
	s='<input name=Num_'+sIpName+' value="'+iValue+'" type=text size=3 maxlength=3 class='+sClassInputText+' onfocus="this.select();" onkeypress="return NumKeyPress(this, event);" onkeyup="return IpNumKeyUp(this, event);" onblur="return IpBlur(this);">';
	document.write(s);
}

function IpNumKeyUp(o, event)
{
	var key;
	if (iBrowser == 0) key = event.keyCode;
	else if(iBrowser == 1) key = event.which;
	else return true;

	if(key>=48 && key<=57 && o.value.length==3)
		IpBlur(o);
}

function GetNum(sFormName, sNumName)
{
	var rt;
	rt=parseInt(document[sFormName]['Num_'+sNumName].value, 10);
	if(isNaN(rt)) return '';
	else return rt;
}

function NumKeyPress(o, event)
{
	var key;
	if (iBrowser == 0) key = event.keyCode;
	else if(iBrowser == 1) key = event.which;
	else return true;

	if(key>=48 && key<=57 || key==0 || key==8)
		return true;
	else return false;
}

function DrawTxtBox(sFormName, sTxtName, sValue, iLength, iMaxLength, iType)
{
	var s;
	s='<input type='+(iType==1?'password':'text')+' name=Txt_'+sTxtName+' value="'+sValue+'" size='+iLength+' maxlength='+iMaxLength+' class='+sClassInputText+' onfocus="this.select();">';
	document.write(s);
}

function GetTxt(sFormName, sTxtName)
{
	return document[sFormName]['Txt_'+sTxtName].value;
}

function IP2Uint(sIP)
{
	var i, aIP;
	if (sIP.length == 0) return 0;
	var aIP = sIP.split('.', 4);
	if (aIP.length != 4) return 0;
	for (i=0; i<4; i++) aIP[i] = aIP[i] - 0;
	return ((aIP[0]*256+aIP[1])*256+aIP[2])*256+aIP[3];
}

function GetRadio(sFormName, sRadioName)
{

	for (i=0; i<document[sFormName][sRadioName].length; i++)
		if (document[sFormName][sRadioName][i].checked) return document[sFormName][sRadioName][i].value;
	return 'on';
}
/*********************************************************************************/
//ASFunc.htm used
/*********************************************************************************/
function byBoxWie()
{
	if(document.CF.ASBoxwie.selectedIndex == 3 )
	{
		document.CF.ControlType2.disabled = false;
		document.CF.AuthMode2.disabled = false;
	}
	else
	{
		document.CF.ControlType2.disabled = true;
		document.CF.AuthMode2.disabled = true;
	}
}

function byWorkMode()
{
	if(document.CF.WorkMode.selectedIndex == 0)
	{
		document.CF.Anti.selectedIndex = 0;
		document.CF.Anti.disabled = true;
	}
	else
		document.CF.Anti.disabled = false;
}

function SetEditMask(sFormName, sIpName, sMask)
{
	var i,o;
	o = document[sFormName]['IP_'+sIpName];

	for(i=0;i<4;i++)
	{
		if(i<sMask.length && sMask.charAt(i)=='0')
		{
			if (o[i].style)
				o[i].style.color='gray';
			o[i].readOnly = true;
		}
		else
		{
			if (o[i].style)
				o[i].style.color='';
			o[i].readOnly = false;
		}
	}
}

function SelectBoxwie(indata)
{
	var s,v0,v1,v2,v3;

	v0=' ';v1=' '; v2=' '; v3=' ';

	if(indata == 0)
		v0 = ' selected';
	else if(indata == 1)
		v1 = ' selected';
	else if(indata == 2)
		v2 = ' selected';
	else
		v3 = ' selected';

	s='<nobr>';
	s+='<select name=ASBoxwie size=1 onchange="return byBoxWie()"><option value=0'+v0+'>No Function<option value=1'+v1+'>Door/Gate A Entry<option value=2'+v2+'>Door/Gate A Exit<option value=3'+v3+'>Door/Gate B</select></nobr>';
	document.write(s);
}

function SelectFunc(num,indata)
{
	var s,v0,v1,v2,v3,v4;

	v0=' '; v1=' ';	v2=' ';	v3=' ';	v4= ' ';

	if(indata == 0)
		v0 = ' selected';
	else if(indata == 1)
		v1 = ' selected';
	else
		v4 = ' selected';

	s='<nobr>';
	s+='<select name=ControlType'+num+' size=1><option value=0'+v0+'>Door Control<option value=1'+v1+'>Parking Control<option value=4'+v4+'>Elevator Control</select></nobr>';
	document.write(s);
}

function SelectAuth(num,mode)
{
    var s,v1,v2,v3,v4,v5,v6;

    v1 = ' ';	v2 = ' ';	v3 = ' ';
    v4 = ' ';	v5 = ' ';	v6 = ' ';

    if(mode == 5)
        v1 = ' selected';
    else if(mode == 8)
        v2 = ' selected';
    else if(mode == 10)
        v3 = ' selected';
    else if(mode == 11)
        v4 = ' selected';
	else if(mode == 12)
        v5 = ' selected';
	else
        v6 = ' selected';

    s='<nobr>';
    s+='<select name=AuthMode'+num+' size=1 ><option value=5'+v1+'>Local Unlock Mode<option value=8'+v2+'>Local Lock Mode<option value=10'+v3+'>Fixed Card Mode<option value=11'+v4+'>Fixed Card+PIN Mode<option value=12'+v5+'>Fixed Card/Common Mode<option value=255'+v6+'>Authentication Schedule Mode</select></nobr>';
    document.write(s);
}

