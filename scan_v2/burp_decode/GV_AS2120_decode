	var s;
	s='<input style="font-size:9pt" type='+(iType==1?'password':'text')+' name=Txt_'+sTxtName+' value="'+sValue+'" size='+iLength+' maxlength='+iMaxLength+' class='+sClassInputText+' onfocus="this.select();">';
	document.write(s);
}

function NCNO(num,indata)
{
	var s,v0,v1;

	v0=' '; v1=' ';

	if(indata == 0)
		v0 = ' selected';
	else
		v1 = ' selected';
	s='<nobr>';
	s+='<select name=InType'+num+' size=1><option value=0'+v0+'>NC<option value=1'+v1+'>NO</select></nobr>';
	document.write(s);
}
//-------------------------------------------------------------
//	idx = 1-16;
//	isele = 0: No Function 1:A, 2:B, 3:C, 4:D, 5:Normal Input ;
//	inx	= 0:door, 1:Parking, 4:Elevator, 9:no function
//------------------------------------------------------------
function SelectFuncInput(idx,isele,inx)
{
	var s;
	var v = new Array(' ',' ',' ',' ',' ',' ');

	if(isele == 1)			v[1]=' selected';	else if(isele == 2)		v[2]=' selected';	else if(isele == 3)		v[3]=' selected';
	else if(isele == 4)		v[4]=' selected';	else if(isele == 5)		v[5]=' selected';	else 					v[0]=' selected';

	s='<nobr>';
	s+='<select id=FuncInput'+idx+' name=FuncInput'+idx+' size=1 onchange="return EventInputChange('+idx+',vain[0],vain[1],vain[2],vain[3])">';

	s+='<option value=0'+v[0]+'>Normal Input';

	if(inx[0] == 0)
		s+='<option value=1'+v[1]+'>Door 1';
	else if(inx[0] == 1)
		s+='<option value=1'+v[1]+'>Gate 1';
	else if(inx[0] == 4)
		s+='<option value=1'+v[1]+'>Relay 1';

	if(inx[1] == 0)
		s+='<option value=2'+v[2]+'>Door 2';
	else if(inx[1] == 1)
		s+='<option value=2'+v[2]+'>Gate 2';
	else if(inx[1] == 4)
		s+='<option value=2'+v[2]+'>Relay 2';

	if(inx[2] == 0)
		s+='<option value=3'+v[3]+'>Door 3';
	else if(inx[2] == 1)
		s+='<option value=3'+v[3]+'>Gate 3';
	else if(inx[2] == 4)
		s+='<option value=3'+v[3]+'>Relay 3';

	if(inx[3] == 0)
		s+='<option value=4'+v[4]+'>Door 4';
	else if(inx[3] == 1)
		s+='<option value=4'+v[4]+'>Gate 4';
	else if(inx[3] == 4)
		s+='<option value=4'+v[4]+'>Relay 4';

	s+='<option value=5'+v[5]+'>All Zone';
	
	s+='</select></nobr>';
    document.write(s);

}
//------------------------------------------------------------------------
//	ivalue:	0: no function, 1:choice 1, 2:choice 2, 3:choice 3, 4:choice 4
//	i2:	[0,1,4] 		which one, 0:door, 1:Parking, 4:Elevator
//------------------------------------------------------------------------
function SelectEventInput(idx,ivalue,ifu,i2)
{
	var s,v0,v1,v2,v3,v4,v5;

	v0=' ';	v1=' ';	v2=' ';	v3=' ';	v4=' ';v5=' ';

	if(ivalue == 1)			v1 = ' selected';	else if(ivalue == 2)	v2 = ' selected';	else if(ivalue == 3)	v3 = ' selected';
	else if(ivalue == 4)	v4 = ' selected';	else if(ivalue == 5)	v5 = ' selected';	else					v0 = ' selected';

	s='<nobr><select id=EventInput'+idx+' name=EventInput'+idx+' size=1>';

	if((i2 == 0) )		//	No function or normal input
		s+='<option value=0'+v0+'> ';
	else if( (i2 == 5) ){
		s+='<option value=1'+v1+'>Fire Sensor';
	}
	else{
		if(ifu[i2-1] == 0){
			s+='<option value=1'+v1+'>Fire Sensor';
			s+='<option value=2'+v2+'>Tamper Sensor';
			s+='<option value=3'+v3+'>Exit Button';
			s+='<option value=4'+v4+'>Door Contact';
			s+='<option value=5'+v5+'>Armed status ';
		}
		else if(ifu[i2-1] == 1){
			s+='<option value=1'+v1+'>Fire Sensor';
			s+='<option value=2'+v2+'>Tamper Sensor';
			s+='<option value=3'+v3+'>Exit Button';
			s+='<option value=4'+v4+'>Car Detection';
			s+='<option value=5'+v5+'>Armed status';
		}
		else if(ifu[i2-1] == 4){
			s+='<option value=1'+v1+'>Fire Sensor';
			s+='<option value=2'+v2+'>Tamper Sensor';
		}
	}
	s+='</select></nobr>';
    document.write(s);

	var a = document.getElementById('EventInput'+idx);
	var Fi = document.getElementById('FuncInput'+idx);

	if(Fi.selectedIndex == 0)
		a.style.display = "none";
	else
		a.style.display = "inline";

}
//-------------------------------------------------------
//	inx	= 0:door, 1:Parking, 4:Elevator, 9:no function
//-------------------------------------------------------
function EventInputChange(idx,ina,inb,inc,ind)
{
	var i;
	var NewSe02=document.createElement('option');
	var NewSe03=document.createElement('option');
	var NewSe04=document.createElement('option');
	var NewSe05=document.createElement('option');
	var NewSe06=document.createElement('option');

	var a = document.getElementById('EventInput'+idx);
	var b = document.getElementById('LatchInput'+idx);
	var Fi = document.getElementById('FuncInput'+idx);

	for(i=a.length-1;i>=0;i--)
		a.remove(i);

	for(i=b.length-1;i>=0;i--)
		b.remove(i);

	NewSe02.text = "Fire Sensor";	NewSe02.value = 1;
	NewSe03.text = "Tamper Sensor";	NewSe03.value = 2;
	NewSe04.text = "Exit Button";	NewSe04.value = 3;
	NewSe06.text = "Armed status";	NewSe06.value = 5;
	if(Fi.selectedIndex == 1)
	{
		a.style.display = "inline";
		b.style.display = "none";
		if(ina == 0)	//	Door
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Door Contact";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(ina == 1)	//	Car
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Car Detection";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(ina == 4)
		{
			a.add(NewSe02);						a.add(NewSe03);
		}
	}
	else if(Fi.selectedIndex == 2)
	{
		a.style.display = "inline";
		b.style.display = "none";
		if(inb == 0)	//	Door
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Door Contact";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(inb == 1)	//	Car
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Car Detection";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(inb == 4)
		{
			a.add(NewSe02);						a.add(NewSe03);
		}
	}
	else if(Fi.selectedIndex == 3)
	{
		a.style.display = "inline";
		b.style.display = "none";
		if(inc == 0)	//	Door
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Door Contact";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(inc == 1)	//	Car
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Car Detection";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(inc == 4)
		{
			a.add(NewSe02);						a.add(NewSe03);
		}
	}
	else if(Fi.selectedIndex == 4)
	{
		a.style.display = "inline";
		b.style.display = "none";
		if(ind == 0)	//	Door
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Door Contact";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(ind == 1)	//	Car
		{
			a.add(NewSe02);						a.add(NewSe03);			a.add(NewSe04);
			NewSe05.text = "Car Detection";		NewSe05.value = 4;		a.add(NewSe05);		a.add(NewSe06);
		}
		else if(ind == 4)
		{
			a.add(NewSe02);						a.add(NewSe03);
		}
	}
	else if(Fi.selectedIndex == 5)
	{
		a.style.display = "inline";
		b.style.display = "none";
		a.add(NewSe02);
	}
	else		//		0
	{
		NewSe02.text = "Latch Disable";		NewSe02.value = 0;
		NewSe03.text = "Latch Enable";		NewSe03.value = 1;
		b.add(NewSe02);
		b.add(NewSe03);
		a.style.display = "none";
		b.style.display = "inline";
	}
}

function InnerLatch(idx, fuuc, En)
{
	var s,v0,v1;

	v0=' ';	v1=' ';

	if(En == 1)			v1 = ' selected';
	else				v0 = ' selected';

	s='<nobr><select id=LatchInput'+idx+' name=LatchInput'+idx+' size=1><option value=0'+v0+'>Latch Disable';
	s+='<option value=1'+v1+'>Latch Enable</nobr>';
	document.write(s);

	var a = document.getElementById('LatchInput'+idx);
	var Fi = document.getElementById('FuncInput'+idx);

	if(Fi.selectedIndex == 0)
		a.style.display = "inline";
	else
		a.style.display = "none";
}

