function ExdBox(idx,En)
{
	var s,v;

	if(En == 1)
		v = ' checked';
	else
		v = ' ';

	s='<nobr>';
	s+='<input name=ExdBox'+idx+' type=checkbox value=1 onclick="return ChkExdBox('+idx+')"'+v+'></nobr>';
	document.write(s);
}
