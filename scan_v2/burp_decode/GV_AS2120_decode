		v1 = ' checked';
	else
		v2 = ' checked';

	s='<nobr>';
	s+='<input type=radio name=WR_'+sNumName+' value=1'+v1+'>Enable<input type=radio name=WR_'+sNumName+' value=0'+v2+'>Disable</nobr>';

	document.write(s);
}

