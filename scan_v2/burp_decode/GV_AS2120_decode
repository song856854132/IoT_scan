    v31 = ' ';  v32 = ' ';  v33 = ' ';  v34 = ' ';  v35 = ' ';  v36 = ' ';  v37 = ' ';  v38 = ' ';  v39 = ' ';  v40 = ' ';
    v41 = ' ';  v42 = ' ';  v43 = ' ';  v44 = ' ';

    if(time == 2008)       v2 = ' selected';   else if(time == 2009)  v3 = ' selected';
    else if(time == 2010)  v4 = ' selected';   else if(time == 2011)  v5 = ' selected';   else if(time == 2012)  v6 = ' selected';
    else if(time == 2013)  v7 = ' selected';   else if(time == 2014)  v8 = ' selected';   else if(time == 2015)  v9 = ' selected';
    else if(time == 2016) v10 = ' selected';  else if(time == 2017) v11 = ' selected';  else if(time == 2018) v12 = ' selected';
    else if(time == 2019) v13 = ' selected';  else if(time == 2020) v14 = ' selected';  else if(time == 2021) v15 = ' selected';
    else if(time == 2022) v16 = ' selected';  else if(time == 2023) v17 = ' selected';  else if(time == 2024) v18 = ' selected';
    else if(time == 2025) v19 = ' selected';  else if(time == 2026) v20 = ' selected';  else if(time == 2027) v21 = ' selected';
    else if(time == 2028) v22 = ' selected';  else if(time == 2029) v23 = ' selected';  else if(time == 2030) v24 = ' selected';
    else if(time == 2031) v25 = ' selected';  else if(time == 2032) v26 = ' selected';  else if(time == 2033) v27 = ' selected';
    else if(time == 2034) v28 = ' selected';  else if(time == 2035) v29 = ' selected';  else if(time == 2036) v30 = ' selected';
    else if(time == 2037) v31 = ' selected';  else if(time == 2038) v32 = ' selected';  else if(time == 2039) v33 = ' selected';
    else if(time == 2040) v34 = ' selected';  else if(time == 2041) v35 = ' selected';  else if(time == 2042) v36 = ' selected';
    else if(time == 2043) v37 = ' selected';  else if(time == 2044) v38 = ' selected';  else if(time == 2045) v39 = ' selected';
    else if(time == 2046) v40 = ' selected';  else if(time == 2047) v41 = ' selected';  else if(time == 2048) v42 = ' selected';
    else if(time == 2049) v43 = ' selected';  else    v44 = ' selected';

    s='<nobr>';
    s+='<select name=Year'+index+' size=1 > <option value=2008'+v2+'>2008 <option value=2009'+v3+'>2009<option value=2010'+v4+'>2010 <option value=2011'+v5+'>2011 <option value=2012'+v6+'>2012 <option value=2013'+v7+'>2013 <option value=2014'+v8+'>2014 <option value=2015'+v9+'>2015 <option value=2016'+v10+'>2016 <option value=2017'+v11+'>2017 <option value=2018'+v12+'>2018 <option value=2019'+v13+'>2019 <option value=2020'+v14+'>2020 <option value=2021'+v15+'>2021 <option value=2022'+v16+'>2022 <option value=2023'+v17+'>2023 <option value=2024'+v18+'>2024 <option value=2025'+v19+'>2025 <option value=2026'+v20+'>2026 <option value=2027'+v21+'>2027 <option value=2028'+v22+'>2028 <option value=2029'+v23+'>2029 <option value=2030'+v24+'>2030 <option value=2031'+v25+'>2031 <option value=2032'+v26+'>2032 <option value=2033'+v27+'>2033 <option value=2034'+v28+'>2034 <option value=2035'+v29+'>2035 <option value=2036'+v30+'>2036 <option value=2037'+v31+'>2037 <option value=2038'+v32+'> </select></nobr>'
    document.write(s);
