function getCurrentTime() {
    var today = new Date();
    var i=null, tmp=null;
    var obj;
    var timezoneArr=[ [1,-12],
                   [2,-11],
                   [3,-10],
                   [4,-9],
                   [5,-8],
                   [6,-7],
                   [7,-6],
                   [8,-5],
                   [9,-4],
                   [10,-3],
                   [11,-2],
                   [12,-1],
                   [13,0],
                   [14,1],
                   [15,2],
                   [16,3],
                   [17,4],
                   [18,5],
                   [19,6],
                   [20,7],
                   [21,8],
                   [22,9],
                   [23,10],
                   [24,11],
                   [25,12],
                   [26,13]];

    // time zone hour
    tmp = today.getTimezoneOffset()/(-60);
    for (i=0; i< timezoneArr.length; i++)
    {
        if (tmp == timezoneArr[i][1])
        {
            break;
        }
    }
    tmp = timezoneArr[i][0];

    obj = document.getElementsByName("Tzone");
    for (i=0; i< obj[0].options.length; i++)
    {
        if (obj[0].options[i].value == tmp)
        {
            obj[0].options[i].selected = true;
            break;;
        }
    }

    // time zone min
    tmp = today.getTimezoneOffset()%60;
    obj = document.getElementsByName("Min3");
    for (i=0; i< obj[0].options.length; i++)
    {
        if (obj[0].options[i].value == tmp)
        {
            obj[0].options[i].selected = true;
            break;;
        }
    }    
   
    // year
    if (today.getYear() < 1999) {
        tmp = 1900+today.getYear();
    } else {
        tmp = today.getYear();
    }
    if (tmp>2050) {
        tmp = 2050;
    }
    if (tmp<2008) {
        tmp =2008;
    }

    obj = document.getElementsByName("Year3");
    for (i=0; i< obj[0].options.length; i++)
    {
        if (obj[0].options[i].value == tmp)
        {
            obj[0].options[i].selected = true;
            break;;
        }
    }

    // month
    tmp = (today.getMonth()+1);
    obj = document.getElementsByName("Month3");
    for (i=0; i< obj[0].options.length; i++)
    {
        if (obj[0].options[i].value == tmp)
        {
            obj[0].selectedIndex = i;
            break;;
        }
    }
    
    // date
    tmp = today.getDate();    
    obj = document.getElementsByName("Day3");
    for (i=0; i< obj[0].options.length; i++)
    {
        if (obj[0].options[i].value == tmp)
        {
            obj[0].selectedIndex = i;
            break;;
        }
    }
    
    // hour
    tmp = today.getHours();
    obj = document.getElementsByName("Hour3");
    for (i=0; i< obj[0].options.length; i++)
    {
        if (obj[0].options[i].value == tmp)
        {
            obj[0].selectedIndex = i;
            break;;
        }
    }

    // minutes
    tmp = today.getMinutes();
    obj = document.getElementsByName("Min1");
    for (i=0; i< obj[0].options.length; i++)
    {
        if (obj[0].options[i].value == tmp)
        {
            obj[0].selectedIndex = i;
            break;;
        }
    }
    
    // second
    tmp = 0;
    obj = document.getElementsByName("Min2");
    for (i=0; i< obj[0].options.length; i++)
    {
        if (obj[0].options[i].value == tmp)
        {
            obj[0].selectedIndex = i;
            break;;
        }
    }
    //setTimeout("ShowDate()",700);
}

