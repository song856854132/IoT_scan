function DrawButtom(inum)
{
	var	s;
	if(inum == 1)
	{
		s='<input type=submit value=Submit name=Submit style="font-family: Verdana; font-weight:bold"><input type=reset value=Cancel name=Cancel style="font-family: Verdana; font-weight:bold">';
		document.write(s);
	}
}

function OutputSelect(index,num)
{
	var s,v1,v2;
	
	v1='';
	v2='';
	
	if(num == 4)
		v1 = ' selected';
	else
		v2 = ' selected';

    s='<nobr>';
    s+='<select name=OutputNum'+index+' size=1 > <option value=4'+v1+'>4 <option value=16'+v2+'>16 </select></nobr>'
    document.write(s);
}

