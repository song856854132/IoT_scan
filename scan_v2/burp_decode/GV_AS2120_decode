function INPUT_bCheckWepKey(bIsASCII, szKey)
{
    if(bIsASCII==0) {
        if(szKey.length != 10 && szKey.length != 26) {
            alert(GetXML("IDS_VS_Chk_Wireless_Key1"));
            return false;
        }
        
        for(dwIndex=0;dwIndex<szKey.length;dwIndex++) {
            switch(szKey.charAt(dwIndex)) {
                case '0': case '1': case '2': case '3': case '4': case '5': case '6': case '7': case '8': case '9':
                case 'a': case 'b': case 'c': case 'd': case 'e': case 'f':
                case 'A': case 'B': case 'C': case 'D': case 'E': case 'F':
                    break;
                default:
                    alert(GetXML("IDS_VS_Chk_Wireless_Key2"));
                    return false;
            }
        }
    }
    else {
        if(szKey.length != 5 && szKey.length != 13) {
            alert(GetXML("IDS_VS_Chk_Wireless_Key3"));
            return false;
        }	            
	}
	
	return true;
}

function INPUT_bCheckWpapsk(szKey)
{
    if(szKey.length < 8 || szKey.length > 63) {
        alert(GetXML("IDS_VS_LAN_Wlan_KeyProm"));
        return false;
    }
	return true;
}

function lengthInUtf8Bytes(str) 
{
    // Matches only the 10.. bytes that are non-initial characters in a multi-byte sequence.
    var m = encodeURIComponent(str).match(/%[89ABab]/g);
    return str.length + (m ? m.length : 0);
}

function htmlEncode(html_str) {
    html_str = html_str.replace(/"/g, "&quot;");
    html_str = html_str.replace(/'/g, "&apos;");
    html_str = html_str.replace(/&/g, "&amp;");
    html_str = html_str.replace(/</g, "&lt;");
    html_str = html_str.replace(/>/g, "&gt;");
    return html_str;
}

function PW_char_mode(char)
{
  if (char >= 48 && char <= 57) // Number
    return 1;
  if (char >= 65 && char <= 90)
    return 2;
  if (char >= 97 && char <= 122)
    return 4;
  else
    return 8;
}

function PW_bit_total(num)
{
  modes = 0;
  for (i=0; i<4; i++) {
   if (num & 1)
     modes++;
   num=num>>>1;
  }

  return modes;
}

function PW_check_strong(spw) // 0: Weak, 1: Normal, 2: Great
{
  if (spw.length <= 7)
    { return -1; } // Weak

  Modes=0;
  for (i=0; i<spw.length; i++)
    { Modes |= PW_char_mode(spw.charCodeAt(i)); }
  count = PW_bit_total(Modes);

  if (count <= 2) 
    { return -2; } // Weak

  if (spw.length <= 11)
    { return 1; } // Normal

  if (Modes <= 8) // Not contain speical char
    { return 1; } // Normal

  return 2; // Great
}


function INPUT_bCheckLength(fieldTitle, value, length)
{
    if(value.length > length){
        alert( fieldTitle + ' is over max length :' + length);
        return false;
    }
    return true;
}
