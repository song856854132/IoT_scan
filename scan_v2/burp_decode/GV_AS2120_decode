function SOrder(index, time)
{
	var s,v1,v2,v3,v4,v5;

	v1 = ' ';   v2 = ' ';   v3 = ' ';   v4 = ' ';   v5 = ' ';

	if(time == 1)       v1 = ' selected';   else if(time == 2)  v2 = ' selected';   else if(time == 3)  v3 = ' selected';
    else if(time == 4)  v4 = ' selected';   else if(time == 5)  v5 = ' selected';

    s='<nobr>';
    s+='<select name=Order'+index+' size=1 > <option value=1'+v1+'>First <option value=2'+v2+'>Second <option value=3'+v3+'>Third <option value=4'+v4+'>Fourth <option value=5'+v5+'>Last </select></nobr>'
    document.write(s);
}

function Sweek(index,time)
{
    var s,v1,v2,v3,v4,v5,v6,v7;

    v1 = ' ';   v2 = ' ';   v3 = ' ';   v4 = ' ';   v5 = ' ';   v6 = ' ';	v7 = ' ';

    if(time == 1) v1 = ' selected'; else if(time == 2) v2 = ' selected';    else if(time == 3) v3 = ' selected';
    else if(time == 4) v4 = ' selected';    else if(time == 5) v5 = ' selected';    else if(time == 6) v6 = ' selected';
    else v7 = ' selected';

    s='<nobr>';
    s+='<select name=Week'+index+' size=1 > <option value=7'+v7+'>Sunday <option value=1'+v1+'>Monday <option value=2'+v2+'>Tuesday <option value=3'+v3+'>Wednesday <option value=4'+v4+'>Thursday <option value=5'+v5+'>Friday <option value=6'+v6+'>Saturday </select></nobr>'
    document.write(s);
}

function Smon(index, time)
{
    var s,v1,v2,v3,v4,v5,v6,v7,v8,v9,v10,v11,v12;


    v1 = ' ';   v2 = ' ';   v3 = ' ';   v4 = ' ';   v5 = ' ';   v6 = ' ';   v7 = ' ';   v8 = ' ';   v9 = ' ';   v10 = ' ';
    v11 = ' ';  v12 = ' ';

    if(time == 2) v2 = ' selected';    		else if(time == 3) v3 = ' selected';	else if(time == 4) v4 = ' selected';
	else if(time == 5) v5 = ' selected';    else if(time == 6) v6 = ' selected';	else if(time == 7) v7 = ' selected';
	else if(time == 8) v8 = ' selected';    else if(time == 9) v9 = ' selected';    else if(time == 10) v10 = ' selected';  
	else if(time == 11) v11 = ' selected';  else if(time == 12) v12 = ' selected';	else	v1=' selected';

    s='<nobr>';
    s+='<select name=Month'+index+' size=1 > <option value=1'+v1+'>January <option value=2'+v2+'>February <option value=3'+v3+'>March <option value=4'+v4+'>April <option value=5'+v5+'>May <option value=6'+v6+'>June <option value=7'+v7+'>July <option value=8'+v8+'>August <option value=9'+v9+'>September <option value=10'+v10+'>October <option value=11'+v11+'>November <option value=12'+v12+'>December </select></nobr>'
    document.write(s);
}

function Sday(index, time)
{
    var s,v1,v2,v3,v4,v5,v6,v7,v8,v9,v10,v11,v12,v13,v14,v15,v16,v17,v18,v19,v20,v21,v22,v23,v24,v25,v26,v27,v28,v29,v30,v31;

    v1 = ' ';   v2 = ' ';   v3 = ' ';   v4 = ' ';   v5 = ' ';   v6 = ' ';   v7 = ' ';   v8 = ' ';   v9 = ' ';   v10 = ' ';
    v11 = ' ';  v12 = ' ';  v13 = ' ';  v14 = ' ';  v15 = ' ';  v16 = ' ';  v17 = ' ';  v18 = ' ';  v19 = ' ';  v20 = ' ';
    v21 = ' ';  v22 = ' ';  v23 = ' ';  v24 = ' ';  v25 = ' ';  v26 = ' ';  v27 = ' ';  v28 = ' ';  v29 = ' ';  v30 = ' ';
    v31 = ' ';

    if(time == 1)       v1 = ' selected';   else if(time == 2)  v2 = ' selected';   else if(time == 3)  v3 = ' selected';
    else if(time == 4)  v4 = ' selected';   else if(time == 5)  v5 = ' selected';   else if(time == 6)  v6 = ' selected';
    else if(time == 7)  v7 = ' selected';   else if(time == 8)  v8 = ' selected';   else if(time == 9)  v9 = ' selected';
    else if(time == 10) v10 = ' selected';  else if(time == 11) v11 = ' selected';  else if(time == 12) v12 = ' selected';
    else if(time == 13) v13 = ' selected';  else if(time == 14) v14 = ' selected';  else if(time == 15) v15 = ' selected';
    else if(time == 16) v16 = ' selected';  else if(time == 17) v17 = ' selected';  else if(time == 18) v18 = ' selected';
    else if(time == 19) v19 = ' selected';  else if(time == 20) v20 = ' selected';  else if(time == 21) v21 = ' selected';
    else if(time == 22) v22 = ' selected';  else if(time == 23) v23 = ' selected';  else if(time == 24) v24 = ' selected';
    else if(time == 25) v25 = ' selected';  else if(time == 26) v26 = ' selected';  else if(time == 27) v27 = ' selected';
    else if(time == 28) v28 = ' selected';  else if(time == 29) v29 = ' selected';  else if(time == 30) v30 = ' selected';
    else v31 = ' selected';

    s='<nobr>';
    s+='<select name=Day'+index+' size=1 >   <option value=1'+v1+'>1 <option value=2'+v2+'>2 <option value=3'+v3+'>3 <option value=4'+v4+'>4 <option value=5'+v5+'>5 <option value=6'+v6+'>6 <option value=7'+v7+'>7 <option value=8'+v8+'>8 <option value=9'+v9+'>9 <option value=10'+v10+'>10 <option value=11'+v11+'>11 <option value=12'+v12+'>12 <option value=13'+v13+'>13 <option value=14'+v14+'>14 <option value=15'+v15+'>15 <option value=16'+v16+'>16 <option value=17'+v17+'>17 <option value=18'+v18+'>18 <option value=19'+v19+'>19 <option value=20'+v20+'>20 <option value=21'+v21+'>21 <option value=22'+v22+'>22 <option value=23'+v23+'>23 <option value=24'+v24+'>24 <option value=25'+v25+'>25 <option value=26'+v26+'>26 <option value=27'+v27+'>27 <option value=28'+v28+'>28 <option value=29'+v29+'>29 <option value=30'+v30+'>30 <option value=31'+v31+'>31 </select></nobr>'
    document.write(s);
}

