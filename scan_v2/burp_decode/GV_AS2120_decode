function SubmitCheck()
{
	if(document.CF.IP_IpAddress[0].value > 255 || document.CF.IP_IpAddress[1].value > 255 || document.CF.IP_IpAddress[2].value > 255 || document.CF.IP_IpAddress[3].value > 255)
	{
		alert('IP Address value error!\nPlease input again');
		return false;
	}
	if ( GetNum('CF', 'DevicePort') < 1025 )
	{
		alert('Device Port must bigger than 1024\nPlease input again!');
		return false;
	}

	if( GetNum('CF', 'DevicePort') > 65535 )
	{
		alert('Device Port must less than 65536\nPlease input again!');
		return false;
	}
	
	return SubExd();
}
