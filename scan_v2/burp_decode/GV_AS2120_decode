        v1 = ' selected';
    else if(mode == 8)
        v2 = ' selected';
    else if(mode == 10)
        v3 = ' selected';
    else if(mode == 11)
        v4 = ' selected';
	else if(mode == 12)
        v5 = ' selected';
	else
        v6 = ' selected';

    s='<nobr>';
    s+='<select name=AuthMode'+num+' size=1 ><option value=5'+v1+'>Local Unlock Mode<option value=8'+v2+'>Local Lock Mode<option value=10'+v3+'>Fixed Card Mode<option value=11'+v4+'>Fixed Card+PIN Mode<option value=12'+v5+'>Fixed Card/Common Mode<option value=255'+v6+'>Authentication Schedule Mode</select></nobr>';
    document.write(s);
}

function SelectAnti(indata)
