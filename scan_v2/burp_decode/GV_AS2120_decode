{
	var s,v0,v1,v2,v3,v4;

	v0=' '; v1=' ';	v2=' ';	v3=' ';	v4= ' ';

	if(indata == 0)
		v0 = ' selected';
	else if(indata == 1)
		v1 = ' selected';
	else
		v4 = ' selected';

	s='<nobr>';
